cerif:ruizrube  a  foaf:Person ;
        foaf:name  "Iv√°n Ruiz Rube" .

cerif:money  a            owl:DatatypeProperty , rdf:Property ;
        rdfs:comment      "This property embraces all Currency-dependent properties " ;
