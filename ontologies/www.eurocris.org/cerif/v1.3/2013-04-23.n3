cerif:country  a          rdf:Property ;
        rdfs:comment      "A given country - http://dbpedia.org/ontology/Country" ;
        rdfs:isDefinedBy  <http://www.eurocris.org/ontologies/cerif/1.3> ;
        rdfs:label        "Country " .

cerif:brigitte  a  foaf:Person ;
