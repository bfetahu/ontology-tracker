ebucore:targetAudienceSystem a owl:DatatypeProperty ;
	rdfs:label "Target audience system"^^xsd:string ;
	rdfs:comment "To define the system used to provide a target audience rating"^^xsd:string ;
	rdfs:domain ebucore:TargetAudience ;
	rdfs:range xsd:string .

ebucore:telephoneNumber a owl:DatatypeProperty ;
	rdfs:label "Telephone"^^xsd:string ;
	rdfs:comment """To provide the telephone number of an Agent
            (Contact/person or Organisation)"""^^xsd:string ;
	rdfs:domain ebucore:Agent ;
	rdfs:range xsd:string .

ebucore:textualAnnotation a owl:DatatypeProperty ;
	rdfs:label "Note"^^xsd:string ;
	rdfs:comment """The value of an Annotation as free
            text."""^^xsd:string ;
	rdfs:domain ebucore:Annotation ;
	rdfs:range xsd:string .

ebucore:title a owl:DatatypeProperty ;
	rdfs:label "Title"^^xsd:string ;
	rdfs:comment """All value of the EBU title status
            classification scheme
            (http://www.ebu.ch/metadata/cs/web/ebu_TitleStatusCodeCS_p.xml.htm) are candidates
            subproperties of the title property as implemented for an example with
            alternativeTitle."""^^xsd:string , """Specifies the title or name given to the
            resource. The type can be used as a sub-property to optionally refine the category of
            the title."""^^xsd:string ;
	rdfs:domain ebucore:BusinessObject ;
	rdfs:range xsd:string .

ebucore:trackDefinition a owl:DatatypeProperty ;
	rdfs:label "Definition"^^xsd:string ;
	rdfs:comment """To provide a definition associated to a
            Track."""^^xsd:string ;
	rdfs:domain ebucore:Track ;
	rdfs:range xsd:string .

ebucore:trackName a owl:DatatypeProperty ;
	rdfs:label "Track name"^^xsd:string ;
	rdfs:comment "The name attributed to a Track."^^xsd:string ;
	rdfs:domain ebucore:Track ;
	rdfs:range xsd:string .

ebucore:translationTitle a owl:DatatypeProperty ;
	rdfs:label "Translation title"^^xsd:string ;
	rdfs:subPropertyOf ebucore:alternativeTitle ;
	rdfs:range xsd:string .

ebucore:typeDefinition a owl:DatatypeProperty ;
	rdfs:label "type definition"^^xsd:string ;
	rdfs:comment "To provide a definition for a Type"^^xsd:string ;
	rdfs:domain ebucore:Type ;
	rdfs:range xsd:string .

ebucore:username a owl:DatatypeProperty ;
	rdfs:label "Username"^^xsd:string ;
	rdfs:comment """The username by which a contact / person is
            known e.g. when attributing a rating value"""^^xsd:string ;
	rdfs:domain ebucore:Contact ;
	rdfs:range xsd:string .

ebucore:versionTitle a owl:DatatypeProperty ;
	rdfs:label "Version title"^^xsd:string ;
	rdfs:subPropertyOf ebucore:alternativeTitle ;
	rdfs:range xsd:string .

ebucore:webHomepage a owl:DatatypeProperty ;
	rdfs:label "Homepage"^^xsd:string ;
	rdfs:comment """To provide the address of the webpage of an
            agent (person or organisation)."""^^xsd:string ;
	rdfs:domain ebucore:Agent ;
	rdfs:range xsd:anyURI .

ebucore:width a owl:DatatypeProperty ;
	rdfs:label "Width"^^xsd:string ;
	rdfs:comment """The width of e.g. a video frametypically
            expressed as a number of pixels, or picture/image in millimeters."""^^xsd:string ;
	rdfs:domain ebucore:Resource ;
	rdfs:range xsd:integer .

ebucore:widthUnit a owl:DatatypeProperty ;
	rdfs:label "Width unit"^^xsd:string ;
	rdfs:comment """The unit used to measure a width e.g. in pixels
            or number of lines or millimeters or else."""^^xsd:string ;
	rdfs:domain ebucore:Resource ;
	rdfs:range xsd:string .

ebucore:wordCount a owl:DatatypeProperty ;
	rdfs:label "Word count"^^xsd:string ;
	rdfs:comment """The number of words contained in a
            document."""^^xsd:string ;
	rdfs:domain ebucore:Document ;
	rdfs:range xsd:integer .

ebucore:workingTitle a owl:DatatypeProperty ;
	rdfs:label "Working title"^^xsd:string ;
	rdfs:subPropertyOf ebucore:alternativeTitle ;
	rdfs:range xsd:string .

ebucore:wrappingType a owl:DatatypeProperty ;
	rdfs:label "Wrapping type"^^xsd:string ;
	rdfs:comment """To provide additional information on the
            wrapping type of ancillary data."""^^xsd:string ;
	rdfs:domain ebucore:AncillaryData ;
	rdfs:range xsd:string .

ebucore:AccessConditions a owl:Class ;
	rdfs:subClassOf ebucore:Rights .

ebucore:Agent a owl:Class ;
	rdfs:label "Agent"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """A person / contact or organisation contributing
            to the media resource"""^^xsd:string .

ebucore:AncillaryData a owl:Class ;
	rdfs:label "Ancillary data"^^xsd:string ;
	rdfs:subClassOf ebucore:DataTrack ;
	rdfs:comment """Any ancillary data provided with the content
            other than captioning and subtitling."""^^xsd:string .

ebucore:AncillaryDataFormat a owl:Class ;
	rdfs:subClassOf ebucore:DataFormat ;
	rdfs:comment """To define the format of AncillaryData such as
            legacy data used to be carried in vertical blanking intervals. This is provided as free
            text in an annotation label or as an identifier pointing to a term in a classification
            scheme."""^^xsd:string .

ebucore:Annotation a owl:Class ;
	rdfs:label "Annotation"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """A set of descriptive or technical metadata.
            Annotation can also be made in the form of a document or media resource such as an or
            video audio file . An annotation can exist on its own and be related / associated to
            other editorial objects or resources."""^^xsd:string .

ebucore:Asset a owl:Class ;
	rdfs:label "Asset"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """The Class \"Asset\" is an
            object to which an identifier will be associated at commissioning. It will serve as a
            central reference point to manage rights associated to EditorialObjects, Resources,
            MediaResources or Essences, and PublicationEvents (distribution and exploitation
            conditions)."""^^xsd:string .

ebucore:AudienceLevel a owl:Class ;
	rdfs:label "Audience level"^^xsd:string ;
	rdfs:subClassOf ebucore:TargetAudience ;
	rdfs:comment """The age or occupational group of users for which the BusinessObject
            or resource has been designed or conceptualised."""^^xsd:string .

ebucore:AudienceRating a owl:Class ;
	rdfs:label "Audience rating"^^xsd:string ;
	rdfs:subClassOf ebucore:TargetAudience ;
	rdfs:comment """The audience by which the Resource can be
            seen according to ratings like MPAA or other organisational / national / local standards."""^^xsd:string .

ebucore:AudioBlock a owl:Class ;
	rdfs:label "Audio block"^^xsd:string ;
	rdfs:subClassOf ebucore:Essence ;
	rdfs:comment """DirectSpeakers: For channel-based audio, where each channel feeds a
				speaker directly. Type 0001."""^^xsd:string , """An audioBlock represents a single sequence of audio samples with
				fixed parameters, including position, within a specified time. The sub-elements with
				audioBlock are dependent upon the typeDefintion or typeLabel of the parent
				audioChannel element."""^^xsd:string , """Binaural:For binaural audio, where playback is over headphones. Type
				0005."""^^xsd:string , "HOA: For scene-based audio where Ambisonics and HOA are used. Type 0004."^^xsd:string , """Matrix: For channel-based audio where channels are matrixed together,
				such as Mid-Side, Lt/Rt. Type 0002."""^^xsd:string , """Objects: For object-based audio where channels represent audio objects
				(or parts of objects), so include positional information. Type 0003."""^^xsd:string .

ebucore:AudioChannel a owl:Class ;
	rdfs:label "Audio channel"^^xsd:string ;
	rdfs:subClassOf ebucore:Essence ;
	rdfs:comment """An audioChannel represents a single sequence of audio samples. It
				is sub-divided in the time domain into audioBlocks, which is must contain at
				least one of. The typeDefintion of the audioChannel format specifies the type of
				audio it is describing, and also determines which parameters are used within its
				audioBlock children."""^^xsd:string .

ebucore:AudioContent a owl:Class ;
	rdfs:label "Audio content"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """An audioContent defines one component of a programme (e.g. background
				music), its association with an audioGroup (e.g. a 2.0 audioPackFormat of
				audioChannelFormats for stereo reproduction), its association with an
				audioStreamFormat, and its set of loudness parameters."""^^xsd:string .

ebucore:AudioEncodingFormat a owl:Class ;
	rdfs:label "Audio encoding format"^^xsd:string ;
	rdfs:subClassOf ebucore:EncodingFormat .

ebucore:AudioFormat a owl:Class ;
	rdfs:label "Audio Format"^^xsd:string ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """The technical characteristics of an
            AudioResource such as the sampleRate. This is provided as free text in an annotation
            label or as an identifier pointing to a term in a classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_AudioFormatCodeCS.rdf."""^^xsd:string .

ebucore:AudioMatrix a owl:Class ;
	rdfs:label "Audio Matrix"^^xsd:string ;
	rdfs:subClassOf ebucore:Essence .

ebucore:AudioMatrixChannel a owl:Class ;
	rdfs:label "Audio matrix channel"^^xsd:string ;
	rdfs:subClassOf ebucore:Essence ;
	rdfs:comment "Define references to matrix associated audioChannels and parameters"^^xsd:string .

ebucore:AudioObject a owl:Class ;
	rdfs:label "Audio object"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """Establish the relationship between the content using track UIDs and the
				format via audio packs. These can be nested, so refer to other
			audioObjects."""^^xsd:string .

ebucore:AudioPack a owl:Class ;
	rdfs:label "Audio pack"^^xsd:string ;
	rdfs:subClassOf ebucore:Essence ;
	rdfs:comment """The audioPack brings together one or more audioChannels that
				belong together. For example 'stereo' and '5.1' would be an audioPack for
				channel-based formats. It can also contain references to other packs to allow
				nesting. To help define the type of channels described within the pack, a
				typeDefintion is used to define them."""^^xsd:string .

ebucore:AudioProgramme a owl:Class ;
	rdfs:label "Audio programme"^^xsd:string ;
	rdfs:subClassOf ebucore:Programme ;
	rdfs:comment """A set of one or more audioContent that derive from the same material,
				i.e. an audioMultiplex, and the definition of its multiplexed audioContents (e.g.
				foreground and commentary, background music)."""^^xsd:string .

ebucore:AudioStream a owl:Class ;
	rdfs:label "Audio stream"^^xsd:string ;
	rdfs:subClassOf ebucore:Stream .

ebucore:AudioTrack a owl:Class ;
	rdfs:label "Audio track"^^xsd:string ;
	rdfs:subClassOf ebucore:Track ;
	rdfs:comment """An audioTrack is the basic audio data container of a medium. Attribute is
				an unambiguous reference to this container in a given medium"""^^xsd:string , """An audioTrack object defines a component of an audioStream.
				A single set of samples or data in the storage medium."""^^xsd:string , """Represents a physical container or carrier to hold an audio stream. This
				should be usually defined by many attributes such as ID, format (e.g. 48 kHz/24
				bits), linkage information (e.g. odd/even)…"""^^xsd:string .

ebucore:AudioTrackUID a owl:Class ;
	rdfs:label "Audio track UID"^^xsd:string ;
	rdfs:subClassOf owl:Thing .

ebucore:Binaural a owl:Class ;
	rdfs:label "Binaural"^^xsd:string ;
	rdfs:subClassOf ebucore:AudioBlock .

ebucore:Brand a owl:Class ;
	rdfs:label "Brand"^^xsd:string ;
	rdfs:subClassOf ebucore:Group ;
	rdfs:comment """A group of EditorialObjects having a Brand as a
            common denominator"""^^xsd:string .

ebucore:BusinessObject a owl:Class ;
	rdfs:label "Business Object"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """An image, a document, an annotation
            (descriptive textual metadata or audio/video tag), a tag (time related in audiovisual
            media resources), or an audiovisual media resource (optionally composed of one or more
            fragment / part and / or audio, video data tracks). Other types of BusinessObjects may
            be defined as subclasses."""^^xsd:string .

ebucore:Captioning a owl:Class ;
	rdfs:label "Captioning"^^xsd:string ;
	rdfs:subClassOf ebucore:DataTrack ;
	rdfs:comment """To signal the presence of hard of hearing
            captioning."""^^xsd:string .

ebucore:CaptioningFormat a owl:Class ;
	rdfs:subClassOf ebucore:DataFormat ;
	rdfs:comment """To define the format of captioning.
            Captioning's main use isfor hard of hearing transcription. This is provided as
            free text in an annotation label or as an identifier pointing to a term in a
            classification scheme."""^^xsd:string .

ebucore:Cast a owl:Class ;
	rdfs:label "Cast member"^^xsd:string ;
	rdfs:subClassOf ebucore:Agent ;
	rdfs:comment """A member of the cast list (a list of fictitious
            characters)"""^^xsd:string .

ebucore:Character a owl:Class ;
	rdfs:label "Character"^^xsd:string ;
	rdfs:subClassOf ebucore:Agent ;
	rdfs:comment "A fictitious contact / person"^^xsd:string .

ebucore:ClosedCaptions a owl:Class ;
	rdfs:label "Closed caption"^^xsd:string ;
	rdfs:subClassOf ebucore:Captioning ;
	rdfs:comment """Closed captioning is provided as separate
            content."""^^xsd:string .

ebucore:ClosedSubtitling a owl:Class ;
	rdfs:label "Closed subtitling"^^xsd:string ;
	rdfs:subClassOf ebucore:Subtitling ;
	rdfs:comment """Closed subtitles are provided as separate
            content."""^^xsd:string .

ebucore:Codec a owl:Class ;
	rdfs:label "Codec"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment "To provide information on a codec"^^xsd:string .

ebucore:Collection a owl:Class ;
	rdfs:label "Collection"^^xsd:string ;
	rdfs:subClassOf ebucore:Group ;
	rdfs:comment """A group of EditorialObjects. There can be many
            types of collections for which specific sub-classes should be defined. In the worl of
            archives, A collection corresponds to all items belonging to an individual /
            collector."""^^xsd:string .

ebucore:ColourSpace a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """The CoulourSpace of a VideoResource. A
            ColourSpace is defined as free text in an annotation label or as an identifier pointing
            to a term in a classification scheme such as
            http://www.ebu.ch/metadata/ontologies/skos/ebu_ColourCodeCS.rdf"""^^xsd:string .

ebucore:Component a owl:Class ;
	rdfs:label "Component"^^xsd:string ;
	rdfs:subClassOf ebucore:Essence .

ebucore:Contact a owl:Class ;
	rdfs:label "Contact"^^xsd:string ;
	rdfs:subClassOf ebucore:Agent ;
	rdfs:comment "A physical person"^^xsd:string .

ebucore:ContainerFormat a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """The container or wrapper format used to package
            audio, video and data components of a Mediaresource. The format is defined as free text
            or pointing at a term in a classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_ContainerFormatCS.rdf"""^^xsd:string .

ebucore:ContainerMimeType a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """The definition of the container if available as
            a MIME type. This is provided as free text in an annotation label or as an identifier
            pointing to a term in a classification scheme. For more information:
            http://www.iana.org/assignments/media-types/application/index.html"""^^xsd:string .

ebucore:Copyright a owl:Class ;
	rdfs:subClassOf ebucore:Rights ;
	rdfs:comment """To provide a copyright
            statement."""^^xsd:string .

ebucore:CoverageRestrictions a owl:Class ;
	rdfs:subClassOf ebucore:Rights ;
	rdfs:comment """To provide information on possible restrictions
            regarding the temporal and spatial coverage for publication."""^^xsd:string .

ebucore:DataFormat a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """To provide addtional technical information on
            the characteristics of data streams in a MediaResource including but not limited to
            AncillaryData, Subtilting and Captioning. Additional specific data format may be defined
            as subclasses of DataFormat."""^^xsd:string .

ebucore:DataTrack a owl:Class ;
	rdfs:label "Data track"^^xsd:string ;
	rdfs:subClassOf ebucore:Track ;
	rdfs:comment """Ancillary data track e.g. ¨captioning\"
            or \"subtitling\" in addition to video and audio tracks."""^^xsd:string .

ebucore:Department a owl:Class ;
	rdfs:label "Department"^^xsd:string ;
	rdfs:subClassOf ebucore:Organisation ;
	rdfs:comment """A department within and
            organisation"""^^xsd:string .

ebucore:DepictedEvent a owl:Class ;
	rdfs:label "Depicted Event"^^xsd:string ;
	rdfs:subClassOf ebucore:Event ;
	rdfs:comment """A DepictedEVent is fictitious or historical or
            other sort of Event that the content of the BusinessObject or resource relates
            to."""^^xsd:string .

ebucore:DirectSpeakers a owl:Class ;
	rdfs:label "Direct speakers"^^xsd:string ;
	rdfs:subClassOf ebucore:AudioBlock .

ebucore:Disclaimer a owl:Class ;
	rdfs:subClassOf ebucore:Rights ;
	rdfs:comment """To provide a disclaimer of any
            form."""^^xsd:string .

ebucore:Document a owl:Class ;
	rdfs:label "Document"^^xsd:string ;
	rdfs:subClassOf ebucore:Resource ;
	rdfs:comment """To describe a publication in the form of a
            document e.g. a html webpage (news item) or a pdf document e.g. a script."""^^xsd:string .

ebucore:DocumentFormat a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """To provide technical information about the
            format of a document such as the orientation. This is provided as free text in an
            annotation label or as an identifier pointing to a term in a classification
            scheme."""^^xsd:string .

ebucore:Dopesheet a owl:Class ;
	rdfs:label "Dopesheet"^^xsd:string ;
	rdfs:subClassOf ebucore:Document .

ebucore:EditorialObject a owl:Class ;
	rdfs:label "Editorial Object"^^xsd:string ;
	rdfs:subClassOf ebucore:BusinessObject ;
	rdfs:comment """In the audiovisual domain, the Class
            â€œEditorialObjectâ€� transforms a commissioned concept into an editorial definition of a
            MediaResource before fabrication (in the Production Domain) and Distribution (in the
            Distribution Domain). An â€œEditorialObjectâ€� is a set of descriptive metadata summarising
            e.g. editing decisions. An â€œEditorialObjectâ€� can also be a part of an â€œEditorialObjectâ€�,
            which is defined by its start time and duration. An EditorialObject can also be a group
            of EditorialObjects. For example a series composed of episodes is defined as an
            EditorialObject."""^^xsd:string .

ebucore:EncodingFormat a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """To provide a definition of the encoding format
            for audio and video. This is provided as free text in an annotation label or as an
            identifier pointing to a term in a classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_AudioCompressionCodeCS.rdf or
            http://www.ebu.ch/metadata/ontologies/skos/ebu_VideoCompressionCodeCS.rdf."""^^xsd:string .

ebucore:Essence a owl:Class ;
	rdfs:label "Essence"^^xsd:string ;
	rdfs:subClassOf ebucore:MediaResource ;
	rdfs:comment """In some audiovisual standardisation groups,
            Essence is preferred to MediaResource. In also has subclasses well known as MediaObject
            clustered in AudioObjects and VideoObjects."""^^xsd:string .

ebucore:Event a owl:Class ;
	rdfs:label "Event"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """Additional types of event shall be defined as
            new sub-classes of event."""^^xsd:string , """An event related to the media resource, e.g.
            depicted in the resource (possibly fictional), etc."""^^xsd:string .

ebucore:ExploitationIssues a owl:Class ;
	rdfs:subClassOf ebucore:Rights ;
	rdfs:comment """To highlight potential exploitation
            issues."""^^xsd:string .

ebucore:FileFormat a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """A file format for Resources other than
            audiovisual resources. The format is defined as free text or pointing at a term in a
            classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_FileFormatCS.rdf"""^^xsd:string .

ebucore:Format a owl:Class ;
	rdfs:label "Format"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """The format provides technical information on
            the format of a Resource. A BusinessObject can be instantiated in a variety of Resources
            each in a particular Format. Other specific data formats may be defined as subclasses of
            format."""^^xsd:string .

ebucore:Genre a owl:Class ;
	rdfs:comment """This class shall be used to provide information
            on the genre of the BusinessObject or Resource. This is provided as free text in an
            annotation label or as an identifier pointing to a term in a classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_ContentGenreCS.rdf or
            http://www.ebu.ch/metadata/ontologies/skos/ebu_EditorialFormatCodeCS.rdf."""^^xsd:string .

ebucore:Group a owl:Class ;
	rdfs:label "Group"^^xsd:string ;
	rdfs:subClassOf ebucore:EditorialObject ;
	rdfs:comment """To define a collection / group of media
            resources, for example a series made of episodes."""^^xsd:string .

ebucore:HOA a owl:Class ;
	rdfs:label "HOA"^^xsd:string ;
	rdfs:subClassOf ebucore:AudioBlock .

ebucore:IPRRestrictions a owl:Class ;
	rdfs:subClassOf ebucore:Rights ;
	rdfs:comment """To provide information on intellectual
            property."""^^xsd:string .

ebucore:Image a owl:Class ;
	rdfs:label "Image"^^xsd:string ;
	rdfs:subClassOf ebucore:BusinessObject ;
	rdfs:comment """A still image / thumbnail / key frame / logo
            related to the media resource or being the media resource itself"""^^xsd:string .

ebucore:ImageFormat a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """To provide technical information about the
            format of an image such as the orientation. This is provided as free text in an
            annotation label or as an identifier pointing to a term in a classification
            scheme."""^^xsd:string .

ebucore:Keyframe a owl:Class ;
	rdfs:label "key frame"^^xsd:string ;
	rdfs:subClassOf ebucore:Picture ;
	rdfs:comment """A key frame is a frame extarcted from video,
            e.g. representative of a part of a MediaResource."""^^xsd:string .

ebucore:Keyword a owl:Class ;
	rdfs:comment """To proivde keywords and define key concepts
            illustrating the content of the Resource or EditorialObject. This is provided as free
            text in an annotation label or as an identifier pointing to a term in a classification
            scheme."""^^xsd:string .

ebucore:Language a owl:Class ;
	rdfs:comment """To provide information on languages present in
            the BusinessObject and its purpose. This is provided as free text in an annotation label
            or as an identifier pointing to a term in a classification scheme.Other language
            specific types may be added as subclasses of language."""^^xsd:string .

ebucore:Location a owl:Class ;
	rdfs:label "Location"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """A location related to the media resource, e.g.
            depicted in the resource (possibly fictional) or where the resource was created
            (shooting location), etc."""^^xsd:string , """A type of location is defined as a sub-class of
            location."""^^xsd:string , """This is provided as free text in an annotation
            label or as an identifier pointing to a term in a classification scheme."""^^xsd:string .

ebucore:Logo a owl:Class ;
	rdfs:label "Logo"^^xsd:string ;
	rdfs:subClassOf ebucore:Picture ;
	rdfs:comment """A Logo allows to visually identify an
            organisation, publicationService, publicationChannel, or ratings /
            parentalGuidance"""^^xsd:string .

ebucore:Matrix a owl:Class ;
	rdfs:label "Matrix"^^xsd:string ;
	rdfs:subClassOf ebucore:AudioBlock .

ebucore:MediaFragment a owl:Class ;
	rdfs:label "Media Fragment"^^xsd:string ;
	rdfs:subClassOf ebucore:MediaResource .

ebucore:MediaResource a owl:Class ;
	rdfs:label "Media Resource"^^xsd:string ;
	rdfs:subClassOf ebucore:Resource ;
	rdfs:comment """The use of MediaResource is reserved to
            audiovisual content."""^^xsd:string .

ebucore:Medium a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """To provide information on the medium formats in
            which the resource is available. This is provided as free text in an annotation label or
            as an identifier pointing to a term in a classification scheme."""^^xsd:string .

ebucore:MimeType a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """The definition of the container if available as
            a MIME type. This is provided as free text in an annotation label or as an identifier
            pointing to a term in a classification scheme. For more information:
            http://www.iana.org/assignments/media-types/index.html"""^^xsd:string .

ebucore:NewsItem a owl:Class ;
	rdfs:label "News Item"^^xsd:string ;
	rdfs:subClassOf ebucore:EditorialObject .

ebucore:ObjectType a owl:Class ;
	rdfs:comment """To specify the type of BusinessObject e.g. and
            EditorialObject of type \"programme\" or clip\". This is
            provided as free text in an annotation label or as an identifier pointing to a term in a
            classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_ObjectTypeCodeCS.rdf."""^^xsd:string .

ebucore:Objects a owl:Class ;
	rdfs:label "Objects"^^xsd:string ;
	rdfs:subClassOf ebucore:AudioBlock .

ebucore:OpenCaptions a owl:Class ;
	rdfs:label "Open captions"^^xsd:string ;
	rdfs:subClassOf ebucore:Captioning ;
	rdfs:comment """Open Captions are burned in the
            image."""^^xsd:string .

ebucore:OpenSubtitling a owl:Class ;
	rdfs:label "Open subtitling"^^xsd:string ;
	rdfs:subClassOf ebucore:Subtitling ;
	rdfs:comment """Open subtitles are burned in the
            image."""^^xsd:string .

ebucore:Organisation a owl:Class ;
	rdfs:label "Organisation"^^xsd:string ;
	rdfs:subClassOf ebucore:Agent ;
	rdfs:comment "An organisation or moral agent"^^xsd:string .

ebucore:OriginalLanguage a owl:Class ;
	rdfs:subClassOf ebucore:Language ;
	rdfs:comment """The original language in which the
            BusinessObject or Resource has been created and released. This is provided as free text
            in an annotation label or as an identifier pointing to a term in a classification
            scheme."""^^xsd:string .

ebucore:Part a owl:Class ;
	rdfs:label "Part, Fragment, Segment"^^xsd:string ;
	rdfs:subClassOf ebucore:EditorialObject ;
	rdfs:comment """A Fragment is a particular section of a
            MediaResource identified by a start and end time or duration. Fragment can also be
            called segment or part."""^^xsd:string , """One of more media fragment (audio, video, data)
            composing an audiovisual media resource. In other ontolgies fragment is often referred
            to e.g. as a 'part' or 'segment' or
            'fragment'."""^^xsd:string .

ebucore:Pictogram a owl:Class ;
	rdfs:label "Pictogram"^^xsd:string ;
	rdfs:subClassOf ebucore:Picture .

ebucore:Picture a owl:Class ;
	rdfs:label "Picture"^^xsd:string ;
	rdfs:subClassOf ebucore:Resource .

ebucore:Programme a owl:Class ;
	rdfs:label "Programme"^^xsd:string ;
	rdfs:subClassOf ebucore:EditorialObject ;
	rdfs:comment """An EditorialObject corresponding to a
            MediaResource ready for publication."""^^xsd:string .

ebucore:PublicationChannel a owl:Class ;
	rdfs:label "Publication Channel"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """The name of the channel through which a
            Resource has been published as a PublicationEvent. A PublicationChannel can use a
            variety of medias e.g. broadcast or online."""^^xsd:string .

ebucore:PublicationEvent a owl:Class ;
	rdfs:label "Publication Event"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """To describe any manifestation of a media
            resource on any media (live, on demand, catch-up TV, etc.) and the appropriate
            PublciationChannel."""^^xsd:string .

ebucore:PublicationHistory a owl:Class ;
	rdfs:label "Publication History"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """A collection of PublicationEvents through which
            a resource has been published."""^^xsd:string .

ebucore:PublicationPlanning a owl:Class ;
	rdfs:label "Publication History"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment "A collection of PublicationEvents organised as a PublicationPlanning."^^xsd:string .

ebucore:RadioProgramme a owl:Class ;
	rdfs:label "Radio Programme"^^xsd:string ;
	rdfs:subClassOf ebucore:Programme ;
	rdfs:comment """A programme for distribution on radio
            channels"""^^xsd:string .

ebucore:Rating a owl:Class ;
	rdfs:label "Rating"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """All the information about the rating/evaluation
            given to a media resource by an Agent i.e. a person/Contact or
            Organisation."""^^xsd:string , """This is provided as free text in an annotation
            label or as an identifier pointing to a term in a classification scheme."""^^xsd:string .

ebucore:Resource a owl:Class ;
	rdfs:label "Resource"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """A manifestation of a
            BusinessObject."""^^xsd:string .

ebucore:Rights a owl:Class ;
	rdfs:label "Rights"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """To provide information on the rights, including
            intellectual property, related to a BusinessObject or Resource."""^^xsd:string .

ebucore:RightsClearance a owl:Class ;
	rdfs:subClassOf ebucore:Rights ;
	rdfs:comment """To signal that rights have been cleared (or
            not)"""^^xsd:string .

ebucore:Role a owl:Class ;
	rdfs:comment """To define the role / action of an agent. This
            is provided as free text in an annotation label or as an identifier pointing to a term
            in a classification scheme."""^^xsd:string .

ebucore:Season a owl:Class ;
	rdfs:label "Season"^^xsd:string ;
	rdfs:subClassOf ebucore:Group ;
	rdfs:comment """A series can be composed of one or more seasons
            clustering a certain number of episodes. Fro this reason, seasons are related to series
            using the isRelatedTo property."""^^xsd:string .

ebucore:Series a owl:Class ;
	rdfs:label "Series"^^xsd:string ;
	rdfs:subClassOf ebucore:Group ;
	rdfs:comment """Series is a particular type of collection. TV
            or Radio Series are composed of Episodes."""^^xsd:string .

ebucore:Service a owl:Class ;
	rdfs:label "Service"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """A service is the umbrella under which one or
            more PublicationChannel is operated."""^^xsd:string .

ebucore:Signing a owl:Class ;
	rdfs:label "Signing"^^xsd:string ;
	rdfs:subClassOf ebucore:VideoTrack ;
	rdfs:comment """To signal the presence of Signing for hard of
            hearing users. The type of Signing (e.g. incursted in or else) or language of Signing
            can be specified using the appropriate properties."""^^xsd:string .

ebucore:SigningFormat a owl:Class ;
	rdfs:subClassOf ebucore:VideoFormat ;
	rdfs:comment """To provide additional information on the
            signing format. This is provided as free text in an annotation label or as an identifier
            pointing to a term in a classification scheme."""^^xsd:string .

ebucore:Staff a owl:Class ;
	rdfs:label "Staff member"^^xsd:string ;
	rdfs:subClassOf ebucore:Agent .

ebucore:StorageType a owl:Class ;
	rdfs:comment """The type of storage used for the repository.
            This is provided as free text in an annotation label or as an identifier pointing to a
            term in a classification scheme."""^^xsd:string .

ebucore:Stream a owl:Class ;
	rdfs:label "Stream"^^xsd:string ;
	rdfs:subClassOf ebucore:Component .

ebucore:Subject a owl:Class ;
	rdfs:comment """A term describing the topic covered by the
            BusinessObject or resource. This is provided as free text in an annotation label or as
            an identifier pointing to a term in a classification scheme."""^^xsd:string .

ebucore:Subtitling a owl:Class ;
	rdfs:label "Subtitling"^^xsd:string ;
	rdfs:subClassOf ebucore:DataTrack ;
	rdfs:comment """To signal the presence of subtitles for
            translation in alternative languages."""^^xsd:string .

ebucore:SubtitlingFormat a owl:Class ;
	rdfs:subClassOf ebucore:DataFormat ;
	rdfs:comment """To define the format of subtitling.
            subtitling's main use isfor translation. This is provided as free text in an
            annotation label or as an identifier pointing to a term in a classification
            scheme."""^^xsd:string .

ebucore:TVProgramme a owl:Class ;
	rdfs:label "TV Programme"^^xsd:string ;
	rdfs:subClassOf ebucore:Programme ;
	rdfs:comment """A programme for distribution on television
            channels."""^^xsd:string .

ebucore:Tag a owl:Class ;
	rdfs:label "Tag"^^xsd:string ;
	rdfs:subClassOf ebucore:Annotation ;
	rdfs:comment """An annotation specific to a particular
            timestamp in audiovisual MediaResources."""^^xsd:string .

ebucore:TargetAudience a owl:Class ;
	rdfs:comment """The target audience (target region, target
            audience category but also parental guidance recommendation) for which the media
            resource is intended"""^^xsd:string , """This is provided as free text in an annotation
            label or as an identifier pointing to a term in a classification scheme."""^^xsd:string .

ebucore:Thumbnail a owl:Class ;
	rdfs:label "Thumbnail"^^xsd:string ;
	rdfs:subClassOf ebucore:Picture ;
	rdfs:comment """A thumbnail is a low resolution picture that
            can be associated with EditorialObjects or e.g. MediaResources or
            Contacts"""^^xsd:string .

ebucore:Topic a owl:Class ;
	rdfs:subClassOf ebucore:Subject ;
	rdfs:comment """A type subject for use in some contexts. This
            is provided as free text in an annotation label or as an identifier pointing to a term
            in a classification scheme."""^^xsd:string .

ebucore:Track a owl:Class ;
	rdfs:label "Track"^^xsd:string ;
	rdfs:subClassOf ebucore:Resource ;
	rdfs:comment """Audiovisual content can be composed of audio,
            video and data Tracks (including captioning and subtitling)."""^^xsd:string .

ebucore:Type a owl:Class ;
	rdfs:label "Type"^^xsd:string ;
	rdfs:subClassOf owl:Thing .

ebucore:UsageRights a owl:Class ;
	rdfs:subClassOf ebucore:Rights .

ebucore:VideoEncodingFormat a owl:Class ;
	rdfs:label "Video encoding format"^^xsd:string ;
	rdfs:subClassOf ebucore:EncodingFormat .

ebucore:VideoFormat a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """To provide additional technical information
            about a video resource such as the frame rate. This is provided as free text in an
            annotation label or as an identifier pointing to a term in a classification
            scheme."""^^xsd:string .

ebucore:VideoStream a owl:Class ;
	rdfs:label "Video stream"^^xsd:string ;
	rdfs:subClassOf ebucore:Stream .

ebucore:VideoTrack a owl:Class ;
	rdfs:label "Video track"^^xsd:string ;
	rdfs:subClassOf ebucore:Track ;
	rdfs:comment """A specialisation of Track for Video to provide
            a link to specific data properties such as frameRate, etc. Signing is another possible
            example of video track. Specific VideoTracks such as Signing can be defined as sub
            VideoTracks.. In advanced systems, different VideoTracks can be used to provide e.g.
            different viewing angles."""^^xsd:string .

