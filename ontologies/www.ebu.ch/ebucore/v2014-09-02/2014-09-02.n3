@prefix dc: <http://purl.org/dc/terms/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl2xml: <http://www.w3.org/2006/12/owl2-xml#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix ebucore: <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix core: <http://www.bbc.co.uk/ontologies/coreconcepts/> .
@prefix ebuccdm: <http://www.ebu.ch/metadata/ontologies/ebuccdm#> .
@prefix vs: <http://www.w3.org/2003/06/sw#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix cc: <http://creativecommons.org/licenses/> .

<http://www.ebu.ch/metadata/ontologies/ebucore/ebucore> a owl:Ontology ;
	dc:contributor "Christophe Debruyne, RIA"@en ;
	dc:creator """Jean Pierre 
      Evain, EBU"""@en ;
	owl:priorVersion <http://www.ebu.ch/metadata/ontologies/ebucore_skos/20140719/ebucoreskos_2014_07_19.rdf> ;
	dc:description """The EBUCore has been
      designed to make users benefit from the flexibility of RDF to adapt the names of Classes and
      properties to their respective needs. This means users are welcome to add their own subclasses
      (e.g. to define the most appropriate BusinessObjects or Resources or Agents) and subproperties."""@en ;
	dc:contributor "Robert Engels, NRK"@en ;
	owl:versionInfo "dc properties' type changed to rdf:Property; Properties with range skos:Concept and subclasses changed from owl:objectProperties to rdf:Property without range. Keyword now a subclass of skos:Concept. Inverse properties declared when present. Improved consistency on use of foaf properties as subproperties of ebucore. Corrected domain of hasMember."@en , "Version 1.0"@en ;
	owl:imports <http://www.w3.org/2004/02/skos/core> ;
	cc:licence "http://creativecommons.org/licenses/by-sa/3.0/"^^xsd:string ;
	dc:issued "2013-01-01" ;
	dc:contributor "Tormod Vaervagen, NRK"@en ;
	owl:versionInfo "EBUCore based on Dublin Core + SKOS + some BBC Core Concepts + basic FOAF properties  + equivalences to W3C MA-ONT"@en ;
	dc:contributor "Knut-Olav Hoven, NRK"@en ;
	dc:modified "2014-09-02"^^xsd:string ;
	dc:description "The EBUCore ontology now includes the new EBUCore 3D object audio model (ADM)."@en ;
	dc:title "EBUCore - the Dublin Core for media"@en ;
	vs:term_status "stable"@en ;
	dc:rights "Copyright 2014 EBU"@en ;
	vann:preferredNamespaceUri "http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#"^^xsd:string ;
	owl:incompatibleWith <http://www.ebu.ch/metadata/ontologies/ebucore/20140320/ebucore_2014_03_13.rdf> .

ebucore:Rating a owl:Class ;
	rdfs:comment """This is provided as free text in an annotation
            label or as an identifier pointing to a term in a classification scheme."""@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """All the information about the rating/evaluation
            given to a media resource by an Agent i.e. a person/Contact or
            Organisation."""@en ;
	rdfs:label "Rating"@en .

ebucore:Collection a owl:Class ;
	rdfs:label "Collection"^^xsd:string ;
	rdfs:comment """A group of EditorialObjects. There can be many
            types of collections for which specific sub-classes should be defined. In the worl of
            archives, A collection corresponds to all items belonging to an individual /
            collector."""^^xsd:string ;
	rdfs:subClassOf ebucore:Group .

ebucore:Group a owl:Class .

ebucore:Thumbnail a owl:Class ;
	rdfs:subClassOf ebucore:Picture .

ebucore:Picture a owl:Class .

ebucore:Thumbnail rdfs:comment """A thumbnail is a low resolution picture that
            can be associated with EditorialObjects or e.g. MediaResources or
            Contacts."""@en ;
	rdfs:label "Thumbnail"@en .

core:Person a owl:Class ;
	rdfs:subClassOf ebucore:Contact .

ebucore:Contact a owl:Class .

core:Person rdfs:label "Person"@en ;
	rdfs:comment "all types of people such as politicians, athletes, historic figures, contributors in programmes e.t.c."@en .

ebucore:BusinessObject a owl:Class ;
	rdfs:comment """An image, a document, an annotation
            (descriptive textual metadata or audio/video tag), a tag (time related in audiovisual
            media resources), or an audiovisual media resource (optionally composed of one or more
            fragment / part and / or audio, video data tracks). Other types of BusinessObjects may
            be defined as subclasses."""@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Business Object"@en .

ebucore:IPRRestrictions a owl:Class ;
	rdfs:subClassOf ebucore:Rights .

ebucore:Rights a owl:Class .

ebucore:IPRRestrictions rdfs:comment """To provide information on intellectual
            property."""@en ;
	rdfs:label "IPR restrictions"@en .

ebucore:Asset a owl:Class ;
	rdfs:comment """The Class \"Asset\" is an
            object to which an identifier will be associated at commissioning. It will serve as a
            central reference point to manage rights associated to EditorialObjects, Resources,
            MediaResources or Essences, and PublicationEvents (distribution and exploitation
            conditions)."""@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Asset"@en .

ebucore:Essence a owl:Class ;
	rdfs:label "Essence"@en ;
	rdfs:comment """In some audiovisual standardisation groups,
            Essence is preferred to MediaResource. In also has subclasses well known as MediaObject
            clustered in AudioObjects and VideoObjects."""@en ;
	rdfs:subClassOf ebucore:MediaResource .

ebucore:MediaResource a owl:Class .

ebucore:AudioContent a owl:Class ;
	rdfs:subClassOf ebucore:AudioProgramme .

ebucore:AudioProgramme a owl:Class .

ebucore:AudioContent rdfs:comment """An audioContent defines one component of a programme (e.g. background
				music), its association with an audioGroup (e.g. a 2.0 audioPackFormat of
				audioChannelFormats for stereo reproduction), its association with an
				audioStreamFormat, and its set of loudness parameters."""@en ;
	rdfs:label "Audio content"@en .

ebucore:RightsClearance a owl:Class ;
	rdfs:subClassOf ebucore:Rights ;
	rdfs:comment """To signal that rights have been cleared (or
            not)"""@en .

ebucore:AudioChannelMatrix a owl:Class ;
	rdfs:label "Matrix"@en ;
	rdfs:comment """For channel-based audio where channels are matrixed together,
				such as Mid-Side, Lt/Rt."""@en ;
	rdfs:subClassOf ebucore:AudioChannel .

ebucore:AudioChannel a owl:Class .

ebucore:PublicationHistory a owl:Class ;
	rdfs:comment """A collection of PublicationEvents through which
            a resource has been published."""@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Publication History"@en .

ebucore:Format a owl:Class ;
	rdfs:comment """The format provides technical information on
            the format of a Resource. A BusinessObject can be instantiated in a variety of Resources
            each in a particular Format. Other specific data formats may be defined as subclasses of
            format."""@en ;
	rdfs:subClassOf skos:Concept .

skos:Concept rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .

ebucore:Format rdfs:label "Format"@en .

ebucore:EditorialObject a owl:Class ;
	rdfs:comment """In the audiovisual domain, the Class
            EditorialObject transforms a commissioned concept into an editorial definition of a
            MediaResource before fabrication (in the Production Domain) and Distribution (in the
            Distribution Domain). An EditorialObject is a set of descriptive metadata summarising
            e.g. editing decisions. An EditorialObject can also be a part of an EditorialObject,
            which is defined by its start time and duration. An EditorialObject can also be a group
            of EditorialObjects. For example a series composed of episodes is defined as an
            EditorialObject."""@en ;
	rdfs:label "Editorial Object"@en ;
	rdfs:subClassOf ebucore:BusinessObject .

ebucore:AudioBlockMatrix a owl:Class ;
	rdfs:subClassOf ebucore:AudioBlock .

ebucore:AudioBlock a owl:Class .

ebucore:AudioBlockMatrix rdfs:comment """For channel-based audio where channels are matrixed together,
				such as Mid-Side, Lt/Rt."""@en ;
	rdfs:label "Matrix"@en .

ebucore:VideoStream a owl:Class ;
	rdfs:comment "A decodable video stream of bits."@en ;
	rdfs:label "Video stream"@en ;
	rdfs:subClassOf ebucore:Stream .

ebucore:Stream a owl:Class .

ebucore:DataFormat a owl:Class ;
	rdfs:label "Data format"@en ;
	rdfs:comment """To provide addtional technical information on
            the characteristics of data streams in a MediaResource including but not limited to
            AncillaryData, Subtilting and Captioning. Additional specific data format may be defined
            as subclasses of DataFormat."""@en ;
	rdfs:subClassOf ebucore:Format .

ebucore:AudioStream a owl:Class ;
	rdfs:comment "An audioStreamFormat describes a decodable signal - PCM signal or a Dolby E stream for example. It is composed of one or more AudioTracks."@en ;
	rdfs:label "Audio stream"@en ;
	rdfs:subClassOf ebucore:Stream .

ebucore:AudioChannelObjects a owl:Class ;
	rdfs:subClassOf ebucore:AudioChannel ;
	rdfs:comment """For object-based audio where channels represent audio objects
				(or parts of objects), so include positional information."""@en ;
	rdfs:label "Objects"@en .

ebucore:Subtitling a owl:Class ;
	rdfs:comment """To signal the presence of subtitles for
            translation in alternative languages."""@en ;
	rdfs:label "Subtitling"@en ;
	rdfs:subClassOf ebucore:DataTrack .

ebucore:DataTrack a owl:Class .

ebucore:Stream rdfs:subClassOf ebucore:Component .

ebucore:Component a owl:Class .

ebucore:Stream rdfs:label "Stream"@en ;
	rdfs:comment "A continuous stream of bits."@en .

ebucore:DataTrack rdfs:label "Data track"@en ;
	rdfs:comment """Ancillary data track e.g. Â¨captioning\"
            or \"subtitling\" in addition to video and audio tracks."""@en ;
	rdfs:subClassOf ebucore:Track .

ebucore:Track a owl:Class .

ebucore:NewsItem a owl:Class ;
	rdfs:subClassOf ebucore:EditorialObject ;
	rdfs:label "News Item"@en ;
	rdfs:comment "A NewsItem aggregates all information about a particular news event."@en .

ebucore:StorageType a owl:Class ;
	rdfs:comment """The type of storage used for the repository.
            This is provided as free text in an annotation label or as an identifier pointing to a
            term in a classification scheme."""@en ;
	rdfs:subClassOf skos:Concept ;
	rdfs:label "Storage type"@en .

ebucore:Dopesheet a owl:Class ;
	rdfs:subClassOf ebucore:Document .

ebucore:Document a owl:Class .

ebucore:Dopesheet rdfs:label "Dopesheet"@en ;
	rdfs:comment "Provides additional information about a NewsItem, e.g. date and place, subject."@en .

ebucore:Annotation a owl:Class ;
	rdfs:comment """A set of descriptive or technical metadata.
            Annotation can also be made in the form of a document or media resource such as an or
            video audio file . An annotation can exist on its own and be related / associated to
            other editorial objects or resources."""@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Annotation"@en .

ebucore:AudioBlock rdfs:label "Audio block"@en ;
	rdfs:comment """An audioBlock represents a single sequence of audio samples with
				fixed parameters, including position, within a specified time. The sub-elements with
				audioBlock are dependent upon the typeDefintion or typeLabel of the parent
				audioChannel element."""@en ;
	rdfs:subClassOf ebucore:Essence .

ebucore:Event a owl:Class ;
	rdfs:comment """An event related to the media resource, e.g.
            depicted in the resource (possibly fictional), etc."""@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """Additional types of event shall be defined as
            new sub-classes of event."""@en ;
	rdfs:label "Event"@en .

ebucore:AudioProgramme rdfs:label "Audio programme"@en ;
	rdfs:comment """A set of one or more audioContent that derive from the same material,
				i.e. an audioMultiplex, and the definition of its multiplexed audioContents (e.g.
				foreground and commentary, background music)."""@en ;
	rdfs:subClassOf ebucore:Programme .

ebucore:Programme a owl:Class .

ebucore:ContainerFormat a owl:Class ;
	rdfs:label "Container format"@en ;
	rdfs:comment """The container or wrapper format used to package
            audio, video and data components of a Mediaresource. The format is defined as free text
            or pointing at a term in a classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_ContainerFormatCS.rdf."""@en ;
	rdfs:subClassOf ebucore:Format .

ebucore:ClosedSubtitling a owl:Class ;
	rdfs:comment """Closed subtitles are provided as separate
            content."""@en ;
	rdfs:label "Closed subtitling"@en ;
	rdfs:subClassOf ebucore:Subtitling .

ebucore:TVProgramme a owl:Class ;
	rdfs:comment """A programme for distribution on television
            channels."""@en ;
	rdfs:label "TV Programme"@en ;
	rdfs:subClassOf ebucore:Programme .

ebucore:AudioBlockBinaural a owl:Class ;
	rdfs:subClassOf ebucore:AudioBlock ;
	rdfs:comment "For binaural audio, where playback is over headphones."@en ;
	rdfs:label "Binaural"@en .

ebucore:Subject a owl:Class ;
	rdfs:subClassOf skos:Concept ;
	rdfs:comment """A term describing the topic covered by the
            BusinessObject or resource. This is provided as free text in an annotation label or as
            an identifier pointing to a term in a classification scheme."""@en ;
	rdfs:label "Subject"@en .

ebucore:Disclaimer a owl:Class ;
	rdfs:label "Disclaimer"@en ;
	rdfs:comment """To provide a disclaimer of any
            form."""@en ;
	rdfs:subClassOf ebucore:Rights .

ebucore:AudioEncodingFormat a owl:Class ;
	rdfs:comment "The encoding format for the audio."@en ;
	rdfs:label "Audio encoding format"@en ;
	rdfs:subClassOf ebucore:EncodingFormat .

ebucore:EncodingFormat a owl:Class .

ebucore:Tag a owl:Class ;
	rdfs:subClassOf ebucore:Annotation ;
	rdfs:label "Tag"^^xsd:string ;
	rdfs:comment """An annotation specific to a particular
            timestamp in audiovisual MediaResources."""^^xsd:string .

ebucore:Rights rdfs:comment """To provide information on the rights, including
            intellectual property, related to a BusinessObject or Resource."""@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Rights"@en .

ebucore:Language a owl:Class ;
	rdfs:comment """To provide information on languages present in
            the BusinessObject and its purpose. This is provided as free text in an annotation label
            or as an identifier pointing to a term in a classification scheme.Other language
            specific types may be added as subclasses of language."""@en ;
	rdfs:label "Language"@en ;
	rdfs:subClassOf skos:Concept .

ebucore:CaptioningFormat a owl:Class ;
	rdfs:subClassOf ebucore:DataFormat ;
	rdfs:label "Captioning format"@en ;
	rdfs:comment """To define the format of captioning.
            Captioning's main use isfor hard of hearing transcription. This is provided as
            free text in an annotation label or as an identifier pointing to a term in a
            classification scheme."""@en .

ebucore:AudioChannelHOA a owl:Class ;
	rdfs:label "HOA"@en ;
	rdfs:comment "For scene-based audio where Ambisonics and HOA are used."@en ;
	rdfs:subClassOf ebucore:AudioChannel .

ebucore:AudioPackBinaural a owl:Class ;
	rdfs:label "Binaural"@en ;
	rdfs:comment "For binaural audio, where playback is over headphones."@en ;
	rdfs:subClassOf ebucore:AudioPack .

ebucore:AudioPack a owl:Class .

ebucore:Keyword a owl:Class ;
	rdfs:comment """To proivde keywords and define key concepts
            illustrating the content of the Resource or EditorialObject. This is provided as free
            text in an annotation label or as an identifier pointing to a term in a classification
            scheme."""@en ;
	rdfs:label "Keyword"@en ;
	rdfs:subClassOf skos:Concept .

ebucore:ColourSpace a owl:Class ;
	rdfs:comment """The CoulourSpace of a VideoResource. A
            ColourSpace is defined as free text in an annotation label or as an identifier pointing
            to a term in a classification scheme such as
            http://www.ebu.ch/metadata/ontologies/skos/ebu_ColourCodeCS.rdf."""@en ;
	rdfs:label "Colour space"@en ;
	rdfs:subClassOf ebucore:Format .

ebucore:Brand a owl:Class ;
	rdfs:label "Brand"^^xsd:string ;
	rdfs:comment """A group of EditorialObjects having a Brand as a
            common denominator"""^^xsd:string ;
	rdfs:subClassOf ebucore:Group .

ebucore:AudioFormat a owl:Class ;
	rdfs:subClassOf ebucore:Format ;
	rdfs:comment """The technical characteristics of an
            AudioResource such as the sampleRate. This is provided as free text in an annotation
            label or as an identifier pointing to a term in a classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_AudioFormatCodeCS.rdf."""@en ;
	rdfs:label "Audio Format"@en .

ebucore:PublicationChannel a owl:Class ;
	rdfs:comment """The name of the channel through which a
            Resource has been published as a PublicationEvent. A PublicationChannel can use a
            variety of medias e.g. broadcast or online."""@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Publication Channel"@en .

ebucore:Track rdfs:subClassOf ebucore:Resource .

ebucore:Resource a owl:Class .

ebucore:Track rdfs:comment """Audiovisual content can be composed of audio,
            video and data Tracks (including captioning and subtitling)."""@en ;
	rdfs:label "Track"@en .

ebucore:Component rdfs:comment "A component e.g. audio, video, data or else or a MediaResource or Essence."@en ;
	rdfs:label "Component"@en ;
	rdfs:subClassOf ebucore:Essence .

ebucore:AudioTrack a owl:Class ;
	rdfs:comment """An audioTrack is the basic audio data container of a medium. Attribute is
				an unambiguous reference to this container in a given medium."""@en ;
	rdfs:label "Audio track"@en ;
	rdfs:comment """An audioTrack object defines a component of an audioStream.
				A single set of samples or data in the storage medium."""@en , """Represents a physical container or carrier to hold an audio stream. This
				should be usually defined by many attributes such as ID, format (e.g. 48 kHz/24
				bits), linkage information (e.g. odd/even)â¦"""@en ;
	rdfs:subClassOf ebucore:Track .

ebucore:Series a owl:Class ;
	rdfs:subClassOf ebucore:Group ;
	rdfs:label "Series"^^xsd:string ;
	rdfs:comment """Series is a particular type of collection. TV
            or Radio Series are composed of Episodes."""^^xsd:string .

ebucore:ExploitationIssues a owl:Class ;
	rdfs:subClassOf ebucore:Rights ;
	rdfs:comment """To highlight potential exploitation
            issues."""@en ;
	rdfs:label "Exploitation issues"@en .

ebucore:AudioStreamFormat a owl:Class ;
	rdfs:comment "The format of an AudioStream, e.g. PCM_FrontLeft or Dolby E."@en ;
	rdfs:label "Audio stream format"@en ;
	rdfs:subClassOf ebucore:AudioFormat .

ebucore:PublicationPlanning a owl:Class ;
	rdfs:comment "A collection of PublicationEvents organised as a PublicationPlanning."@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Publication History"@en .

ebucore:Contact rdfs:subClassOf ebucore:Agent .

ebucore:Agent a owl:Class .

ebucore:Contact rdfs:comment "A physical person."@en ;
	rdfs:label "Contact"@en .

ebucore:AudioPackDirectSpeakers a owl:Class ;
	rdfs:label "Direct speakers"@en ;
	rdfs:comment """For channel-based audio, where each channel feeds a
				speaker directly."""@en ;
	rdfs:subClassOf ebucore:AudioPack .

ebucore:AudioChannel rdfs:comment """An audioChannel represents a single sequence of audio samples. It
				is sub-divided in the time domain into audioBlocks, which is must contain at
				least one of. The typeDefintion of the audioChannel format specifies the type of
				audio it is describing, and also determines which parameters are used within its
				audioBlock children."""^^xsd:string ;
	rdfs:label "Audio channel"^^xsd:string ;
	rdfs:subClassOf ebucore:Essence .

ebucore:Location a owl:Class ;
	rdfs:label "Location"^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:comment """A type of location is defined as a sub-class of 
            location."""@en , """A location related to the media resource, e.g.
            depicted in the resource (possibly fictional) or where the resource was created
            (shooting location), etc."""@en , """This is provided as free text in an annotation
            label or as an identifier pointing to a term in a classification scheme."""@en .

ebucore:Topic a owl:Class ;
	rdfs:label "Topic"@en ;
	rdfs:comment """A type subject for use in some contexts. This
            is provided as free text in an annotation label or as an identifier pointing to a term
            in a classification scheme."""@en ;
	rdfs:subClassOf ebucore:Subject .

ebucore:Character a owl:Class ;
	rdfs:subClassOf ebucore:Agent ;
	rdfs:comment "A fictitious contact / person."@en ;
	rdfs:label "Character"@en .

ebucore:Department a owl:Class ;
	rdfs:comment """A department within and
            organisation."""@en ;
	rdfs:label "Department"@en ;
	rdfs:subClassOf ebucore:Organisation .

ebucore:Organisation a owl:Class .

ebucore:PublicationEvent a owl:Class ;
	rdfs:comment """To describe any manifestation of a media
            resource on any media (live, on demand, catch-up TV, etc.) and the appropriate
            PublciationChannel."""@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Publication Event"@en .

ebucore:Resource rdfs:comment """A manifestation of a
            BusinessObject."""@en ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Resource"@en .

ebucore:AudioBlockDirectSpeakers a owl:Class ;
	rdfs:subClassOf ebucore:AudioBlock ;
	rdfs:comment """For channel-based audio, where each channel feeds a
				speaker directly."""@en ;
	rdfs:label "Direct speakers"@en .

ebucore:AudienceLevel a owl:Class ;
	rdfs:subClassOf ebucore:TargetAudience .

ebucore:TargetAudience a owl:Class .

ebucore:AudienceLevel rdfs:comment """The age or occupational group of users for which the BusinessObject
            or resource has been designed or conceptualised."""@en ;
	rdfs:label "Audience level"@en .

ebucore:Agent rdfs:subClassOf foaf:Agent .

foaf:Agent a owl:Class .

ebucore:Agent rdfs:comment """A person / contact or organisation contributing
            to the media resource."""@en ;
	rdfs:label "Agent"@en .

core:Event a owl:Class ;
	rdfs:subClassOf ebucore:Event ;
	rdfs:label "Event"@en ;
	rdfs:comment "Music events, historic events, sports events, politics events, e.t.c."@en .

ebucore:Captioning a owl:Class ;
	rdfs:comment """To signal the presence of hard of hearing
            captioning."""@en ;
	rdfs:label "Captioning"@en ;
	rdfs:subClassOf ebucore:DataTrack .

ebucore:Copyright a owl:Class ;
	rdfs:subClassOf ebucore:Rights ;
	rdfs:comment """To provide a copyright
            statement."""@en ;
	rdfs:label "Copyright"@en .

ebucore:RadioProgramme a owl:Class ;
	rdfs:subClassOf ebucore:Programme ;
	rdfs:comment """A programme for distribution on radio
            channels."""@en ;
	rdfs:label "Radio Programme"@en .

ebucore:Part a owl:Class ;
	rdfs:subClassOf ebucore:EditorialObject ;
	rdfs:comment """One of more media fragment (audio, video, data)
            composing an audiovisual media resource. In other ontolgies fragment is often referred
            to e.g. as a 'part' or 'segment' or
            'fragment'."""@en , """A Fragment is a particular section of a
            MediaResource identified by a start and end time or duration. Fragment can also be
            called segment or part."""@en ;
	rdfs:label "Part, Fragment, Segment"@en .

ebucore:Role a owl:Class ;
	rdfs:subClassOf skos:Concept ;
	rdfs:comment """To define the role / action of an agent. This
            is provided as free text in an annotation label or as an identifier pointing to a term
            in a classification scheme."""@en ;
	rdfs:label "Role"@en .

ebucore:EncodingFormat rdfs:subClassOf ebucore:Format ;
	rdfs:comment """To provide a definition of the encoding format
            for audio and video. This is provided as free text in an annotation label or as an
            identifier pointing to a term in a classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_AudioCompressionCodeCS.rdf or
            http://www.ebu.ch/metadata/ontologies/skos/ebu_VideoCompressionCodeCS.rdf."""@en ;
	rdfs:label "Encoding"@en .

ebucore:ContainerMimeType a owl:Class ;
	rdfs:label "Container Mime type"@en ;
	rdfs:comment """The definition of the container if available as
            a MIME type. This is provided as free text in an annotation label or as an identifier
            pointing to a term in a classification scheme. For more information:
            http://www.iana.org/assignments/media-types/application/index.html."""@en ;
	rdfs:subClassOf ebucore:Format .

ebucore:Genre a owl:Class ;
	rdfs:subClassOf ebucore:Type .

ebucore:Type a owl:Class .

