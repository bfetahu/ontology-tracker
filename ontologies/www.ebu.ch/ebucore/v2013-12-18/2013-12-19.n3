@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl2xml: <http://www.w3.org/2006/12/owl2-xml#> .
@prefix ebucore: <http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .

<file:///ebucore/ebucore_2013_12_18.rdf> a owl:Ontology ;
	rdfs:comment "This version of EBUCore RDF imports SKOS. Genre and similar concepts are now subclasses of skos:Concept. As such, their value should used skos properties or by default rdfs properties such as rdfs:Label. Names and definitions (e.g. genreName) have been left for backward compatibility purposes."^^xsd:string , "The EBUCore ontology now includes the new EBUCore audio model"^^xsd:string .

ebucore:Rating a owl:Class ;
	rdfs:comment """This is provided as free text in an annotation
            label or as an identifier pointing to a term in a classification scheme."""^^xsd:string , """All the information about the rating/evaluation
            given to a media resource by an Agent i.e. a person/Contact or
            Organisation."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Rating"^^xsd:string .

ebucore:Collection a owl:Class ;
	rdfs:comment """A group of EditorialObjects. There can be many
            types of collections for which specific sub-classes should be defined. In the worl of
            archives, A collection corresponds to all items belonging to an individual /
            collector."""^^xsd:string ;
	owl:disjointWith ebucore:Series .

ebucore:Series a owl:Class .

ebucore:Collection owl:disjointWith ebucore:Season .

ebucore:Season a owl:Class .

ebucore:Collection rdfs:subClassOf ebucore:Group .

ebucore:Group a owl:Class .

ebucore:Collection rdfs:label "Collection"^^xsd:string .

ebucore:Thumbnail a owl:Class ;
	rdfs:comment """A thumbnail is a low resolution picture that
            can be associated with EditorialObjects or e.g. MediaResources or
            Contacts"""^^xsd:string ;
	rdfs:subClassOf ebucore:Picture .

ebucore:Picture a owl:Class .

ebucore:Thumbnail rdfs:label "Thumbnail"^^xsd:string .

ebucore:BusinessObject a owl:Class ;
	rdfs:comment """An image, a document, an annotation
            (descriptive textual metadata or audio/video tag), a tag (time related in audiovisual
            media resources), or an audiovisual media resource (optionally composed of one or more
            fragment / part and / or audio, video data tracks). Other types of BusinessObjects may
            be defined as subclasses."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Business Object"^^xsd:string ;
	owl:disjointWith ebucore:Resource .

ebucore:Resource a owl:Class .

ebucore:IPRRestrictions a owl:Class ;
	rdfs:comment """To provide information on intellectual
            property."""^^xsd:string ;
	owl:disjointWith ebucore:RightsClearance .

ebucore:RightsClearance a owl:Class .

ebucore:IPRRestrictions rdfs:subClassOf ebucore:Rights .

ebucore:Rights a owl:Class .

ebucore:Asset a owl:Class ;
	rdfs:comment """The Class \"Asset\" is an
            object to which an identifier will be associated at commissioning. It will serve as a
            central reference point to manage rights associated to EditorialObjects, Resources,
            MediaResources or Essences, and PublicationEvents (distribution and exploitation
            conditions)."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Asset"^^xsd:string .

ebucore:Essence a owl:Class ;
	rdfs:comment """In some audiovisual standardisation groups,
            Essence is preferred to MediaResource. In also has subclasses well known as MediaObject
            clustered in AudioObjects and VideoObjects."""^^xsd:string ;
	owl:disjointWith ebucore:Part .

ebucore:Part a owl:Class .

ebucore:Essence rdfs:subClassOf ebucore:MediaResource .

ebucore:MediaResource a owl:Class .

ebucore:Essence rdfs:label "Essence"^^xsd:string .

ebucore:AudioContent a owl:Class ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Audio content"^^xsd:string ;
	owl:disjointWith ebucore:AudioObject .

ebucore:AudioObject a owl:Class .

ebucore:AudioContent owl:disjointWith ebucore:AudioProgramme .

ebucore:AudioProgramme a owl:Class .

ebucore:AudioContent rdfs:comment """An audioContent defines one component of a programme (e.g. background
				music), its association with an audioGroup (e.g. a 2.0 audioPackFormat of
				audioChannelFormats for stereo reproduction), its association with an
				audioStreamFormat, and its set of loudness parameters."""^^xsd:string .

ebucore:RightsClearance rdfs:comment """To signal that rights have been cleared (or
            not)"""^^xsd:string ;
	rdfs:subClassOf ebucore:Rights .

ebucore:PublicationHistory a owl:Class ;
	rdfs:comment """A collection of PublicationEvents through which
            a resource has been published."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Publication History"^^xsd:string .

ebucore:Format a owl:Class ;
	rdfs:comment """The format provides technical information on
            the format of a Resource. A BusinessObject can be instantiated in a variety of Resources
            each in a particular Format. Other specific data formats may be defined as subclasses of
            format."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Format"^^xsd:string .

ebucore:EditorialObject a owl:Class ;
	rdfs:comment """In the audiovisual domain, the Class
            â€œEditorialObjectâ€� transforms a commissioned concept into an editorial definition of a
            MediaResource before fabrication (in the Production Domain) and Distribution (in the
            Distribution Domain). An â€œEditorialObjectâ€� is a set of descriptive metadata summarising
            e.g. editing decisions. An â€œEditorialObjectâ€� can also be a part of an â€œEditorialObjectâ€�,
            which is defined by its start time and duration. An EditorialObject can also be a group
            of EditorialObjects. For example a series composed of episodes is defined as an
            EditorialObject."""^^xsd:string ;
	rdfs:subClassOf ebucore:BusinessObject ;
	rdfs:label "Editorial Object"^^xsd:string .

ebucore:ProductionDevice a owl:Class ;
	rdfs:comment """A device that was used during the creation
            process ."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	ebucore:ratingScaleMin "Production Device"^^xsd:string ;
	rdfs:label "Production device"^^xsd:string .

ebucore:DataFormat a owl:Class ;
	owl:disjointWith ebucore:DocumentFormat .

ebucore:DocumentFormat a owl:Class .

ebucore:DataFormat owl:disjointWith ebucore:MimeType .

ebucore:MimeType a owl:Class .

ebucore:DataFormat owl:disjointWith ebucore:EncodingFormat .

ebucore:EncodingFormat a owl:Class .

ebucore:DataFormat rdfs:subClassOf ebucore:Format ;
	owl:disjointWith ebucore:FileFormat .

ebucore:FileFormat a owl:Class .

ebucore:DataFormat owl:disjointWith ebucore:ImageFormat .

ebucore:ImageFormat a owl:Class .

ebucore:DataFormat owl:disjointWith ebucore:Medium .

ebucore:Medium a owl:Class .

ebucore:DataFormat rdfs:comment """To provide addtional technical information on
            the characteristics of data streams in a MediaResource including but not limited to
            AncillaryData, Subtilting and Captioning. Additional specific data format may be defined
            as subclasses of DataFormat."""^^xsd:string ;
	owl:disjointWith ebucore:VideoFormat .

ebucore:VideoFormat a owl:Class .

ebucore:AudioStream a owl:Class ;
	rdfs:subClassOf ebucore:Stream .

ebucore:Stream a owl:Class .

ebucore:AudioStream rdfs:label "Audio stream"^^xsd:string .

ebucore:Subtitling a owl:Class ;
	rdfs:comment """To signal the presence of subtitles for
            translation in alternative languages."""^^xsd:string ;
	rdfs:subClassOf ebucore:DataTrack .

ebucore:DataTrack a owl:Class .

ebucore:Subtitling rdfs:label "Subtitling"^^xsd:string .

ebucore:Stream rdfs:label "Stream"^^xsd:string ;
	rdfs:subClassOf ebucore:Component .

ebucore:Component a owl:Class .

ebucore:DataTrack rdfs:comment """Ancillary data track e.g. Â¨captioning\"
            or \"subtitling\" in addition to video and audio tracks."""^^xsd:string ;
	owl:disjointWith ebucore:VideoTrack .

ebucore:VideoTrack a owl:Class .

ebucore:DataTrack rdfs:subClassOf ebucore:Track .

ebucore:Track a owl:Class .

ebucore:DataTrack rdfs:label "Data track"^^xsd:string .

ebucore:SoftwareCodec a owl:Class ;
	rdfs:comment """To provide information on a software
            codec"""^^xsd:string ;
	rdfs:subClassOf ebucore:ProductionDevice ;
	rdfs:label "Software Codec"^^xsd:string .

ebucore:NewsItem a owl:Class ;
	owl:disjointWith ebucore:Group , ebucore:Part , ebucore:Programme .

ebucore:Programme a owl:Class .

ebucore:NewsItem rdfs:subClassOf ebucore:EditorialObject ;
	rdfs:label "News Item"^^xsd:string .

ebucore:StorageType a owl:Class ;
	rdfs:comment """The type of storage used for the repository.
            This is provided as free text in an annotation label or as an identifier pointing to a
            term in a classification scheme."""^^xsd:string ;
	rdfs:subClassOf skos:Concept .

skos:Concept rdfs:isDefinedBy <http://www.w3.org/2004/02/skos/core> .

ebucore:Dopesheet a owl:Class ;
	rdfs:subClassOf ebucore:Document .

ebucore:Document a owl:Class .

ebucore:Dopesheet rdfs:label "Dopesheet"^^xsd:string .

ebucore:Annotation a owl:Class ;
	rdfs:comment """A set of descriptive or technical metadata.
            Annotation can also be made in the form of a document or media resource such as an or
            video audio file . An annotation can exist on its own and be related / associated to
            other editorial objects or resources."""^^xsd:string ;
	owl:disjointWith ebucore:EditorialObject , ebucore:Document ;
	rdfs:subClassOf ebucore:BusinessObject ;
	rdfs:label "Annotation"^^xsd:string .

ebucore:AudioBlock a owl:Class ;
	rdfs:comment """DirectSpeakers: For channel-based audio, where each channel feeds a
				speaker directly. Type 0001."""^^xsd:string ;
	rdfs:subClassOf ebucore:Essence ;
	rdfs:comment "HOA: For scene-based audio where Ambisonics and HOA are used. Type 0004."^^xsd:string ;
	owl:disjointWith ebucore:AudioChannel .

ebucore:AudioChannel a owl:Class .

ebucore:AudioBlock rdfs:comment """An audioBlock represents a single sequence of audio samples with
				fixed parameters, including position, within a specified time. The sub-elements with
				audioBlock are dependent upon the typeDefintion or typeLabel of the parent
				audioChannel element."""^^xsd:string , """Matrix: For channel-based audio where channels are matrixed together,
				such as Mid-Side, Lt/Rt. Type 0002."""^^xsd:string , """Objects: For object-based audio where channels represent audio objects
				(or parts of objects), so include positional information. Type 0003."""^^xsd:string , """Binaural:For binaural audio, where playback is over headphones. Type
				0005."""^^xsd:string ;
	rdfs:label "Audio block"^^xsd:string ;
	owl:disjointWith ebucore:AudioPack .

ebucore:AudioPack a owl:Class .

ebucore:Event a owl:Class ;
	rdfs:comment """An event related to the media resource, e.g.
            depicted in the resource (possibly fictional), etc."""^^xsd:string , """Additional types of event shall be defined as
            new sub-classes of event."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Event"^^xsd:string .

ebucore:AudioProgramme rdfs:subClassOf ebucore:Programme ;
	rdfs:label "Audio programme"^^xsd:string ;
	owl:disjointWith ebucore:AudioContent ;
	rdfs:comment """A set of one or more audioContent that derive from the same material,
				i.e. an audioMultiplex, and the definition of its multiplexed audioContents (e.g.
				foreground and commentary, background music)."""^^xsd:string .

ebucore:ContainerFormat a owl:Class ;
	owl:disjointWith ebucore:VideoFormat , ebucore:ContainerMimeType .

ebucore:ContainerMimeType a owl:Class .

ebucore:ContainerFormat owl:disjointWith ebucore:FileFormat ;
	rdfs:subClassOf ebucore:Format ;
	owl:disjointWith ebucore:ImageFormat , ebucore:Medium , ebucore:DocumentFormat , ebucore:MimeType ;
	rdfs:comment """The container or wrapper format used to package
            audio, video and data components of a Mediaresource. The format is defined as free text
            or pointing at a term in a classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_ContainerFormatCS.rdf"""^^xsd:string ;
	owl:disjointWith ebucore:EncodingFormat , ebucore:DataFormat .

ebucore:ClosedSubtitling a owl:Class ;
	rdfs:comment """Closed subtitles are provided as separate
            content."""^^xsd:string ;
	owl:disjointWith ebucore:OpenSubtitling .

ebucore:OpenSubtitling a owl:Class .

ebucore:ClosedSubtitling rdfs:subClassOf ebucore:Subtitling ;
	rdfs:label "Closed subtitling"^^xsd:string .

ebucore:TVProgramme a owl:Class ;
	rdfs:comment """A programme for distribution on television
            channels."""^^xsd:string ;
	rdfs:subClassOf ebucore:Programme ;
	rdfs:label "TV Programme"^^xsd:string .

ebucore:HOA a owl:Class ;
	rdfs:subClassOf ebucore:AudioBlock ;
	rdfs:label "HOA"^^xsd:string .

ebucore:Subject a owl:Class ;
	rdfs:comment """A term describing the topic covered by the
            BusinessObject or resource. This is provided as free text in an annotation label or as
            an identifier pointing to a term in a classification scheme."""^^xsd:string ;
	rdfs:subClassOf skos:Concept .

ebucore:Tag a owl:Class ;
	rdfs:comment """An annotation specific to a particular
            timestamp in audiovisual MediaResources."""^^xsd:string ;
	rdfs:subClassOf ebucore:Annotation ;
	rdfs:label "Tag"^^xsd:string .

ebucore:AudioEncodingFormat a owl:Class ;
	rdfs:label "Audio encoding format"^^xsd:string ;
	rdfs:subClassOf ebucore:EncodingFormat .

ebucore:Disclaimer a owl:Class ;
	rdfs:comment """To provide a disclaimer of any
            form."""^^xsd:string ;
	owl:disjointWith ebucore:RightsClearance , ebucore:IPRRestrictions , ebucore:ExploitationIssues .

ebucore:ExploitationIssues a owl:Class .

ebucore:Disclaimer rdfs:subClassOf ebucore:Rights .

ebucore:Rights rdfs:comment """To provide information on the rights, including
            intellectual property, related to a BusinessObject or Resource."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Rights"^^xsd:string .

ebucore:Language a owl:Class ;
	rdfs:comment """To provide information on languages present in
            the BusinessObject and its purpose. This is provided as free text in an annotation label
            or as an identifier pointing to a term in a classification scheme.Other language
            specific types may be added as subclasses of language."""^^xsd:string ;
	rdfs:subClassOf skos:Concept .

ebucore:HardwareCodec a owl:Class ;
	rdfs:comment """To provide information on a hardware
            codec"""^^xsd:string ;
	owl:disjointWith ebucore:SoftwareCodec ;
	rdfs:subClassOf ebucore:ProductionDevice ;
	rdfs:label "Hardware Codec"^^xsd:string .

ebucore:CaptioningFormat a owl:Class ;
	rdfs:comment """To define the format of captioning.
            Captioning's main use isfor hard of hearing transcription. This is provided as
            free text in an annotation label or as an identifier pointing to a term in a
            classification scheme."""^^xsd:string ;
	owl:disjointWith ebucore:SubtitlingFormat .

ebucore:SubtitlingFormat a owl:Class .

ebucore:CaptioningFormat rdfs:subClassOf ebucore:DataFormat .

ebucore:Keyword a owl:Class ;
	rdfs:comment """To proivde keywords and define key concepts
            illustrating the content of the Resource or EditorialObject. This is provided as free
            text in an annotation label or as an identifier pointing to a term in a classification
            scheme."""^^xsd:string ;
	rdfs:subClassOf skos:Concept .

ebucore:ColourSpace a owl:Class ;
	owl:disjointWith ebucore:ContainerFormat , ebucore:DocumentFormat ;
	rdfs:subClassOf ebucore:Format ;
	owl:disjointWith ebucore:VideoFormat ;
	rdfs:comment """The CoulourSpace of a VideoResource. A
            ColourSpace is defined as free text in an annotation label or as an identifier pointing
            to a term in a classification scheme such as
            http://www.ebu.ch/metadata/ontologies/skos/ebu_ColourCodeCS.rdf"""^^xsd:string ;
	owl:disjointWith ebucore:FileFormat , ebucore:MimeType , ebucore:ImageFormat , ebucore:DataFormat , ebucore:ContainerMimeType , ebucore:EncodingFormat , ebucore:Medium .

ebucore:AudioFormat a owl:Class ;
	owl:disjointWith ebucore:Medium , ebucore:ColourSpace , ebucore:EncodingFormat ;
	rdfs:comment """The technical characteristics of an
            AudioResource such as the sampleRate. This is provided as free text in an annotation
            label or as an identifier pointing to a term in a classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_AudioFormatCodeCS.rdf."""^^xsd:string ;
	owl:disjointWith ebucore:DataFormat , ebucore:VideoFormat , ebucore:ImageFormat ;
	rdfs:label "Audio Format"^^xsd:string ;
	owl:disjointWith ebucore:DocumentFormat , ebucore:ContainerMimeType ;
	rdfs:subClassOf ebucore:Format ;
	owl:disjointWith ebucore:FileFormat , ebucore:MimeType , ebucore:ContainerFormat .

ebucore:Brand a owl:Class ;
	rdfs:comment """A group of EditorialObjects having a Brand as a
            common denominator"""^^xsd:string ;
	owl:disjointWith ebucore:Series , ebucore:Season , ebucore:Collection ;
	rdfs:subClassOf ebucore:Group ;
	rdfs:label "Brand"^^xsd:string .

ebucore:Binaural a owl:Class ;
	rdfs:subClassOf ebucore:AudioBlock ;
	rdfs:label "Binaural"^^xsd:string .

ebucore:PublicationChannel a owl:Class ;
	rdfs:comment """The name of the channel through which a
            Resource has been published as a PublicationEvent. A PublicationChannel can use a
            variety of medias e.g. broadcast or online."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Publication Channel"^^xsd:string .

ebucore:TargetGroup a owl:Class ;
	rdfs:comment """The group of user for which the BusinessObject
            or resource has been designed or conceptualised."""^^xsd:string ;
	rdfs:subClassOf ebucore:TargetAudience .

ebucore:TargetAudience a owl:Class .

ebucore:Track rdfs:comment """Audiovisual content can be composed of audio,
            video and data Tracks (including captioning and subtitling)."""^^xsd:string ;
	rdfs:subClassOf ebucore:Resource ;
	rdfs:label "Track"^^xsd:string .

ebucore:Component rdfs:label "Component"^^xsd:string ;
	rdfs:subClassOf ebucore:Essence .

ebucore:AudioTrack a owl:Class ;
	rdfs:label "Audio track"^^xsd:string ;
	owl:disjointWith ebucore:VideoTrack , ebucore:DataTrack ;
	rdfs:subClassOf ebucore:Track ;
	rdfs:comment """An audioTrack object defines a component of an audioStream.
				A single set of samples or data in the storage medium."""^^xsd:string , """An audioTrack is the basic audio data container of a medium. Attribute is
				an unambiguous reference to this container in a given medium"""^^xsd:string , """Represents a physical container or carrier to hold an audio stream. This
				should be usually defined by many attributes such as ID, format (e.g. 48 kHz/24
				bits), linkage information (e.g. odd/even)…"""^^xsd:string .

ebucore:Series rdfs:comment """Series is a particular type of collection. TV
            or Radio Series are composed of Episodes."""^^xsd:string ;
	rdfs:subClassOf ebucore:Group ;
	rdfs:label "Series"^^xsd:string .

ebucore:ExploitationIssues rdfs:comment """To highlight potential exploitation
            issues."""^^xsd:string ;
	owl:disjointWith ebucore:RightsClearance , ebucore:IPRRestrictions ;
	rdfs:subClassOf ebucore:Rights .

ebucore:Contact a owl:Class ;
	rdfs:comment "A physical person"^^xsd:string ;
	owl:disjointWith ebucore:Organisation .

ebucore:Organisation a owl:Class .

ebucore:Contact rdfs:subClassOf ebucore:Agent .

ebucore:Agent a owl:Class .

ebucore:Contact rdfs:label "Contact"^^xsd:string .

ebucore:Matrix a owl:Class ;
	rdfs:subClassOf ebucore:AudioBlock ;
	rdfs:label "Matrix"^^xsd:string .

ebucore:AudioChannel rdfs:subClassOf ebucore:Essence ;
	rdfs:label "Audio channel"^^xsd:string ;
	owl:disjointWith ebucore:AudioPack , ebucore:AudioBlock ;
	rdfs:comment """An audioChannel represents a single sequence of audio samples. It
				is sub-divided in the time domain into audioBlocks, which is must contain at
				least one of. The typeDefintion of the audioChannel format specifies the type of
				audio it is describing, and also determines which parameters are used within its
				audioBlock children."""^^xsd:string .

ebucore:Location a owl:Class ;
	rdfs:comment """This is provided as free text in an annotation
            label or as an identifier pointing to a term in a classification scheme."""^^xsd:string , """A type of location is defined as a sub-class of
            location."""^^xsd:string , """A location related to the media resource, e.g.
            depicted in the resource (possibly fictional) or where the resource was created
            (shooting location), etc."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Location"^^xsd:string .

ebucore:Topic a owl:Class ;
	rdfs:comment """A type subject for use in some contexts. This
            is provided as free text in an annotation label or as an identifier pointing to a term
            in a classification scheme."""^^xsd:string ;
	rdfs:subClassOf ebucore:Subject .

ebucore:Character a owl:Class ;
	rdfs:comment "A fictitious contact / person"^^xsd:string ;
	owl:disjointWith ebucore:Organisation , ebucore:Contact ;
	rdfs:subClassOf ebucore:Agent ;
	rdfs:label "Character"^^xsd:string .

ebucore:Department a owl:Class ;
	rdfs:comment """A department within and
            organisation"""^^xsd:string ;
	rdfs:subClassOf ebucore:Organisation ;
	rdfs:label "Department"^^xsd:string .

ebucore:PublicationEvent a owl:Class ;
	rdfs:comment """To describe any manifestation of a media
            resource on any media (live, on demand, catch-up TV, etc.) and the appropriate
            PublciationChannel."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Publication Event"^^xsd:string .

ebucore:Resource rdfs:comment """A manifestation of a
            BusinessObject."""^^xsd:string ;
	rdfs:subClassOf owl:Thing ;
	rdfs:label "Resource"^^xsd:string .

ebucore:Agent rdfs:comment """A person / contact or organisation contributing
            to the media resource"""^^xsd:string ;
	rdfs:label "Agent"^^xsd:string ;
	rdfs:subClassOf owl:Thing .

ebucore:ParentalGuidance a owl:Class ;
	rdfs:comment """The audience by which the Resource can be
            seen."""^^xsd:string ;
	owl:disjointWith ebucore:TargetGroup ;
	rdfs:subClassOf ebucore:TargetAudience .

ebucore:Captioning a owl:Class ;
	rdfs:comment """To signal the presence of hard of hearing
            captioning."""^^xsd:string ;
	owl:disjointWith ebucore:Subtitling ;
	rdfs:subClassOf ebucore:DataTrack ;
	rdfs:label "Captioning"^^xsd:string .

ebucore:RadioProgramme a owl:Class ;
	rdfs:comment """A programme for distribution on radio
            channels"""^^xsd:string ;
	owl:disjointWith ebucore:TVProgramme ;
	rdfs:subClassOf ebucore:Programme ;
	rdfs:label "Radio Programme"^^xsd:string .

ebucore:Copyright a owl:Class ;
	rdfs:comment """To provide a copyright
            statement."""^^xsd:string ;
	owl:disjointWith ebucore:RightsClearance , ebucore:IPRRestrictions , ebucore:ExploitationIssues , ebucore:Disclaimer , ebucore:CoverageRestrictions .

ebucore:CoverageRestrictions a owl:Class .

ebucore:Copyright rdfs:subClassOf ebucore:Rights .

ebucore:Part rdfs:comment """One of more media fragment (audio, video, data)
            composing an audiovisual media resource. In other ontolgies fragment is often referred
            to e.g. as a 'part' or 'segment' or
            'fragment'."""^^xsd:string , """A Fragment is a particular section of a
            MediaResource identified by a start and end time or duration. Fragment can also be
            called segment or part."""^^xsd:string ;
	owl:disjointWith ebucore:Programme ;
	rdfs:subClassOf ebucore:EditorialObject ;
	rdfs:label "Part, Fragment, Segment"^^xsd:string .

ebucore:Role a owl:Class ;
	rdfs:comment """To define the role / action of an agent. This
            is provided as free text in an annotation label or as an identifier pointing to a term
            in a classification scheme."""^^xsd:string ;
	rdfs:subClassOf skos:Concept .

ebucore:EncodingFormat rdfs:comment """To provide a definition of the encoding format
            for audio and video. This is provided as free text in an annotation label or as an
            identifier pointing to a term in a classification scheme e.g.
            http://www.ebu.ch/metadata/ontologies/skos/ebu_AudioCompressionCodeCS.rdf or
            http://www.ebu.ch/metadata/ontologies/skos/ebu_VideoCompressionCodeCS.rdf."""^^xsd:string ;
	owl:disjointWith ebucore:VideoFormat , ebucore:MimeType , ebucore:Medium , ebucore:ImageFormat , ebucore:FileFormat ;
	rdfs:subClassOf ebucore:Format .

ebucore:ContainerMimeType rdfs:subClassOf ebucore:Format ;
	owl:disjointWith ebucore:ImageFormat , ebucore:VideoFormat ;
	rdfs:comment """The definition of the container if available as
            a MIME type. This is provided as free text in an annotation label or as an identifier
            pointing to a term in a classification scheme. For more information:
            http://www.iana.org/assignments/media-types/application/index.html"""^^xsd:string ;
