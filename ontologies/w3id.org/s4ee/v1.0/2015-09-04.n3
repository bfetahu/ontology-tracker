s4ee:RemainingPauseTime
        a                owl:Class ;
        rdfs:comment     "The duration that the current slot (s4ee:activeSlotNumber) permits being paused. This element SHALL ONLY be present if the power sequence is interruptible. Otherwise, it SHALL be omitted. In case the power sequence is interruptible the following rules apply: If the element is absent this means there is no explicit pause duration restriction for the current slot; a value of 0s denoted the slot does not permit being paused."^^xsd:string ;
        rdfs:label       "Remaining pause time"^^xsd:string ;
        rdfs:subClassOf  <http://www.w3.org/2006/time#TemporalEntity> .

s4ee:dayOfWeek  a     owl:ObjectProperty ;
        rdfs:comment  "A relationship modelling the actual day of week (monday to sunday). It refers to the local time. If no utcOffset is given, it is assumed that the local time is equivalent to UTC-time."^^xsd:string ;
        rdfs:label    "day of week "^^xsd:string ;
        rdfs:range    <http://www.w3.org/2006/time#DayOfWeek> .

s4ee:evaluationPeriod
        a             owl:DatatypeProperty ;
        rdfs:comment  "In the Measurement class some valueType values can be detailed or even require information about the time period. For example, an averageValue is usually computed from values of a finite time period. For smart devices, the start and end time of the evaluationPeriod can be filled in directly. For simple sensors, the values (average, min, max, ...) could to be calculated (if desired) by the technology interface component together with the corresponding evaluationPeriod."^^xsd:string ;
        rdfs:domain   s4ee:Measurement ;
        rdfs:label    "evaluation period"^^xsd:string .

s4ee:DurationDescription
        a                owl:Class ;
        rdfs:comment     "The duration description for the duration temporal entity"^^xsd:string ;
        rdfs:label       "Duration description"^^xsd:string ;
        rdfs:subClassOf  <http://www.w3.org/2006/time#DurationDescription> .

s4ee:NativeSetup  a      owl:Class ;
        rdfs:comment     "Used in the Remote Network Management use case for add device, remove device and modify device. When sending a request to add, remove or modify a device, the NativeSetup SHALL be present in order to provide the RemoteRNW server with the necessary information for the device to add, remove or modify and the content is evaluated by the recipient to get the proper information which device to add, remove or modify. Otherwise MAY be present. Details on the content of “nativeSetup” are beyond the scope of this document as each technology and implementation typically requires different parameters to execute this task."^^xsd:string ;
        rdfs:label       "Native setup"^^xsd:string ;
        rdfs:subClassOf  owl:Thing .

s4ee:calendarWeek  a  owl:DatatypeProperty ;
        rdfs:comment  "A value between 1 and 53 stating the current calendar week number. This element refers to the local time. If no utcOffset is given, it is assumed that the local time is equivalent to UTC-time."^^xsd:string ;
        rdfs:label    "calendar week "^^xsd:string ;
        rdfs:range    xsd:unsignedInt .

s4ee:logicalConnective
        a             owl:DatatypeProperty ;
        rdfs:comment  "Used to connect different expressions, can be AND/OR"^^xsd:string ;
        rdfs:label    "logical connective"^^xsd:string ;
        rdfs:range    [ a          rdfs:Datatype ;
                        owl:oneOf  [ a          rdf:List ;
                                     rdf:first  "AND"^^xsd:string ;
                                     rdf:rest   [ a          rdf:List ;
                                                  rdf:first  "OR"^^xsd:string ;
                                                  rdf:rest   ()
                                                
                                                ]
                                   ]
                      ] .

s4ee:optionalSlot  a  owl:DatatypeProperty ;
        rdfs:comment  "Indicates whether a slot is optional (if set to TRUE). Otherwise, if a slot is mandatory, SHALL be omitted or set to FALSE. Note: This element applies to every repetition of the slot number."^^xsd:string ;
        rdfs:label    "optional slot"^^xsd:string ;
        rdfs:range    xsd:boolean .

s4ee:PowerProfile  a     owl:Class ;
        rdfs:comment     "A way to model curves of power and energy over time, which also provides definitions for the modelling of power scheduling including alternative plans. With a PowerProfile, a device (or power sequences server) exposes the power sequences that are potentially relevant for the CEM (or power sequences client). "^^xsd:string ;
        rdfs:label       "Power profile"^^xsd:string ;
        rdfs:subClassOf  <https://w3id.org/saref#Profile> , owl:Thing ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   s4ee:supportsSingleSlotSchedulingOnly
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:Alternative ;
                           owl:onProperty     <https://w3id.org/saref#consistsOf>
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:Device ;
                           owl:onProperty     s4ee:belongsTo
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   s4ee:supportsReselection
                         ] ;
        rdfs:subClassOf  [ a                         owl:Restriction ;
                           owl:onClass               s4ee:Device ;
                           owl:onProperty            s4ee:belongsTo ;
                           owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:Alternative ;
                           owl:onProperty               <https://w3id.org/saref#consistsOf>
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   s4ee:alternativesCount
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   s4ee:totalSequencesCountMax
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   s4ee:nodeRemoteControllable
                         ] .

s4ee:slotNumber  a    owl:DatatypeProperty ;
        rdfs:comment  "Provides a unique slot identifier for a certain power sequence"^^xsd:string ;
        rdfs:label    "slot number"^^xsd:string ;
        rdfs:range    xsd:unsignedInt .

s4ee:notAlarmed  a    s4ee:SensorState , owl:NamedIndividual ;
        rdfs:comment  "Sensor state with value 'not alarmed'"^^xsd:string ;
        rdfs:label    "not alarmed"^^xsd:string .

s4ee:hasFunction  a   owl:ObjectProperty ;
        rdfs:comment  "A relationship identifying the functions performed by an actuator level and an actuator switch"^^xsd:string ;
        rdfs:label    "has function"^^xsd:string ;
        rdfs:range    s4ee:Function .

s4ee:action_3_set_to_MaxValue
        a             s4ee:ApplianceParameterCompatibilityAction , owl:NamedIndividual ;
        rdfs:comment  "Appliance parameter compatibility action type with value 3 = set to max value"^^xsd:string ;
        rdfs:label    "3 = set to max value"^^xsd:string .

s4ee:ScanSetup  a        owl:Class ;
        rdfs:comment     "Used in the Remote Network Management use case for scan network, the Scan Setup SHALL be present. Some communications technologies provide measures to scan which other devices are accessible. When scanning for potential new devices, the communications technology’s native devices could be announced as 'candidates'. The request for scan network is initiated by a RemoteNWM client feature of device 'CLIENT'. It is sent to the RemoteNWM server feature of device 'SERVER'. Then, device “SERVER” executes the scan network task, if possible. In case of success device 'SERVER' announces found candidates. Subsequently the whole process is defined in more detail. "^^xsd:string ;
        rdfs:label       "Scan setup"^^xsd:string ;
        rdfs:subClassOf  s4ee:DeviceConnectionSetup ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:minCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onProperty      s4ee:timeout
                         ] .

s4ee:PauseTime  a        owl:Class ;
        rdfs:comment     "The pause time of a power sequence or a slot"^^xsd:string ;
        rdfs:label       "Pause time"^^xsd:string ;
        rdfs:subClassOf  <http://www.w3.org/2006/time#TemporalEntity> .

s4ee:MaxDerivativeValue
        a                owl:Class ;
        rdfs:comment     "A possible value for a measurement"^^xsd:string ;
        rdfs:label       "Maximum derivative value"^^xsd:string ;
        rdfs:subClassOf  s4ee:Value .

s4ee:brandName  a     owl:DatatypeProperty ;
        rdfs:comment  "Provides the name of the brand of a device. Useful where the name of the brand and the vendor differs."^^xsd:string ;
        rdfs:domain   s4ee:Device ;
        rdfs:label    "brand name"^^xsd:string ;
        rdfs:range    xsd:string .

s4ee:sequenceRemoteControllable
        a             owl:DatatypeProperty ;
        rdfs:comment  "Denotes whether a power sequence is modifiable (if value is TRUE) or not (if value is FALSE). Modifiability is required to configure power sequences and slots. It is also required to change a power sequence state."^^xsd:string ;
        rdfs:domain   s4ee:PowerSequence ;
        rdfs:label    "sequence remote controllable"^^xsd:string ;
        rdfs:range    xsd:boolean .

s4ee:ApplianceParameter
        a                owl:Class ;
        rdfs:comment     "A parameter used to represent an appliance's function, e.g. 'temperature', 'spin' etc."^^xsd:string ;
        rdfs:label       "Appliance parameter"^^xsd:string ;
        rdfs:subClassOf  owl:Thing ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      <https://w3id.org/saref#hasDescription>
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   s4ee:parameterId
                         ] ;
        rdfs:subClassOf  [ a                         owl:Restriction ;
                           owl:onClass               s4ee:ApplianceParameterState ;
                           owl:onProperty            <https://w3id.org/saref#hasState> ;
                           owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:minCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onProperty      s4ee:hasTable
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:ApplianceParameterState ;
                           owl:onProperty     <https://w3id.org/saref#hasState>
                         ] .

s4ee:vendorCode  a    owl:DatatypeProperty ;
        rdfs:comment  "Provides a code for the vendor of the device as defined by the manufacturer."^^xsd:string ;
        rdfs:label    "vendor code"^^xsd:string ;
        rdfs:range    xsd:string .

s4ee:ApplianceControl
        a                owl:Class ;
        rdfs:comment     "An entity used to represent the control phase of an appliance"^^xsd:string ;
        rdfs:label       "Appliance control"^^xsd:string ;
        rdfs:subClassOf  owl:Thing ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   s4ee:isActive
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:ApplianceParameterState ;
                           owl:onProperty               s4ee:controls
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:ApplianceParameterState ;
                           owl:onProperty     s4ee:controls
                         ] .

s4ee:DeviceConnection
        a                owl:Class ;
        rdfs:comment     "In the Remote Network Management (RemoteNWM) use case represents the data used by a node in a network (i.e., a s4ee:Device ) to handle its connections with a RemoteNWM server in the network. The same s4ee:Device (or node) can have multiple device connections as it can be connected to several networks at the same time."^^xsd:string ;
        rdfs:label       "Device connection"^^xsd:string ;
        rdfs:subClassOf  owl:Thing ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:DeviceConnectionSetup ;
                           owl:onProperty     s4ee:isInitializedWith
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:NativeSetup ;
                           owl:onProperty               s4ee:hasNativeSetup
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:DeviceConnectionSetup ;
                           owl:onProperty               s4ee:isInitializedWith
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:DeviceConnectionState ;
                           owl:onProperty     <https://w3id.org/saref#hasState>
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:DeviceConnectionState ;
                           owl:onProperty               <https://w3id.org/saref#hasState>
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:NativeSetup ;
                           owl:onProperty     s4ee:hasNativeSetup
                         ] .

s4ee:scheduledPaused  a  s4ee:PowerSequenceState , owl:NamedIndividual ;
        rdfs:comment  "Power sequence state with value 'scheduled paused'"^^xsd:string ;
        rdfs:label    "scheduled paused"^^xsd:string .

s4ee:BooleanParameterTable
        a                owl:Class ;
        rdfs:comment     "A table that represents a boolean parameter"^^xsd:string ;
        rdfs:label       "Boolean parameter table"^^xsd:string ;
        rdfs:subClassOf  s4ee:ApplianceParameterTable .

s4ee:MessagingInformation
        a                owl:Class ;
        rdfs:comment     "A type of messaging"^^xsd:string ;
        rdfs:label       "Messaging information"^^xsd:string ;
        rdfs:subClassOf  s4ee:Messaging .

s4ee:removed  a       s4ee:DeviceConnectionState , owl:NamedIndividual ;
        rdfs:comment  "Device connection state with value 'removed'"^^xsd:string ;
        rdfs:label    "removed"^^xsd:string .

s4ee:levelRelative  a  s4ee:SensorState , owl:NamedIndividual ;
        rdfs:comment  "Sensor state with value 'level relative'"^^xsd:string ;
        rdfs:label    "level relative"^^xsd:string .

s4ee:hasPowerValueType
        a                   owl:ObjectProperty ;
        rdfs:comment        "A relationship representing a power value type"^^xsd:string ;
        rdfs:label          "has power value type"^^xsd:string ;
        rdfs:subPropertyOf  s4ee:hasValueType .

s4ee:EnergyExpected  a   owl:Class ;
        rdfs:comment     "A possible type of energy in a slot"^^xsd:string ;
        rdfs:label       "Energy expected"^^xsd:string ;
        rdfs:subClassOf  s4ee:Energy .

s4ee:deviceName  a    owl:DatatypeProperty ;
        rdfs:comment  "Provides the name of the device as defined by the manufacturer."^^xsd:string ;
        rdfs:domain   s4ee:Device ;
        rdfs:label    "device name"^^xsd:string ;
        rdfs:range    xsd:string .

s4ee:closed  a        s4ee:SensorState , owl:NamedIndividual ;
        rdfs:comment  "Sensor state with value 'closed'"^^xsd:string ;
        rdfs:label    "closed"^^xsd:string .

s4ee:slotActivated  a  owl:DatatypeProperty ;
        rdfs:comment  "If a slot is optional, represents the current status of the slot (true = the slot will be executed, false = the slot will not be executed). If the slot is not optional, this element SHALL be absent."^^xsd:string ;
        rdfs:domain   s4ee:Slot ;
        rdfs:label    "slot activated"^^xsd:string ;
        rdfs:range    xsd:boolean .

s4ee:Value  a            owl:Class ;
        rdfs:comment     "The value of an entity. In the monitor and control use case, this value can be 1) an integer to be compared to the current set point, according with the math operator; 2) an integer that expresses MaxValue, MinValue or DefaultValue in the ApplianceParameterSettings class. In the Measurement class, it allows to model different types of measurement values such as AverageValue, MinValue, MaxValue, AverageDerivativeValue, MinDerivativeValue, MaxDerivativeValue, StandardDeviationValue"^^xsd:string ;
        rdfs:label       "Value"^^xsd:string ;
        rdfs:subClassOf  owl:Thing ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:minCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onProperty      <https://w3id.org/saref#hasValue>
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:double ;
                           owl:onProperty     <https://w3id.org/saref#hasValue>
                         ] .

s4ee:utcOffset  a     owl:DatatypeProperty ;
        rdfs:comment  "Indicates the configured offset to UTC (e.g. PT2H [= +2 hours] for German summertime)."^^xsd:string ;
        rdfs:label    "utc offset "^^xsd:string ;
        rdfs:range    xsd:duration .

s4ee:activeSlotNumber
        a             owl:DatatypeProperty ;
        rdfs:comment  "Indicates the currently active slot number, if the power sequence state is set to 'running' or 'paused'. Otherwise it SHALL be omitted."^^xsd:string ;
        rdfs:label    "active slot number"^^xsd:string ;
        rdfs:range    xsd:unsignedInt .

s4ee:hasEnergy  a     owl:ObjectProperty ;
        rdfs:comment  "A relationship between a power sequence and its energy (in terms of value and unit of measure)"^^xsd:string ;
        rdfs:label    "has energy"^^xsd:string ;
        rdfs:range    s4ee:Energy .

s4ee:measurementId  a  owl:DatatypeProperty ;
        rdfs:comment  "Enables the identification of different sensors on one EEBus address."^^xsd:string ;
        rdfs:domain   s4ee:Measurement ;
        rdfs:label    "measurement id"^^xsd:string ;
        rdfs:range    xsd:unsignedInt .

s4ee:messagingType  a  owl:DatatypeProperty ;
        rdfs:comment  "Indicates the type of the message. Possible values are:\n-\tlogging (used for messages, that shall be stored in a log file)\n-\tinformation (messages, that shall be presented to the customer on a display; lower priority)\n-\twarning (messages, that shall be presented to the customer on a display; medium priority)\n-\talarm (messages, that shall be presented to the customer on a display; high priority; if there is an alarm device in the household, it shall generate an alarm)\n-\temergency (messages, that shall be presented to the customer on a display; very high priority; if there is an alarm device in the household, it shall generate an alarm; additionally, if possible and configured, an emergency call shall be done)\n-\tobsolete (previously sent messages, that shall be marked as obsolete)"^^xsd:string ;
        rdfs:domain   s4ee:Messaging ;
        rdfs:label    "messaging type"^^xsd:string ;
        rdfs:range    [ a          rdfs:Datatype ;
                        owl:oneOf  [ a          rdf:List ;
                                     rdf:first  "alarm"^^xsd:string ;
                                     rdf:rest   [ a          rdf:List ;
                                                  rdf:first  "emergency"^^xsd:string ;
                                                  rdf:rest   [ a          rdf:List ;
                                                               rdf:first  "information"^^xsd:string ;
                                                               rdf:rest   [ a          rdf:List ;
                                                                            rdf:first  "logging"^^xsd:string ;
                                                                            rdf:rest   [ a          rdf:List ;
                                                                                         rdf:first  "obsolete"^^xsd:string ;
                                                                                         rdf:rest   [ a          rdf:List ;
                                                                                                      rdf:first  "warning"^^xsd:string ;
                                                                                                      rdf:rest   ()
                                                                                                    
                                                                                                    ]
                                                                                       ]
                                                                          ]
                                                             ]
                                                ]
                                   ]
                      ] .

s4ee:stop  a          s4ee:ActuatorLevelFctType , owl:NamedIndividual ;
        rdfs:comment  "Function type with value STOP"^^xsd:string ;
        rdfs:label    "stop"^^xsd:string .

s4ee:hasPoint  a      owl:ObjectProperty ;
        rdfs:comment  "A relationship between an appliance parameter table and a point in this table"^^xsd:string ;
        rdfs:domain   s4ee:PointwiseParameterTable ;
        rdfs:label    "has point"^^xsd:string ;
        rdfs:range    s4ee:ParameterTablePoint .

s4ee:level  a         s4ee:SensorState , owl:NamedIndividual ;
        rdfs:comment  "Sensor state with value 'level'"^^xsd:string ;
        rdfs:label    "level"^^xsd:string .

s4ee:measurand_state_normal
        a             s4ee:MeasurandState , owl:NamedIndividual ;
        rdfs:comment  "Measurand state with value NORMAL"^^xsd:string ;
        rdfs:label    "measurand state normal"^^xsd:string .

s4ee:action_1_reset_to_OFF_value
        a             s4ee:ApplianceParameterCompatibilityAction , owl:NamedIndividual ;
        rdfs:comment  "Appliance parameter compatibility action type with value 1 = reset to OFF value"^^xsd:string ;
        rdfs:label    "1 = reset to OFF value"^^xsd:string .

s4ee:DefaultValue  a     owl:Class ;
        rdfs:comment     "A possible value for a parameter"^^xsd:string ;
        rdfs:label       "Default value"^^xsd:string ;
        rdfs:subClassOf  s4ee:Value ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  xsd:int ;
                           owl:onProperty     <https://w3id.org/saref#hasValue>
                         ] .

xsd:duration  a  rdfs:Datatype .

s4ee:DefaultDuration  a  owl:Class ;
        rdfs:comment     "The duration of a slot (SHALL be present in case of 'determined slot'). If a slot has a configurable lenght, this element SHALL reflect the currently configured lenght"^^xsd:string ;
        rdfs:label       "Default duration"^^xsd:string ;
        rdfs:subClassOf  <http://www.w3.org/2006/time#TemporalEntity> .

s4ee:MinValue  a         owl:Class ;
        rdfs:comment     "A possible value for a measurement or a parameter"^^xsd:string ;
        rdfs:label       "Min value"^^xsd:string ;
        rdfs:subClassOf  s4ee:Value .

s4ee:up  a            s4ee:SensorState , s4ee:ActuatorLevelFctType , owl:NamedIndividual ;
        rdfs:comment  "Function type or sensor state with value UP"^^xsd:string ;
        rdfs:label    "up"^^xsd:string .

s4ee:paused  a        s4ee:SensorState , s4ee:PowerSequenceState , owl:NamedIndividual ;
        rdfs:comment  "Power sequence state or sensor state with value 'paused'"^^xsd:string ;
        rdfs:label    "paused"^^xsd:string .

s4ee:deviceCode  a    owl:DatatypeProperty ;
        rdfs:comment  "Provides a device code for the device as defined by the manufacturer."^^xsd:string ;
        rdfs:domain   s4ee:Device ;
        rdfs:label    "device code"^^xsd:string ;
        rdfs:range    xsd:string .

s4ee:day  a           s4ee:SensorState , owl:NamedIndividual ;
        rdfs:comment  "Sensor state with value 'day'"^^xsd:string ;
        rdfs:label    "day"^^xsd:string .

s4ee:alternativesCount
        a             owl:DatatypeProperty ;
        rdfs:comment  "Indicates the number of alternatives provided by a power profile"^^xsd:string ;
        rdfs:label    "alternatives count"^^xsd:string ;
        rdfs:range    xsd:integer .

s4ee:ActuatorLevel  a    owl:Class ;
        rdfs:comment     "Independent from the ActuatorSwitch class, the ActuatorLevel class enables a user or application to model LEVEL commands (start, up, percentageAbsolute, relative, etc.). This can be used to dim a light, set the speed of an electric motor, etc."^^xsd:string ;
        rdfs:label       "Actuator level"^^xsd:string ;
        rdfs:subClassOf  owl:Thing ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:ActuatorLevelFctType ;
                           owl:onProperty               s4ee:hasFunction
                         ] .

s4ee:start  a         s4ee:ActuatorLevelFctType , owl:NamedIndividual ;
        rdfs:comment  "Function type with value START"^^xsd:string ;
        rdfs:label    "start"^^xsd:string .

s4ee:levelDown  a     s4ee:SensorState , owl:NamedIndividual ;
        rdfs:comment  "Sensor state with value 'level down'"^^xsd:string ;
        rdfs:label    "level down"^^xsd:string .

s4ee:SensorState  a          owl:Class ;
        rdfs:comment         "The possible states of a sensor"^^xsd:string ;
        rdfs:label           "Sensor State"^^xsd:string ;
        rdfs:subClassOf      s4ee:State ;
        owl:equivalentClass  [ a          owl:Class ;
                               owl:oneOf  ( s4ee:alarmed s4ee:backwards s4ee:changed s4ee:closed s4ee:closing s4ee:day s4ee:detected s4ee:down s4ee:forward s4ee:high s4ee:level s4ee:levelAbsolute s4ee:levelDown s4ee:levelPercentageAbsolute s4ee:levelPercentageRelative s4ee:levelRelative s4ee:levelStart s4ee:levelStop s4ee:levelUp s4ee:longPressed s4ee:low s4ee:middle s4ee:night s4ee:notAlarmed s4ee:notDetected s4ee:off s4ee:on s4ee:open s4ee:opening s4ee:paused s4ee:pressed s4ee:released s4ee:started s4ee:stopped s4ee:toggle s4ee:up )
                             ] .

s4ee:failed  a        s4ee:DeviceConnectionState , owl:NamedIndividual ;
        rdfs:comment  "Device connection state with value 'failed'"^^xsd:string ;
        rdfs:label    "failed"^^xsd:string .

s4ee:MessagingObsolete
        a                owl:Class ;
        rdfs:comment     "A type of messaging"^^xsd:string ;
        rdfs:label       "Messaging obsolete"^^xsd:string ;
        rdfs:subClassOf  s4ee:Messaging .

s4ee:TimePeriod  a       owl:Class ;
        rdfs:comment     "In the demand response use case, the time period associated with a demand response event"^^xsd:string ;
        rdfs:label       "Time period"^^xsd:string ;
        rdfs:subClassOf  <http://www.w3.org/2006/time#TemporalEntity> .

s4ee:running  a       s4ee:PowerSequenceState , owl:NamedIndividual ;
        rdfs:comment  "Power sequence state with value 'running'"^^xsd:string ;
        rdfs:label    "running"^^xsd:string .

s4ee:PowerSkewness  a    owl:Class ;
        rdfs:comment     "A possible type of energy in a slot"^^xsd:string ;
        rdfs:label       "Power skewness"^^xsd:string ;
        rdfs:subClassOf  s4ee:Power .

