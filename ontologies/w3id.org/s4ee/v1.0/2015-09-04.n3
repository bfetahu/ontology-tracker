@prefix :      <https://w3id.org/saref4ee#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix s4ee:  <https://w3id.org/saref4ee#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

s4ee:nodeRemoteControllable
        a             owl:DatatypeProperty ;
        rdfs:comment  "Indicates whether the power profile of a device is configured for remote control (e.g., by an energy management system).This refers to the selection chosen by the user on the remote control feature of the device. If nodeRemoteControllable is equal to FALSE, all the supported power sequences shall set the sequenceRemoteControllable property to FALSE. If nodeRemoteControllable is equal to TRUE, at least one power sequence shall be remotely controllable by setting the sequenceRemoteControllable property to TRUE."^^xsd:string ;
        rdfs:label    "node remote controllable"^^xsd:string ;
        rdfs:range    xsd:boolean .

s4ee:alarmed  a       s4ee:SensorState , owl:NamedIndividual ;
        rdfs:comment  "Sensor state with value 'alarmed'"^^xsd:string ;
        rdfs:label    "alarmed"^^xsd:string .

s4ee:Appliance  a        owl:Class ;
        rdfs:comment     "Any kind of smart home device of interest for the monitoring and control use case, and the demand response use case "^^xsd:string ;
        rdfs:label       "Appliance"^^xsd:string ;
        rdfs:subClassOf  s4ee:Device , owl:Thing ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:ApplianceMonitor ;
                           owl:onProperty               s4ee:hasListOfMonitor
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:DemandResponseEventData ;
                           owl:onProperty               s4ee:receives
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:ApplianceParameter ;
                           owl:onProperty               s4ee:hasListOfParameter
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:ApplianceControl ;
                           owl:onProperty               s4ee:hasListOfControl
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:ApplianceParameterCompatibilityAction ;
                           owl:onProperty               s4ee:hasListOfAction
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:ApplianceControl ;
                           owl:onProperty     s4ee:hasListOfControl
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:ApplianceParameterCompatibilityAction ;
                           owl:onProperty     s4ee:hasListOfAction
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:DemandResponseEventData ;
                           owl:onProperty     s4ee:receives
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:ApplianceParameter ;
                           owl:onProperty     s4ee:hasListOfParameter
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:ApplianceWorkingMode ;
                           owl:onProperty               s4ee:hasListOfMode
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:ApplianceMonitor ;
                           owl:onProperty     s4ee:hasListOfMonitor
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:ApplianceWorkingMode ;
                           owl:onProperty     s4ee:hasListOfMode
                         ] .

s4ee:hardwareRevision
        a             owl:DatatypeProperty ;
        rdfs:comment  "Indicates the hardware revision of the device as defined by the manufacturer."^^xsd:string ;
        rdfs:domain   s4ee:Device ;
        rdfs:label    "hardware revision "^^xsd:string ;
        rdfs:range    xsd:string .

s4ee:changed  a       s4ee:SensorState , owl:NamedIndividual ;
        rdfs:comment  "Sensor state with value 'changed'"^^xsd:string ;
        rdfs:label    "changed"^^xsd:string .

s4ee:modified  a      s4ee:DeviceConnectionState , owl:NamedIndividual ;
        rdfs:comment  "Device connection state with value 'modified'"^^xsd:string ;
        rdfs:label    "modified"^^xsd:string .

s4ee:PowerSequenceState
        a                    owl:Class ;
        rdfs:comment         "The current state of the power sequence. It can assume one of the values 'running', 'paused', 'scheduled', 'scheduled paused', 'pending', 'inactive', 'completed',  or 'invalid'."^^xsd:string ;
        rdfs:label           "Power sequence state"^^xsd:string ;
        rdfs:subClassOf      s4ee:State ;
        owl:equivalentClass  [ a          owl:Class ;
                               owl:oneOf  ()
                             
                             ] .

s4ee:parameterId  a   owl:DatatypeProperty ;
        rdfs:comment  "Used to identify a parameter"^^xsd:string ;
        rdfs:label    "parameter id"^^xsd:string ;
        rdfs:range    xsd:unsignedInt .

s4ee:Timestamp  a        owl:Class ;
        rdfs:comment     "Used in the following classes as alternative to the datatype property \"s4ee:timestamp\" in case one wants to describe the timestamp using duration (instead of xsd:date time):\n- In the Sensing class is the creation time of the sensingData instance.\n- In the Messaging class is the point in time in which the message was generated.\n- In the Measurement class is the time of creation of a value element (measured, calculated, ...). Some sensors and communications technologies may already provide this information natively. In other cases the technology interface may set timestamp to the time of creation of a measurementData instance.\n- In the DemandResponseEvent and s4ee:DemandResponseStateData of teh demand response use case"^^xsd:string ;
        rdfs:label       "Timestamp"^^xsd:string ;
        rdfs:subClassOf  <http://www.w3.org/2006/time#TemporalEntity> .

s4ee:resume  a        s4ee:DemandResponseEventAction , owl:NamedIndividual ;
        rdfs:comment  "Demand response event action type with value resume"^^xsd:string ;
        rdfs:label    "resume"^^xsd:string .

s4ee:absolute  a      s4ee:ActuatorLevelFctType , owl:NamedIndividual ;
        rdfs:comment  "Function type with value ABSOLUTE"^^xsd:string ;
        rdfs:label    "absolute"^^xsd:string .

s4ee:pause  a         s4ee:DemandResponseEventAction , owl:NamedIndividual ;
        rdfs:comment  "Demand response event action type with value pause"^^xsd:string ;
        rdfs:label    "pause"^^xsd:string .

s4ee:measurand_state_outOfRange
        a             s4ee:MeasurandState , owl:NamedIndividual ;
        rdfs:comment  "Measurand state with value OUT OF RANGE"^^xsd:string ;
        rdfs:label    "measurand state out of range"^^xsd:string .

s4ee:EnergyStandardDeviation
        a                owl:Class ;
        rdfs:comment     "A possible type of energy in a slot"^^xsd:string ;
        rdfs:label       "Energy Standard Deviation"^^xsd:string ;
        rdfs:subClassOf  s4ee:Energy .

s4ee:succeeded  a     s4ee:DeviceConnectionState , owl:NamedIndividual ;
        rdfs:comment  "Device connection state with value 'succeeded'"^^xsd:string ;
        rdfs:label    "succeeded"^^xsd:string .

s4ee:supportsSingleSlotSchedulingOnly
        a             owl:DatatypeProperty ;
        rdfs:comment  "If set toTRUE, the power profile of a device does NOT permit the modification of more than one slot per configuration command."^^xsd:string ;
        rdfs:label    "supports single slot scheduling only"^^xsd:string ;
        rdfs:range    xsd:boolean .

s4ee:eventCancelled  a  s4ee:DemandResponseState , owl:NamedIndividual ;
        rdfs:comment  "Demand response state with value 'event cancelled'"^^xsd:string ;
        rdfs:label    "event cancelled"^^xsd:string .

s4ee:levelStart  a    s4ee:SensorState , owl:NamedIndividual ;
        rdfs:comment  "Sensor state with value 'level start'"^^xsd:string ;
        rdfs:label    "level start"^^xsd:string .

s4ee:DeviceConnectionState
        a                    owl:Class ;
        rdfs:comment         "The possible states in the Remote Newtork Management use case, when adding a new device, removing an existing device, scanning the network for candidate devices, or modifying parametrs of an existing device. "^^xsd:string ;
        rdfs:label           "Device connection state"^^xsd:string ;
        rdfs:subClassOf      s4ee:State ;
        owl:equivalentClass  [ a          owl:Class ;
                               owl:oneOf  ()
                             
                             ] .

s4ee:step  a          owl:DatatypeProperty ;
        rdfs:comment  "Indicates the step in a step paramater table"^^xsd:string ;
        rdfs:label    "step"^^xsd:string ;
        rdfs:range    xsd:int .

s4ee:Address  a          owl:Class ;
        rdfs:comment     "An entity used in the Remote Network Management (RemoteNWM) use case to represent the node address. SHALL be present when adding, removing or modifying a device in the network."^^xsd:string ;
        rdfs:label       "Address"^^xsd:string ;
        rdfs:subClassOf  owl:Thing .

s4ee:MaxDuration  a      owl:Class ;
        rdfs:comment     "If a slot has a configurable duration, it SHALL be present and denote the maximum supported configuration. Note: This element applies to the first repetition of the slot number only"^^xsd:string ;
        rdfs:label       "Max duration"^^xsd:string ;
        rdfs:subClassOf  <http://www.w3.org/2006/time#TemporalEntity> .

s4ee:belongsTo  a     owl:ObjectProperty ;
        rdfs:comment  "A relationship identifying the ownership of an entity. In particular, a slot belongs to a power sequence, a power sequence belongs to an alternative, an alternative  belongs to a power profile, a power profile belongs to a device.  "^^xsd:string ;
        rdfs:label    "belongs to"^^xsd:string .

s4ee:StartTimeDurationDescription
        a                owl:Class ;
        rdfs:comment     "The duration description for the start time duration temporal entities"^^xsd:string ;
        rdfs:label       "Start time duration description"^^xsd:string ;
        rdfs:subClassOf  <http://www.w3.org/2006/time#DurationDescription> .

s4ee:hasTable  a      owl:ObjectProperty ;
        rdfs:comment  "A relationship between an appliance parameter and an associated table"^^xsd:string ;
        rdfs:domain   s4ee:ApplianceParameter ;
        rdfs:label    "has table"^^xsd:string ;
        rdfs:range    s4ee:ApplianceParameterTable .

s4ee:valueSource  a   owl:DatatypeProperty ;
        rdfs:comment  "Indicates the source (origin/foundation) of the measurement forecasted values for a power sequence. If absent, the source is undefined. Remark: This element shall express the reliability of the forecast."^^xsd:string ;
        rdfs:label    "value source"^^xsd:string ;
        rdfs:range    [ a          rdfs:Datatype ;
                        owl:oneOf  [ a          rdf:List ;
                                     rdf:first  "calculatedValue"^^xsd:string ;
                                     rdf:rest   [ a          rdf:List ;
                                                  rdf:first  "empiricalValue"^^xsd:string ;
                                                  rdf:rest   [ a          rdf:List ;
                                                               rdf:first  "measuredValue"^^xsd:string ;
                                                               rdf:rest   ()
                                                             
                                                             ]
                                                ]
                                   ]
                      ] .

s4ee:supportsReselection
        a             owl:DatatypeProperty ;
        rdfs:comment  "If set toTRUE, the power profile of a device does not restrict the number of power sequence re-selections by the CEM, i.e. within a given alternative the CEM may first choose one power sequence, alter the selection by configuring another sequence later on, then alter the selection again, etc. (provided the process rules and data still permit configuration). If supportsReselection is set to FALSE, the device permits the CEM to select a power sequence of an alternative only once."^^xsd:string ;
        rdfs:label    "supports reselection"^^xsd:string ;
        rdfs:range    xsd:boolean .

s4ee:utc  a           owl:DatatypeProperty ;
        rdfs:comment  "Indicates the current time in UTC."^^xsd:string ;
        rdfs:label    "utc"^^xsd:string ;
        rdfs:range    xsd:dateTime .

s4ee:DemandResponseState
        a                    owl:Class ;
        rdfs:comment         "In the demand response use case, it expresses the possible states of a demand response event"^^xsd:string ;
        rdfs:label           "Demand response state "^^xsd:string ;
        rdfs:subClassOf      s4ee:State ;
        owl:equivalentClass  [ a          owl:Class ;
                               owl:oneOf  ()
                             
                             ] .

s4ee:hasAppliance  a  owl:ObjectProperty ;
        rdfs:comment  "A relationship between a demand response event and an appliance."^^xsd:string ;
        rdfs:label    "has appliance"^^xsd:string .

s4ee:percentageAbsolute
        a             s4ee:ActuatorLevelFctType , owl:NamedIndividual ;
        rdfs:comment  "Function type with value PRECENTAGE ABSOLUTE"^^xsd:string ;
        rdfs:label    "percentage absolute"^^xsd:string .

s4ee:totalSequencesCountMax
        a             owl:DatatypeProperty ;
        rdfs:comment  "Represents the total number of power sequences supported by the power profile of a device"^^xsd:string ;
        rdfs:label    "total sequences count max"^^xsd:string ;
        rdfs:range    xsd:unsignedInt .

s4ee:Energy  a           owl:Class ;
        rdfs:comment     "Energy type and value in a slot (i.e., Energy, EnergyMin, EnergyMax, EnergyExpected, EnergyStandardDeviation, EnergySkewness); or the ResumeEnergyEstimated in a power sequence, i.e., the additional energy the device will consume before resuming its normal operation (after a pause)"^^xsd:string ;
        rdfs:label       "Energy"^^xsd:string ;
        rdfs:subClassOf  <https://w3id.org/saref#Energy> .

s4ee:AverageDerivativeValue
        a                owl:Class ;
        rdfs:comment     "A possible value for a measurement"^^xsd:string ;
        rdfs:label       "Average derivative value"^^xsd:string ;
        rdfs:subClassOf  s4ee:Value .

s4ee:manufacturerNodeIdentification
        a             owl:DatatypeProperty ;
        rdfs:comment  "Indicates a node identification for the device as defined by the manufacturer. This could be used for the identification of a device, even if it was removed from the network and rejoined later with changed node address."^^xsd:string ;
        rdfs:domain   s4ee:Device ;
        rdfs:label    "manufacturer node identification"^^xsd:string ;
        rdfs:range    xsd:string .

s4ee:ApplianceParameterState
        a                owl:Class ;
        rdfs:comment     "The actual state of a parameter (by means of the ApplianceMonitor class), or the new state to be set for a parameter (by means of the ApplianceControl class)"^^xsd:string ;
        rdfs:label       "Appliance parameter state"^^xsd:string ;
        rdfs:subClassOf  s4ee:State ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "0"^^xsd:nonNegativeInteger ;
                           owl:onDataRange              xsd:int ;
                           owl:onProperty               <https://w3id.org/saref#hasValue>
                         ] .

s4ee:StandardDeviationValue
        a                owl:Class ;
        rdfs:comment     "A possible value for a measurement"^^xsd:string ;
        rdfs:label       "Standard deviation value"^^xsd:string ;
        rdfs:subClassOf  s4ee:Value .

s4ee:open  a          s4ee:SensorState , owl:NamedIndividual ;
        rdfs:comment  "Sensor state with value 'open'"^^xsd:string ;
        rdfs:label    "open"^^xsd:string .

s4ee:ResumeCostEstimated
        a                owl:Class ;
        rdfs:comment     "In a power sequence the additional costs for the resumption of a device to its normal operation (after a pause)."^^xsd:string ;
        rdfs:label       "Resume cost estimated"^^xsd:string ;
        rdfs:subClassOf  <https://w3id.org/saref#Price> .

s4ee:setUpUsableForAdd
        a             owl:DatatypeProperty ;
        rdfs:comment  "Used in the remote Network Management use case for the report candidate, in which case SHALL be present. If this value is set to TRUE, then CandidateSetup can be used for NativeSetup of networkManagementAddNodeCall. "^^xsd:string ;
        rdfs:label    "set up usable for add"^^xsd:string ;
        rdfs:range    xsd:boolean .

s4ee:greenest  a      owl:DatatypeProperty ;
        rdfs:comment  "Indicates whether a power sequence optimises the configuration towards the maximum availability of renewable energy (if set to TRUE). MAY be present. Absence of this element is equal to the presence with value FALSE. "^^xsd:string ;
        rdfs:label    "greenest"^^xsd:string ;
        rdfs:range    xsd:boolean .

s4ee:DeviceConnectionSetup
        a                owl:Class ;
        rdfs:comment     "Before the connection between two devices is established, there is an initialization that may consists of zero or more setup phases of type Candidate Setup, Scan Setup, and JoinModeConfiguration."^^xsd:string ;
        rdfs:label       "Device connection setup"^^xsd:string ;
        rdfs:subClassOf  owl:Thing .

s4ee:eventError  a    s4ee:DemandResponseState , owl:NamedIndividual ;
        rdfs:comment  "Demand response state with value 'event error'"^^xsd:string ;
        rdfs:label    "event error"^^xsd:string .

s4ee:cheapest  a      owl:DatatypeProperty ;
        rdfs:comment  "Indicates whether a power sequence applies a configuration that minimises the user’s energy bill (if set to TRUE). MAY be present. Absence of this element is equal to the presence with value FALSE. "^^xsd:string ;
        rdfs:label    "cheapest"^^xsd:string ;
        rdfs:range    xsd:boolean .

s4ee:manufacturerLabel
        a                   owl:DatatypeProperty ;
        rdfs:comment        "A short label of the device as defined by the manufacturer. Used also in the Remote Network Management use case where it MAY be present: manufacturers may assign their devices a brief label, this makes interfacing, e.g. to a smartphone application, a lot easier. However, the content of the “label” section is not standardized. "^^xsd:string ;
        rdfs:domain         s4ee:Device ;
        rdfs:label          "manufacturer label"^^xsd:string ;
        rdfs:range          xsd:string ;
        rdfs:subPropertyOf  <https://w3id.org/saref#hasManufacturer> .

s4ee:eventRejected  a  s4ee:DemandResponseState , owl:NamedIndividual ;
        rdfs:comment  "Demand response state with value 'event rejected'"^^xsd:string ;
        rdfs:label    "event rejected"^^xsd:string .

s4ee:MinDuration  a      owl:Class ;
        rdfs:comment     "If a slot has a configurable duration, it SHALL be present and denote the minimum supported configuration. Note: This element applies to the first repetition of the slot number only."^^xsd:string ;
        rdfs:label       "Min duration"^^xsd:string ;
        rdfs:subClassOf  <http://www.w3.org/2006/time#TemporalEntity> .

s4ee:hasListOfMonitor
        a                   owl:ObjectProperty ;
        rdfs:comment        "A relationship representing a list of monitors"^^xsd:string ;
        rdfs:label          "has list of monitor"^^xsd:string ;
        rdfs:subPropertyOf  s4ee:hasListOf .

s4ee:messagingNumber  a  owl:DatatypeProperty ;
        rdfs:comment  "An identifier for one specific message. If a message is marked as obsolete, this number can be used to identify the original message."^^xsd:string ;
        rdfs:domain   s4ee:Messaging ;
        rdfs:label    "messaging number"^^xsd:string ;
        rdfs:range    xsd:unsignedInt .

s4ee:MessagingLogging
        a                owl:Class ;
        rdfs:comment     "A type of messaging"^^xsd:string ;
        rdfs:label       " Messaging Logging"^^xsd:string ;
        rdfs:subClassOf  s4ee:Messaging .

s4ee:State  a            owl:Class ;
        rdfs:comment     "An entity that represents the possible states in SAREF4EE"^^xsd:string ;
        rdfs:label       "State"^^xsd:string ;
        rdfs:subClassOf  owl:Thing .

s4ee:Slot  a             owl:Class ;
        rdfs:comment     "The single steps of a power sequence are represented by slots. A slot is associated with a slot number (while a power sequence is associated with a power sequence identifier). The slot numbers of two power sequences should be considered independent from each other, i.e., slot number 7 of sequence 1 describes a different slot than slot number 7 of sequence 2. Therefore a slot is only uniquely identified in combination with a sequence ID. "^^xsd:string ;
        rdfs:label       "Slot"^^xsd:string ;
        rdfs:subClassOf  <https://w3id.org/saref#Profile> , owl:Thing ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:LatestEndTime ;
                           owl:onProperty               <https://w3id.org/saref#hasTime>
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:RemainingPauseTime ;
                           owl:onProperty               <https://w3id.org/saref#hasTime>
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:DefaultDuration ;
                           owl:onProperty               <https://w3id.org/saref#hasTime>
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:Energy ;
                           owl:onProperty     s4ee:hasEnergyValueType
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      s4ee:slotActivated
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:EarliestStartTime ;
                           owl:onProperty               <https://w3id.org/saref#hasTime>
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:MaxDuration ;
                           owl:onProperty               <https://w3id.org/saref#hasTime>
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:minQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  [ a            owl:Class ;
                                                          owl:unionOf  ( s4ee:Energy s4ee:Power )
                                                        ] ;
                           owl:onProperty               s4ee:hasValueType
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  <https://w3id.org/saref#Time> ;
                           owl:onProperty     <https://w3id.org/saref#hasTime>
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:StartTime ;
                           owl:onProperty               <https://w3id.org/saref#hasTime>
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   s4ee:sequenceId
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:Duration ;
                           owl:onProperty               <https://w3id.org/saref#hasTime>
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      <https://w3id.org/saref#hasDescription>
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:MinDuration ;
                           owl:onProperty               <https://w3id.org/saref#hasTime>
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:EndTime ;
                           owl:onProperty               <https://w3id.org/saref#hasTime>
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:Power ;
                           owl:onProperty     s4ee:hasPowerValueType
                         ] ;
        rdfs:subClassOf  [ a                  owl:Restriction ;
                           owl:allValuesFrom  s4ee:PowerSequence ;
                           owl:onProperty     s4ee:belongsTo
                         ] ;
        rdfs:subClassOf  [ a                         owl:Restriction ;
                           owl:onClass               s4ee:PowerSequence ;
                           owl:onProperty            s4ee:belongsTo ;
                           owl:qualifiedCardinality  "1"^^xsd:nonNegativeInteger
                         ] ;
        rdfs:subClassOf  [ a                            owl:Restriction ;
                           owl:maxQualifiedCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onClass                  s4ee:DurationUncertainty ;
                           owl:onProperty               <https://w3id.org/saref#hasTime>
                         ] ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:maxCardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty      s4ee:optionalSlot
                         ] ;
        rdfs:subClassOf  [ a                owl:Restriction ;
                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                           owl:onProperty   s4ee:slotNumber
                         ] .

s4ee:mathOperator  a  owl:DatatypeProperty ;
        rdfs:comment  "Represents a math operator in an expression and can assume the following values:  == (equal), != (different), > (greater than), < (smaller than)"^^xsd:string ;
        rdfs:label    "math operator"^^xsd:string ;
        rdfs:range    [ a          rdfs:Datatype ;
                        owl:oneOf  [ a          rdf:List ;
                                     rdf:first  "!="^^xsd:string ;
                                     rdf:rest   [ a          rdf:List ;
                                                  rdf:first  "<"^^xsd:string ;
                                                  rdf:rest   [ a          rdf:List ;
                                                               rdf:first  "=="^^xsd:string ;
                                                               rdf:rest   [ a          rdf:List ;
                                                                            rdf:first  ">"^^xsd:string ;
                                                                            rdf:rest   ()
                                                                          
                                                                          ]
                                                             ]
                                                ]
                                   ]
