lexinfo:Numeral  a           owl:Class ;
        rdfs:isDefinedBy     <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf      lemon:Word ;
        owl:equivalentClass  [ a                   owl:Restriction ;
                               owl:onProperty      lexinfo:partOfSpeech ;
                               owl:someValuesFrom  lexinfo:NumeralPOS
                             ] .

lexinfo:numeral  a        owl:NamedIndividual , lexinfo:PartOfSpeech , lexinfo:NumeralPOS , owl:Thing ;
        rdfs:comment      "Part of speech that expresses a number or the relation to a number." ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "numeral" ;
        dc:creator        "Francopoulo, Gil" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-1334> ;
        owl:versionInfo   "1:0" .

lexinfo:ImpersonalIntransitiveFrame
        a                    owl:Class ;
        rdfs:isDefinedBy     <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf      lexinfo:ImpersonalFrame ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( lexinfo:ImpersonalFrame [ a                owl:Restriction ;
                                                                               owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                               owl:onProperty   lexinfo:object
                                                                             ] )
                             ] .

lexinfo:DativeTransitiveFrame
        a                    owl:Class ;
        rdfs:isDefinedBy     <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf      lexinfo:VerbFrame ;
        lexinfo:example      "ich DANKE dir"@de ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( lexinfo:VerbFrame [ a                owl:Restriction ;
                                                                         owl:cardinality  "0"^^xsd:nonNegativeInteger ;
                                                                         owl:onProperty   lexinfo:directObject
                                                                       ] [ a                owl:Restriction ;
                                                                           owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                                                                           owl:onProperty   lexinfo:indirectObject
                                                                         ] [ a                owl:Restriction ;
                                                                             owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                                                                             owl:onProperty   lexinfo:subject
                                                                           ] )
                             ] .

