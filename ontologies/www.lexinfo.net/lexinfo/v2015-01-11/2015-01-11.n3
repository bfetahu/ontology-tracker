lexinfo:SuperlativeAdjunct
        a                 owl:Class ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf   lexinfo:Adjunct .

lexinfo:absolutiveCase
        a                 owl:NamedIndividual , lexinfo:Case , owl:Thing ;
        rdfs:comment      "Case for nouns in ergative-absolute languages that would generally be the subjects of intransitive verbs or the objects of transitive verbs in the translational equivalents of nominative-accusative languages such as English." ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "absolutive case" ;
        dc:creator        "Francopoulo, Gil" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-1225> ;
        owl:versionInfo   "1:0" .

lexinfo:TransitiveInterrogativeFrame
        a                    owl:Class ;
        rdfs:isDefinedBy     <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf      lexinfo:InterrogativeFrame ;
        lexinfo:example      "he ASKED her [what he should do]"@en ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( lexinfo:InterrogativeFrame lexinfo:TransitiveFrame )
                             ] .

lexinfo:conjunction  a    owl:NamedIndividual , lexinfo:PartOfSpeech , lexinfo:ConjunctionPOS , owl:Thing ;
        rdfs:comment      "Word that syntactically links words or constituents, and expresses a semantic relationship between them." ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "conjunction" ;
        dc:creator        "Francopoulo, Gil" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-1260> ;
        owl:versionInfo   "1:0" .

lexinfo:ReflexiveDitransitiveFrame
        a                    owl:Class ;
        rdfs:isDefinedBy     <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf      lexinfo:ReflexiveFrame ;
        lexinfo:example      "das habe ich mir EINGEBILDET"@de ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( lexinfo:DitransitiveFrame lexinfo:ReflexiveFrame [ a                   owl:Restriction ;
                                                                                                        owl:onProperty      lexinfo:directObject ;
                                                                                                        owl:someValuesFrom  lexinfo:ReflexiveObject
                                                                                                      ] )
                             ] .

lexinfo:object  a           owl:ObjectProperty ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:range          lexinfo:Object ;
        rdfs:subPropertyOf  lemon:synArg .

lexinfo:Control  a        owl:Class ;
        rdfs:comment      "Control indicates how a an argument from a main clause will be utilized in a subclause. This class includes both control structures and raising structures"@en ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf   lemon:Frame .

lexinfo:nominativeCaseForm
        a                   owl:ObjectProperty ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subPropertyOf  lexinfo:formCaseVariant .

lexinfo:predicativeAdjective
        a                   owl:ObjectProperty ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:range          lexinfo:PredicativeAdjective ;
        rdfs:subPropertyOf  lexinfo:complement .

lexinfo:firstPersonForm
        a                   owl:ObjectProperty ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subPropertyOf  lexinfo:formPersonVariant .

lexinfo:futureTenseForm
        a                   owl:ObjectProperty ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subPropertyOf  lexinfo:formTenseVariant .

lexinfo:dativeCaseForm
        a                   owl:ObjectProperty ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subPropertyOf  lexinfo:formCaseVariant .

lexinfo:collective  a     owl:NamedIndividual , lexinfo:Number , owl:Thing ;
        rdfs:comment      "A collective number is a number referring to 'a set of things'. Languages that have this feature can use it to get a phrase like 'flock of sheeps' by using 'sheep' in collective number." ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "collective" ;
        dc:creator        "Francopoulo, Gil" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-1254> ;
        owl:versionInfo   "1:0" .

lexinfo:PossessiveAdjunct
        a                 owl:Class ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf   lexinfo:Adjunct .

lexinfo:quadrial  a       owl:NamedIndividual , lexinfo:Number , owl:Thing ;
        rdfs:comment      "Property related to four elements." ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "quadrial" ;
        dc:creator        "Francopoulo, Gil" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-2000> ;
        owl:versionInfo   "1:0" .

lexinfo:delativeCase  a   owl:NamedIndividual , lexinfo:Case , owl:Thing ;
        rdfs:comment      "Case which expresses motion downward from the referent of the noun it marks." ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "delative case" ;
        dc:creator        "Francopoulo, Gil" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-1268> ;
        owl:versionInfo   "1:0" .

lexinfo:appellation  a    owl:NamedIndividual , lexinfo:TermType , owl:Thing ;
        rdfs:comment      "A designation that represents an individual concept." ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "appellation" ;
        dc:creator        "Wright, Sue Ellen" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-347> ;
        owl:versionInfo   "1:0" .

lexinfo:DeclarativeFrame
        a                    owl:Class ;
        rdfs:comment         "Declarative frames take a clause as an argument, this argument is marker with the declarative conjunction (\"that\" in English)"@en ;
        rdfs:isDefinedBy     <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf      lexinfo:VerbFrame ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( lexinfo:VerbFrame [ a                   owl:Restriction ;
                                                                         owl:minCardinality  "1"^^xsd:nonNegativeInteger ;
                                                                         owl:onProperty      lexinfo:declarativeClause
                                                                       ] )
                             ] .

lemon:Phrase  a  owl:Class .

lexinfo:productName  a    owl:NamedIndividual , lexinfo:TermType , owl:Thing ;
        rdfs:comment      "An attribute assigned to a term indicating that that term is the official designator for a product." ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "productName" ;
        dc:creator        "Wright, Sue Ellen" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-346> ;
        owl:versionInfo   "1:0" .

lexinfo:exact  a            owl:ObjectProperty ;
        rdfs:comment        "Completely correct in every detail" ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label          "exact" ;
        rdfs:subPropertyOf  lemon:equivalent ;
        dc:creator          "Francopoulo, Gil" ;
        dcr:datcat          <http://www.isocat.org/datcat/DC-2205> ;
        owl:versionInfo     "1:0" .

lexinfo:hyponym  a          owl:ObjectProperty ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subPropertyOf  lemon:narrower .

lexinfo:SubjectlessTransitiveFrame
        a                    owl:Class ;
        rdfs:isDefinedBy     <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf      lexinfo:SubjectlessFrame ;
        lexinfo:example      "mich FRIERT"@de ;
        owl:equivalentClass  [ a                   owl:Class ;
                               owl:intersectionOf  ( lexinfo:SubjectlessFrame [ a                owl:Restriction ;
                                                                                owl:cardinality  "1"^^xsd:nonNegativeInteger ;
                                                                                owl:onProperty   lexinfo:directObject
                                                                              ] )
                             ] .

lexinfo:RaisingSubject
        a                    owl:Class ;
        rdfs:comment         "Indicates the syntactic subject of the main clause is in fact the subject of the subclause. The main clause should then be interpreted as being impersonal."@en ;
        rdfs:isDefinedBy     <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf      lexinfo:Control ;
        owl:equivalentClass  [ a                   owl:Restriction ;
                               owl:onProperty      lexinfo:subject ;
                               owl:someValuesFrom  lexinfo:RaisableSubject
                             ] .

lexinfo:Symbol  a            owl:Class ;
        rdfs:isDefinedBy     <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:subClassOf      lemon:Word ;
        owl:equivalentClass  [ a                   owl:Restriction ;
                               owl:onProperty      lexinfo:partOfSpeech ;
                               owl:someValuesFrom  lexinfo:SymbolPOS
                             ] .

lexinfo:pronominalAdverb
        a                 lexinfo:AdverbPOS , lexinfo:PartOfSpeech , owl:NamedIndividual , owl:Thing ;
        rdfs:comment      "adverb formed in replacement of a preposition and a pronoun" ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "pronominal adverb" ;
        dc:creator        "Francopoulo, Gil" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-2998> ;
        owl:versionInfo   "1:0" .

lexinfo:verbFormMood  a     owl:ObjectProperty ;
        rdfs:comment        "One of a set of distinctive forms that are used to signal modality. Modality is a facet of illocutionary point or general intent of a speaker, or a speaker's degree of commitment to the expressed proposition's believability, obligatoriness, desirability or reality." ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label          "verb form mood" ;
        rdfs:range          lexinfo:VerbFormMood ;
        rdfs:subPropertyOf  lexinfo:morphosyntacticProperty ;
        dc:creator          "Francopoulo, Gil" ;
        dcr:datcat          <http://www.isocat.org/datcat/DC-1427> ;
        owl:versionInfo     "1:0" .

lexinfo:adpositionalObject
        a                   owl:ObjectProperty ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:range          lexinfo:AdpositionalObject ;
        rdfs:subPropertyOf  lexinfo:object .

lexinfo:exclamativePronoun
        a                 lexinfo:PronounPOS , lexinfo:PartOfSpeech , owl:Thing , owl:NamedIndividual ;
        rdfs:comment      "Pronoun marked with an emotional utterance and/or a strong intonation." ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "exclamative pronoun" ;
        dc:creator        "Francopoulo, Gil" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-1285> ;
        owl:versionInfo   "1:0" .

lexinfo:indirectObject
        a                   owl:ObjectProperty ;
        rdfs:comment        "An indirect object is a grammatical relation that is one means of expressing the semantic role of goal and other similar roles. It is proposed for languages in which the role is distinct from the direct object and the oblique object on the basis of multiple independent syntactic or morphological criteria, such as the following: - Having a particular case marking, commonly dative - Governing an agreement affix on the verb, such as person or number - Being distinct from oblique relations in that it may be relativized" ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label          "indirect object" ;
        rdfs:range          lexinfo:IndirectObject ;
        rdfs:subPropertyOf  lexinfo:object ;
        dcr:datcat          <http://www.isocat.org/datcat/DC-1310> .

lexinfo:radical  a        owl:NamedIndividual , lexinfo:TermElement , owl:Thing ;
        rdfs:comment      "A basic identifiable component of every CJKV character, often found on the left side of the character, that sometimes gives a rough indication of meaning and is used for collating lexical and terminological resources." ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "radical" ;
        dc:creator        "Wright, Sue Ellen" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-296> ;
        owl:versionInfo   "1:0" .

lexinfo:inchoative  a     owl:NamedIndividual , lexinfo:Aspect , owl:Thing ;
        rdfs:comment      "Aspect that expresses the beginning of an event or state." ;
        rdfs:isDefinedBy  <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label        "inchoative" ;
        dc:creator        "Francopoulo, Gil" ;
        dcr:datcat        <http://www.isocat.org/datcat/DC-2002> ;
        owl:versionInfo   "1:0" .

lexinfo:frequency  a        owl:ObjectProperty ;
        rdfs:comment        "The relative commonness with which a term occurs." ;
        rdfs:isDefinedBy    <http://www.lexinfo.net/ontology/2.0/lexinfo> ;
        rdfs:label          "frequency" ;
        rdfs:range          lexinfo:Frequency ;
        rdfs:subPropertyOf  lemon:property ;
        dc:creator          "Wright, Sue Ellen" , "Francopoulo, Gil" ;
        dcr:datcat          <http://www.isocat.org/datcat/DC-1965> , <http://www.isocat.org/datcat/DC-233> ;
        owl:versionInfo     "1:0" .
