<http://rdvocab.info/roles/onScreenPresenterExpression>
        a                 rdf:Property ;
        rdfs:comment      "Use when a more specific term (e.g., Narrator or Host) is either not applicable or not desired."@en ;
        rdfs:domain       <http://rdvocab.info/uri/schema/FRBRentitiesRDA/Expression> ;
