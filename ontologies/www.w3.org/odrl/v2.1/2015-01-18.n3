@prefix :      <http://www.w3.org/ns/odrl/2/> .
@prefix voaf:  <http://purl.org/vocommons/voaf#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix ont:   <http://purl.org/net/ns/ontology-annot#> .
@prefix dct:   <http://purl.org/dc/terms/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix vann:  <http://purl.org/vocab/vann/> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix vs:    <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix dcmit: <http://purl.org/dc/dcmitype/> .
@prefix skos:  <http://www.w3.org/2004/02/skos/core#> .
@prefix odrl:  <http://www.w3.org/ns/odrl/2/> .

odrl:device  a                  owl:ObjectProperty , rdf:Property ;
        rdfs:comment            "An identifiable computing system."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "device"@en ;
        rdfs:subPropertyOf      odrl:rightOperand ;
        ont:deprecatedBy        odrl:systemDevice ;
        owl:equivalentProperty  odrl:system ;
        vs:term_status          "deprecated" .

odrl:unit  a              rdf:Property ;
        rdfs:comment      "The unit of measurement used for the constraint value."@en ;
        rdfs:domain       odrl:Constraint ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "unit"@en ;
        rdfs:range        rdfs:Resource ;
        vs:term_status    "testing" .

odrl:   a                              owl:Ontology , voaf:Vocabulary ;
        rdfs:label                     "ODRL Version 2.1 Ontology"@en ;
        dct:contributor                "ODRL Community Group" ;
        dct:creator                    "Mo McRoberts (BBC)" , "Víctor Rodríguez Doncel (OEG-UPM)" ;
        dct:description                "The Open Digital Rights Language (ODRL) provides flexible and interoperable mechanisms to support transparent and innovative use of digital content in publishing, distribution, and consumption of of digital media across all sectors and communities. The ODRL Policy model is broad enough to support traditional rights expressions for commercial transaction, open access expressions for publicly distributed content, and privacy expressions for social media."@en ;
        dct:issued                     "2015-01-18"^^xsd:date ;
        vann:example                   odrl:Asset ;
        vann:preferredNamespacePrefix  "odrl" ;
        vann:preferredNamespaceUri     <http://w3.org/ns/odrl/2/> ;
        owl:versionInfo                "2.1" .

odrl:isNoneOf  a          owl:NamedIndividual , odrl:Operator ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Is none of"@en ;
        vs:term_status    "testing" .

odrl:compensate  a        skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner requires that the Assignees compensates the Assigner (or other specified compensation Party) by some amount of value, if defined, for use of the Asset. The compensation may use different types of things with a value: (i) the thing is expressed by the value (term) of the Constraint name; (b) the value is expressed by operator, rightOperand, dataType and unit"@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Compensate"@en ;
        vs:term_status    "testing" .

odrl:industry  a            owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "The defined industry sector applicable to the asset usage"@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "industry"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:secondaryUse  a      skos:Concept , odrl:Action ;
        rdfs:comment      "The act of using the asset for a purpose other than the purpose it was intended for."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Secondary use"@en ;
        vs:term_status    "testing" .

odrl:modify  a                  skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to update existing content of the Asset. A new asset is not created by this action. This action will modify an asset which is typically updated from time to time without creating a new asset like a database. If the result from modifying the asset should be a new asset the actions derive or extract should be used. (Note that the notion of whether or not the change is significant enough to qualify as a new asset is subjective)."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Modify"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:display  a                 skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to display the visual media Asset to an audience or the public. For example, displaying an image on a screen."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Display"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:present .

odrl:systemDevice  a            owl:ObjectProperty , rdf:Property ;
        rdfs:comment            "An identifiable computing system."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "SystemDevice"@en ;
        rdfs:subPropertyOf      odrl:rightOperand ;
        owl:equivalentProperty  odrl:device , odrl:system ;
        vs:term_status          "testing" .

odrl:AllGroups  a         owl:Class , rdfs:Class ;
        rdfs:comment      "Specifies that the scope of the relationship is all of the group connections of the Party."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "All groups"@en ;
        rdfs:subClassOf   odrl:Party ;
        vs:term_status    "testing" .

odrl:present  a                 skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to perform or exhibit an Asset to an audience."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Present"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:count  a               owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "The numeric count indicating the number of times the corresponding entity may be exercised"@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "count"@en ;
        rdfs:range          xsd:nonNegativeInteger ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:prohibit  a          owl:NamedIndividual , odrl:ConflictTerm ;
        rdfs:comment      "Prohibitions take precedence over permissions."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Prohibit"@en ;
        vs:term_status    "testing" .

odrl:Party  a             owl:Class , rdfs:Class ;
        rdfs:comment      "An entity which can participate in policy transactions. Use an instance of the Party class where it's necessary to associate a Scope with the relationship; in other cases, simply reference the party directly."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Party"@en ;
        vs:term_status    "testing" .

odrl:Policy  a            owl:Class , rdfs:Class ;
        rdfs:comment      "A top level entity for describing policies."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Policy"@en ;
        rdfs:subClassOf   odrl:Asset ;
        vs:term_status    "testing" .

odrl:version  a             owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "The scope of versions for the asset."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "version"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:All  a               owl:Class , rdfs:Class ;
        rdfs:comment      "Specifies that the scope of the relationship is all of the collective individuals within a context."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "All"@en ;
        rdfs:subClassOf   odrl:Party ;
        vs:term_status    "testing" .

odrl:dataType  a          rdf:Property ;
        rdfs:comment      "The unit of measurement used for the constraint value."@en ;
        rdfs:domain       odrl:Constraint ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "unit"@en ;
        rdfs:range        rdfs:Resource ;
        vs:term_status    "testing" .

odrl:give  a                    skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to transfer the ownership of the Asset to a third party without compensation and while deleting the original asset."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Give"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:transfer .

odrl:Individual  a        owl:Class , rdfs:Class ;
        rdfs:comment      "Specifies that the scope of the relationship is the single Party individual."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Individual"@en ;
        rdfs:subClassOf   odrl:Party ;
        vs:term_status    "testing" .

odrl:Rule  a              owl:Class , rdfs:Class ;
        rdfs:comment      "An abstract common ancestor to Permissions, Prohibitions and Duties."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Rule"@en ;
        vs:term_status    "testing" .

odrl:informedParty  a       owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "The Party to be informed of all uses."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "informed party"@en ;
        rdfs:subPropertyOf  odrl:function ;
        vs:term_status      "testing" .

odrl:write  a             skos:Concept , odrl:Action ;
        rdfs:comment      "The act of writing to the asset."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Write"@en ;
        ont:deprecatedBy  odrl:writeTo ;
        vs:term_status    "deprecated" .

odrl:writeTo  a                 skos:Concept , odrl:Action ;
        rdfs:comment            "The act of adding data to the Asset."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Write to"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:language  a            owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "The natural language applicable to the asset usage"@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "language"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:elapsedTime  a         owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "A period of time in which the policy action can be exercised."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "elapsed time"@en ;
        rdfs:range          rdfs:Literal ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:actions  a             skos:ConceptScheme ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "ODRL Actions vocabulary"@en ;
        skos:hasTopConcept  odrl:secondaryUse , odrl:transform , odrl:execute , odrl:export , odrl:attachPolicy , odrl:grantUse , odrl:concurrentUse , odrl:delete , odrl:reviewPolicy , odrl:install , odrl:reproduce , odrl:writeTo , odrl:modify , odrl:append , odrl:uninstall , odrl:compensate , odrl:distribute , odrl:extract , odrl:copy , odrl:ensureExclusivity , odrl:give , odrl:attribute , odrl:annotate , odrl:write , odrl:preview , odrl:nextPolicy , odrl:lend , odrl:include , odrl:lease , odrl:acceptTracking , odrl:translate , odrl:move , odrl:anonymize , odrl:pay , odrl:appendTo , odrl:present , odrl:derive , odrl:textToSpeech , odrl:sell , odrl:commercialize , odrl:shareAlike , odrl:adHocShare , odrl:play , odrl:share , odrl:read , odrl:print , odrl:archive , odrl:inform , odrl:obtainConsent , odrl:use , odrl:display , odrl:license , odrl:attachSource , odrl:watermark , odrl:index , odrl:aggregate .

odrl:ODRL21.rdf  a         dcmit:Text ;
        rdfs:label         "ODRL 2.1 (RDF/XML)"@en ;
        dct:format         <http://purl.org/NET/mediatypes/application/rdf+xml> ;
        dct:isVersionOf    odrl: ;
        foaf:primaryTopic  odrl: .

odrl:payeeParty  a          owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "The Party is the recipient of the payment."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "payee party"@en ;
        rdfs:subPropertyOf  odrl:function ;
        ont:deprecatedBy    odrl:compensatedParty ;
        vs:term_status      "deprecated" .

odrl:move  a                    skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to move the Asset from one digital location to another including deleting the original copy. After the Asset has been moved, the original copy must be deleted."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Move"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:policyUsage  a       owl:NamedIndividual ;
        rdfs:comment      "When used as an event in constraints, indicates that the event occurs at the time when the policy is executed."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Policy usage time"@en ;
        vs:term_status    "testing" .

odrl:export  a            skos:Concept , odrl:Action ;
        rdfs:comment      "The act of transforming the asset into a new form."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Export"@en ;
        ont:deprecatedBy  odrl:transform ;
        vs:term_status    "deprecated" .

odrl:pay  a               skos:Concept , odrl:Action ;
        rdfs:comment      "The act of paying a financial amount to a party for use of the asset."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Pay"@en ;
        ont:deprecatedBy  odrl:compensate ;
        vs:term_status    "deprecated" .

odrl:play  a                    skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to perform an audio Asset to an audience."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Play"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:present .

odrl:aggregate  a               skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to use the Asset or parts of it as part of a composite collection."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Aggregate"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:perm  a              owl:NamedIndividual , odrl:ConflictTerm ;
        rdfs:comment      "Permissions take precedence over prohibitions."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Permissions"@en ;
        vs:term_status    "testing" .

odrl:Request  a           rdfs:Class , owl:Class ;
        rdfs:comment      "An instance of Request is a Policy expression that proposes terms of usage to an Asset owner."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Request"@en ;
        rdfs:subClassOf   odrl:Policy ;
        owl:disjointWith  odrl:Agreement , odrl:Privacy , odrl:Ticket , odrl:Set , odrl:Offer ;
        vs:term_status    "testing" .

odrl:use  a               skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner permits/prohibits the Assignee to use the Asset as agreed. More details may be defined in the applicable agreements or under applicable commercial laws. Refined types of actions can be expressed by the narrower actions."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Use"@en ;
        vs:term_status    "testing" .

odrl:gteq  a              owl:NamedIndividual , odrl:Operator ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Greater than or equal to"@en ;
        vs:term_status    "testing" .

odrl:hasPart  a           owl:NamedIndividual , odrl:Operator ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Has part"@en ;
        vs:term_status    "testing" .

odrl:extractChar  a       skos:Concept , odrl:Action ;
        rdfs:comment      "The act of extracting (replicating) unchanged characters from the asset."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Extract character"@en ;
        ont:deprecatedBy  <http://www.editeur.org/onix-pl/extract-char> ;
        vs:term_status    "deprecated" ;
        skos:broader      odrl:extract .

odrl:isA  a               owl:NamedIndividual , odrl:Operator ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Is a"@en ;
        vs:term_status    "testing" .

odrl:transfer  a          skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner transfers/does not transfer the ownership in perpetuity to the Assignees."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Transfer"@en ;
        vs:term_status    "testing" .

odrl:Action  a            owl:Class , rdfs:Class ;
        rdfs:comment      "Instances of Action are things one might be permitted to do or prohibited from doing to a work."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Action"@en ;
        rdfs:subClassOf   skos:Concept ;
        vs:term_status    "testing" .

odrl:proximity  a           owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "An value indicating the closeness or nearness."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "proximity"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        ont:deprecatedBy    <http://www.openmobilealliance.com/oma-dd/proximity> ;
        vs:term_status      "deprecated" .

odrl:isPartOf  a          owl:NamedIndividual , odrl:Operator ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Is part of"@en ;
        vs:term_status    "testing" .

odrl:spatial  a             rdf:Property ;
        rdfs:comment        "A code representing a geospatial area."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "spatial"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:purpose  a             owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "Specification of a defined purpose applicable to the asset usage."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "purpose"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:index  a                   skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to record the Asset in an index. For example, to include a link to the Asset in a search engine database."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Index"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:attributedParty  a     owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "The Party to be attributed."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "attributed party"@en ;
        rdfs:subPropertyOf  odrl:function ;
        vs:term_status      "testing" .

odrl:neq  a               owl:NamedIndividual , odrl:Operator ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Not equal to"@en ;
        vs:term_status    "testing" .

odrl:reproduce  a               skos:Concept , odrl:Action ;
        rdfs:comment            "The act of making an exact reproduction of the asset. The Assigner permits/prohibits the Assignees to make exact reproductions of the Asset."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Reproduce"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:relation  a          owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "Relation is an abstract property which creates an explicit link between an Action and an Asset. Sub-properties of relation are used to define the nature of that link."@en ;
        rdfs:domain       odrl:Rule ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "relation"@en ;
        rdfs:range        odrl:Asset ;
        vs:term_status    "testing" .

odrl:read  a                    skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to obtain data from the Asset. For example, the ability to read a record from a database (the Asset)."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Read"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:meteredTime  a         owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "The maximum period of metered usage time."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "metered time"@en ;
        rdfs:range          rdfs:Literal ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:prohibition  a       owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "Relates the description of an individual Prohibition to a Policy. A prohibition can be specified either in terms of an Action alone, or an instance of Prohibition relating an Action and one or more other attributes."@en ;
        rdfs:domain       odrl:Policy ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "prohibition"@en ;
        rdfs:range        [ a            owl:Class ;
                            owl:unionOf  ( odrl:Prohibition odrl:Action )
                          ] ;
        vs:term_status    "testing" .

odrl:nextPolicy  a        skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner requires that the Assignees grants the specified Policy to a third party for their use of the Asset."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Next policy"@en ;
        vs:term_status    "testing" .

odrl:Permission  a        owl:Class , rdfs:Class ;
        rdfs:comment      "A Permission is a rule which indicates the actions the assignee is permitted to perform on the associated asset. In other words, what the assigner (supplier) has granted to the assignee (consumer)."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Permission"@en ;
        rdfs:subClassOf   odrl:Rule ;
        owl:disjointWith  odrl:Duty , odrl:Prohibition ;
        vs:term_status    "testing" .

odrl:Asset  a                owl:Class , rdfs:Class ;
        rdfs:comment         "Asset is anything which can be subject to a policy. Asserting that something the target of a policy implies that it is an Asset."@en ;
        rdfs:isDefinedBy     odrl: ;
        rdfs:label           "Asset"@en ;
        owl:equivalentClass  owl:Thing ;
        vs:term_status       "testing" .

odrl:Prohibition  a       owl:Class , rdfs:Class ;
        rdfs:comment      "A Prohibition is a rule which indicates the Actions that the assignee is prohibited to perform on the related Asset."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Prohibition"@en ;
        rdfs:subClassOf   odrl:Rule ;
        owl:disjointWith  odrl:Permission , odrl:Duty ;
        vs:term_status    "testing" .

odrl:acceptTracking  a    skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner requires that the Assignees accepts that the use of the Asset may be tracked. The collected information may be tracked by the Assigner, or may link to a Party with the role function “trackingParty”."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Accept tracking"@en ;
        vs:term_status    "testing" .

odrl:recipient  a           owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "The party that receives the result of the Action on the Asset."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "recipient"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:ODRL21.ttl  a         dcmit:Text ;
        rdfs:label         "ODRL 2.1 (Turtle)"@en ;
        dct:format         <http://purl.org/NET/mediatypes/text/turtle> ;
        dct:isVersionOf    odrl: ;
        foaf:primaryTopic  odrl: .

odrl:product  a             owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "The specified Product or Service name."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "product"@en ;
        rdfs:range          xsd:string ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:copy  a              skos:Concept , odrl:Action ;
        rdfs:comment      "The act of making an exact reproduction of the asset."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Copy"@en ;
        ont:deprecatedBy  odrl:reproduce ;
        owl:sameAs        odrl:reproduce ;
        vs:term_status    "deprecated" .

odrl:lease  a             skos:Concept , odrl:Action ;
        rdfs:comment      "The act of making available the asset to a third-party for a fixed period of time with exchange of value."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Lease"@en ;
        vs:term_status    "testing" .

odrl:duty  a              owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "A Duty indicates requirements which must be fulfilled in order to receive the permission."@en ;
        rdfs:domain       odrl:Permission ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "duty"@en ;
        rdfs:range        [ a            owl:Class ;
                            owl:unionOf  ( odrl:Duty odrl:Action )
                          ] ;
        vs:term_status    "testing" .

odrl:attachSource  a      skos:Concept , odrl:Action ;
        rdfs:comment      "The act of attaching the source of the asset and its derivatives."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Attach source"@en ;
        ont:deprecatedBy  <http://creativecommons.org/ns#SourceCode> ;
        vs:term_status    "deprecated" .

odrl:isAnyOf  a           owl:NamedIndividual , odrl:Operator ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Is any of"@en ;
        vs:term_status    "testing" .

odrl:percentage  a          owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "The amount (as a percentage) of the action applicable to the asset."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "percentage"@en ;
        rdfs:range          xsd:decimal ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:consentingParty  a     owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "The Party to obtain consent from."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "consenting party"@en ;
        rdfs:subPropertyOf  odrl:function ;
        vs:term_status      "testing" .

odrl:grantUse  a                skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignee to grant the use the Asset to third parties. This action enables the Assignee to create policies for the use of the Asset for third parties. nextPolicy is recommended to be agreed with the third party. Use of temporal constraints is recommended."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Grant use"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:lend  a              skos:Concept , odrl:Action ;
        rdfs:comment      "The act of making available the asset to a third-party for a fixed period of time without exchange of value."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Lend"@en ;
        vs:term_status    "testing" .

odrl:lteq  a              owl:NamedIndividual , odrl:Operator ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Less than or equal to"@en ;
        vs:term_status    "testing" .

odrl:gt  a                owl:NamedIndividual , odrl:Operator ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Greater than"@en ;
        vs:term_status    "testing" .

odrl:timeInterval  a        owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "Recurring period of time in which the usage may be exercised."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "time interval"@en ;
        rdfs:range          rdfs:Literal ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:payAmount  a           owl:DatatypeProperty , rdf:Property ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "payment amount"@en ;
        rdfs:range          xsd:decimal ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:transform  a               skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to make a digital copy of the digital Asset in another digital format. Typically used to convert the Asset into a different format for consumption on/transfer to a third party system."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Transform"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:relativeSize  a        owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "The relative dimension that the Asset may be resized."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "relative size"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:ODRL21.html  a        dcmit:Text ;
        rdfs:label         "ODRL 2.1 (HTML)"@en ;
        dct:format         <http://purl.org/NET/mediatypes/text/html> ;
        dct:isVersionOf    odrl: ;
        foaf:primaryTopic  odrl: .

odrl:attribute  a         skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner requires that the Assignees attributes the Asset to the Assigner or an attributed Party. May link to an Asset with the attribution information. May link to a Party with the role function “attributedParty”."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Attribute"@en ;
        vs:term_status    "testing" .

odrl:dateTime  a            owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "The date (and optional time and timezone) representing a point in time or period."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "date/time"@en ;
        rdfs:range          [ a            owl:Class ;
                              owl:unionOf  ( xsd:date xsd:dateTime )
                            ] ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:lt  a                owl:NamedIndividual , odrl:Operator ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Less than"@en ;
        vs:term_status    "testing" .

odrl:shareAlike  a        skos:Concept , odrl:Action ;
        rdfs:comment      "The act of distributing any derivative asset under the same terms as the original asset."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Share-alike"@en ;
        ont:deprecatedBy  <http://creativecommons.org/ns#ShareAlike> ;
        vs:term_status    "deprecated" .

odrl:Constraint  a        owl:Class , rdfs:Class ;
        rdfs:comment      "Parent class of all Constraints."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Constraint"@en ;
        rdfs:subClassOf   owl:Thing ;
        vs:term_status    "testing" .

odrl:commercialize  a     skos:Concept , odrl:Action ;
        rdfs:comment      "The act of using the asset in a business environment."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Commercialize"@en ;
        ont:deprecatedBy  <http://creativecommons.org/ns#CommercialUse> ;
        vs:term_status    "deprecated" .

odrl:permission  a        owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "Relates the description of an individual Permission to a Policy. A permission can be specified either in terms of an Action alone, or an instance of Permission relating an Action and one or more other attributes."@en ;
        rdfs:domain       odrl:Policy ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "permission"@en ;
        rdfs:range        [ a            owl:Class ;
                            owl:unionOf  ( odrl:Permission odrl:Action )
                          ] ;
        vs:term_status    "testing" .

odrl:appendTo  a                skos:Concept , odrl:Action ;
        rdfs:comment            "The act of appending data to the Asset without modifying the Asset in any other way"@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Append to"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:writeTo .

odrl:compensatedParty
        a                   owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "The Party is the recipient of the compensation."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "payee party"@en ;
        rdfs:subPropertyOf  odrl:function ;
        vs:term_status      "testing" .

odrl:All2ndConnections
        a                 owl:Class , rdfs:Class ;
        rdfs:comment      "Specifies that the scope of the relationship is all of the second-level connections to the Party."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "All second-level connections"@en ;
        rdfs:subClassOf   odrl:Party ;
        vs:term_status    "testing" .

odrl:inheritFrom  a       owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "Relates a policy to another policy from which terms are inherited."@en ;
        rdfs:domain       odrl:Policy ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "inherits from"@en ;
        rdfs:range        odrl:Policy ;
        vs:term_status    "testing" .

odrl:inheritAllowed  a    owl:DatatypeProperty , rdf:Property ;
        rdfs:comment      "A boolean value indicating whether this policy can be inherited from."@en ;
        rdfs:domain       odrl:Policy ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "inheritance allowed"@en ;
        rdfs:range        xsd:boolean ;
        vs:term_status    "testing" .

odrl:Privacy  a           owl:Class , rdfs:Class ;
        rdfs:comment      "An instance of Privacy is a Policy expression that stipulates the terms of usage over personal information."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Privacy"@en ;
        rdfs:subClassOf   odrl:Policy ;
        owl:disjointWith  odrl:Ticket , odrl:Set , odrl:Offer , odrl:Agreement , odrl:Request ;
        vs:term_status    "testing" .

odrl:include  a           skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner requires that the Assignees include other related assets in the Asset. For example: bio picture must be included in the attribution. Use of the Asset relation attribute is required."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Include"@en ;
        vs:term_status    "testing" .

odrl:Ticket  a            owl:Class , rdfs:Class ;
        rdfs:comment      "An instance of Ticket is a Policy expression that stipulates the terms of usage and is redeemable by any Party who currently holds the Ticket in their possession."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Ticket"@en ;
        rdfs:subClassOf   odrl:Policy ;
        owl:disjointWith  odrl:Agreement , odrl:Privacy , odrl:Request , odrl:Set , odrl:Offer ;
        vs:term_status    "testing" .

odrl:operator  a          owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "The Boolean operator applied to a constraint and its operand."@en ;
        rdfs:domain       odrl:Constraint ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "operator"@en ;
        rdfs:range        odrl:Operator ;
        vs:term_status    "testing" .

odrl:support  a           owl:NamedIndividual , odrl:UndefinedTerm ;
        rdfs:comment      "Undefined actions do not invalidate the policy, but notification for review must occur."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Support"@en ;
        vs:term_status    "testing" .

odrl:media  a               rdf:Property ;
        rdfs:comment        "The media type in which the asset may be used."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "media"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:install  a                 skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to load the computer program Asset onto a storage device which allows operating or running the Asset."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Install"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:event  a               owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "Specification of a defined event applicable to the asset usage."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "event"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:execute  a                 skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to run the computer program Asset. For example, machine executable code or Java such as a game or application."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Execute"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:ConflictTerm  a      owl:Class , rdfs:Class ;
        rdfs:comment      "Instances of ConflictTerm describe policies for resolving conflicts."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Conflict term"@en ;
        vs:term_status    "testing" .

odrl:Duty  a              owl:Class , rdfs:Class ;
        rdfs:comment      "A Duty is a rule which indicates requirements which must be fulfilled in order to receive the permission."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Duty"@en ;
        rdfs:subClassOf   odrl:Rule ;
        owl:disjointWith  odrl:Permission , odrl:Prohibition ;
        vs:term_status    "testing" .

odrl:obtainConsent  a     skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner requires that the Assignees obtains explicit consent from the Assigner or a consenting Party to perform the requested action in relation to the Asset. Used as a Duty to ensure that the Assigner or a Party is authorized to approve such actions on a case-by-case basis. May link to a Party with the role function “consentingParty”."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Obtain consent"@en ;
        vs:term_status    "testing" .

odrl:ODRL21.xsd  a         dcmit:Text ;
        rdfs:label         "ODRL 2.1 (XML Schema Definition)"@en ;
        dct:format         <http://purl.org/NET/mediatypes/application/xml> ;
        dct:isVersionOf    odrl: ;
        foaf:primaryTopic  odrl: .

odrl:Offer  a             rdfs:Class , owl:Class ;
        rdfs:comment      "An instance of Offer is a Policy expression that proposes terms of usage from an Asset owner."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Offer"@en ;
        rdfs:subClassOf   odrl:Policy ;
        owl:disjointWith  odrl:Agreement , odrl:Privacy , odrl:Ticket , odrl:Request , odrl:Set ;
        vs:term_status    "testing" .

odrl:uninstall  a         skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner requires that the Assignees unload and delete the computer program Asset from a storage device and disable its readiness for operation. The Asset is no longer accessible to the Assignees."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Uninstall"@en ;
        vs:term_status    "testing" .

odrl:license  a           skos:Concept , odrl:Action ;
        rdfs:comment      "The act of granting the right to use the asset to a third-party."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "License"@en ;
        ont:deprecatedBy  odrl:grantUse ;
        vs:term_status    "deprecated" .

odrl:translate  a               skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to translate the original natural language of an Asset into another natural language. A new derivative Asset is created by that action."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Translate"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:constraint  a        owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "One or more constraints which affect the validity of the Permission; e.g. if the Action play is only permitted for a certain period of time."@en ;
        rdfs:domain       odrl:Rule ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "constraint"@en ;
        rdfs:range        odrl:Constraint ;
        vs:term_status    "testing" .

odrl:ensureExclusivity
        a                 skos:Concept , odrl:Action ;
        rdfs:comment      "The Assignee requires that the Assigners ensure that the permission on the Asset is exclusive to the Assignee."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Ensure exclusivity"@en ;
        vs:term_status    "testing" .

odrl:textToSpeech  a            skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to have a text Asset read out loud to an audience."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Text-to-speech"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:target  a              owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "The target property specifies the Asset upon which the Action is performed."@en ;
        rdfs:domain         odrl:Rule ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "target"@en ;
        rdfs:range          odrl:Asset ;
        rdfs:subPropertyOf  odrl:relation ;
        vs:term_status      "testing" .

odrl:share  a             skos:Concept , odrl:Action ;
        rdfs:comment      "The act of the non-commercial reproduction and distribution of the asset to third-parties."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Share"@en ;
        ont:deprecatedBy  <http://creativecommons.org/ns#Distribution> ;
        vs:term_status    "deprecated" .

odrl:delete  a            skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner requires that the Assignees permanently removes all copies of the Asset. Use a constraint to define under which conditions the Asset should be deleted."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Delete"@en ;
        vs:term_status    "testing" .

odrl:trackingParty  a       owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "The Party is the usage tracker."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "tracking party"@en ;
        rdfs:subPropertyOf  odrl:function ;
        vs:term_status      "testing" .

odrl:undefined  a         owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "Relates the mechanism used for handling undefined actions to a Policy. If no mechanism is specified, the default is invalid."@en ;
        rdfs:domain       odrl:Policy ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "undefined"@en ;
        rdfs:range        odrl:UndefinedTerm ;
        vs:term_status    "testing" .

odrl:eq  a                owl:NamedIndividual , odrl:Operator ;
        rdfs:comment      "Indicating that a given value equals the operand of the Constraint."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Equal to"@en ;
        vs:term_status    "testing" .

odrl:digitize  a                skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to produce a digital copy of (or otherwise digitize) the Asset from its analogue form."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Digitize"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:extractWord  a       skos:Concept , odrl:Action ;
        rdfs:comment      "The act of extracting (replicating) unchanged words from the asset."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Extract word"@en ;
        ont:deprecatedBy  <http://www.editeur.org/onix-pl/extract-page> ;
        vs:term_status    "deprecated" ;
        skos:broader      odrl:extract .

odrl:reviewPolicy  a      skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner requires that the Assignees have a person review the Policy applicable to the Asset. Used when human intervention is required to review the Policy. May link to an Asset which represents the full Policy information."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Review policy"@en ;
        vs:term_status    "testing" .

odrl:absolutePosition
        a                   owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "A point defined with absolute coordinates."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "absolute position"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:Agreement  a         owl:Class , rdfs:Class ;
        rdfs:comment      "Policy expressions that are formal contracts (or licenses) stipulating all the terms of usage and all the parties involved."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Agreement"@en ;
        rdfs:subClassOf   odrl:Policy ;
        owl:disjointWith  odrl:Set , odrl:Offer , odrl:Ticket , odrl:Privacy , odrl:Request ;
        vs:term_status    "testing" .

odrl:watermark  a         skos:Concept , odrl:Action ;
        rdfs:comment      "The Assigner requires that the Assignees apply a watermark as provided by the Assigner to the Asset. It is recommended to embed a link to the watermark."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Watermark"@en ;
        vs:term_status    "testing" .

odrl:derive  a                  skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to create a new derivative Asset from this Asset and to edit or modify the derivative. A new asset is created and may have significant overlaps with the original Asset. (Note that the notion of whether or not the change is significant enough to qualify as a new asset is subjective). To the derived Asset a next policy may be applied."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Derive"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:concurrentUse  a           skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to create multiple copies of the Asset that are being concurrently used."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Concurrent use"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:anonymize  a               skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to anonymize all or parts of the Asset. For example, to remove identifying particulars for statistical or for other comparable purposes, or to use the asset without stating the author/source."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Anonymize"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:use .

odrl:append  a            skos:Concept , odrl:Action ;
        rdfs:comment      "The act of adding to the end of an asset."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Append"@en ;
        ont:deprecatedBy  odrl:appendTo ;
        vs:term_status    "deprecated" .

odrl:UndefinedTerm  a     owl:Class , rdfs:Class ;
        rdfs:comment      "Instances of UndefinedTerm describe policies for processing unsupported actions."@en ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "Undefined values"@en ;
        vs:term_status    "testing" .

odrl:fileFormat  a          owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "The file format applicable to the Asset."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "file format"@en ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:function  a          owl:ObjectProperty , rdf:Property ;
        rdfs:comment      "Function is an abstract property whose sub-properties define the roles which may be fulfilled by a party in relation to a Rule."@en ;
        rdfs:domain       odrl:Rule ;
        rdfs:isDefinedBy  odrl: ;
        rdfs:label        "function"@en ;
        rdfs:range        odrl:Party ;
        vs:term_status    "testing" .

odrl:output  a              owl:ObjectProperty , rdf:Property ;
        rdfs:comment        "The output property specifies the Asset which is created from the output of the Action."@en ;
        rdfs:domain         odrl:Rule ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "output"@en ;
        rdfs:range          odrl:Asset ;
        rdfs:subPropertyOf  odrl:relation ;
        vs:term_status      "testing" .

odrl:absoluteSize  a        owl:DatatypeProperty , rdf:Property ;
        rdfs:comment        "The absolute dimension that the Asset may be resized."@en ;
        rdfs:isDefinedBy    odrl: ;
        rdfs:label          "absolute size"@en ;
        rdfs:range          xsd:decimal ;
        rdfs:subPropertyOf  odrl:rightOperand ;
        vs:term_status      "testing" .

odrl:extract  a                 skos:Concept , odrl:Action ;
        rdfs:comment            "The Assigner permits/prohibits the Assignees to extract parts of the Asset and to use it as a new Asset. A new asset is created and may have very little in common with the original Asset. (Note that the notion of whether or not the change is significant enough to qualify as a new asset is subjective). To the extracted Asset a next policy may be applied."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "Extract"@en ;
        vs:term_status          "testing" ;
        skos:broaderTransitive  odrl:reproduce .

odrl:system  a                  rdf:Property ;
        rdfs:comment            "An identifiable computing system."@en ;
        rdfs:isDefinedBy        odrl: ;
        rdfs:label              "system"@en ;
        rdfs:subPropertyOf      odrl:rightOperand ;
        ont:deprecatedBy        odrl:systemDevice ;
