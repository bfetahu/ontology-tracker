        rdfs:domain     [ a            owl:Class ;
                          owl:unionOf  ( cal:Vtimezone cal:Vtimezone cal:Vtimezone )
                        ] ;
        rdfs:label      "TZURL" ;
        spec:valueType  "URI" .

