cal:Value_RECUR  a  owl:Class .

cal:transp  a           owl:DatatypeProperty ;
        rdfs:comment    "\n\t    value type: TEXT" , "This property defines whether an event is transparent or not to busy time searches." ;
        rdfs:domain     cal:Vevent ;
