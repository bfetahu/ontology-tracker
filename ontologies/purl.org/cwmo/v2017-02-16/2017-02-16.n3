        :applyEvaluationThanks  :Complexity ;
        :evalTechAppliedBy      :Anon3 .

:hasExpressionMode  a  owl:IrreflexiveProperty , owl:AsymmetricProperty , owl:ObjectProperty ;
        rdfs:domain  :CreativeTechnique ;
        rdfs:range   :ExpressionMode .

