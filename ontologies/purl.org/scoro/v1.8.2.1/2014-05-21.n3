@prefix dc:    <http://purl.org/dc/elements/1.1/> .
@prefix :      <http://purl.org/spar/scoro/> .
@prefix foaf:  <http://xmlns.com/foaf/0.1/> .
@prefix scoro: <http://purl.org/spar/scoro/> .
@prefix owl2:  <http://purl.org/spar/scoro/owl:> .
@prefix dcterms: <http://dublincore.org/documents/dcmi-terms/> .
@prefix timeindexedsituation: <http://www.ontologydesignpatterns.org/cp/owl/timeindexedsituation.owl#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix pro:   <http://purl.org/spar/pro/> .
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix owl:   <http://www.w3.org/2002/07/owl#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix tvc:   <http://www.essepuntato.it/2012/04/tvc/> .

scoro:ComputationalAgent
        a                owl:Class ;
        rdfs:comment     "A computer system or software program whose action has an effect on external events, for example a laboratory robot that undertakes your experiments, or the financial control system that controls an institution's payrole, or that, following programmed business rules, prevents the principal investigator from placing an order for research equipment because there is insufficient money left in the project budget."@en ;
        rdfs:label       "computational agent"@en ;
        rdfs:subClassOf  foaf:Agent .

scoro:supervisor  a   scoro:EducationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role of a person who supervises the research activities of a student during that student's research project undertaken as part of a degree course at a university."@en ;
        rdfs:label    "supervisor"@en .

scoro:performs-experiments
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of performing experiments."@en ;
        rdfs:label    "performs experiments"@en .

scoro:ensures-regulatory-compliance
        a             scoro:OrganizationalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of ensuring regulatory compliance for the research undertaken, for example in obtaining informed consent from patients, approval from research ethics or radiation safety committees, or a license for animal experimentation.  "@en ;
        rdfs:label    "ensures regulatory compliance"@en .

scoro:curator  a      scoro:DataRole , owl:NamedIndividual ;
        rdfs:comment  "The role of an agent that documents, cares for and manages collections of objects (for example in a museum) or of data (as in a data repository or database).  [A data curator is a curator who specializes in digital data, while a librarian is a type of curator specializing in textual publications.]"@en ;
        rdfs:label    "curator"@en .

scoro:ContributionEffort
        a                owl:Class ;
        rdfs:comment     "Defines the effort expended in making a contribution."@en ;
        rdfs:label       "contribution effort"@en ;
        rdfs:subClassOf  owl:Thing .

scoro:ProjectRole  a     owl:Class ;
        rdfs:comment     "A role that an agent might hold with respect to a project.  Individual members of this class are used to specify particular roles."@en ;
        rdfs:label       "project role"@en ;
        rdfs:subClassOf  pro:Role .

scoro:prepares-supplementary-information
        a             scoro:AuthorshipContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of preparing the supplementary information files submitted with the manuscript. "@en ;
        rdfs:label    "prepares supplementary information"@en .

scoro:spokesperson  a  scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person who makes statements on behalf of another individual, a group, a project or an institution"@en ;
        rdfs:label    "spokesperson"@en .

scoro:college-head  a  scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a senior academic who heads a college and acts as its CEO.  Among the many alternative titles give to this role, the most common are President or Master of the college."@en ;
        rdfs:label    "college head"@en .

scoro:access-provider
        a             scoro:DataRole , owl:NamedIndividual ;
        rdfs:comment  "The role of an agent who provides access to a resource, for example an online data repository."@en ;
        rdfs:label    "access provider"@en .

scoro:ExperimentalContribution
        a                owl:Class ;
        rdfs:comment     "Defines the type or nature of a practical or experimental contribution made to the research.  Individual members of this class are used to specify particular contributions."@en ;
        rdfs:label       "experimental contribution"@en ;
        rdfs:subClassOf  scoro:Contribution .

scoro:chief-technology-officer
        a             scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person who is the chief technology officer of an organization, responsible for directing and managing technical developments for that organization."@en ;
        rdfs:label    "chief technology officer"@en , "CTO"@en .

scoro:computer-programmer
        a             scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being someone who develops computer software."@en ;
        rdfs:label    "computer programmer"@en .

scoro:senior-author  a  scoro:AuthorshipRole , owl:NamedIndividual ;
        rdfs:comment  "The role an author has of being the senior author of the document. "@en ;
        rdfs:label    "senior author"@en .

scoro:maintains-research-facility
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of managing and operating a research facility, for example a confocal microscopy facility."@en ;
        rdfs:label    "maintains research facility"@en .

scoro:service-engineer
        a             scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person who services, maintains and repairs equipment, facilities or technical infrastructure. "@en ;
        rdfs:label    "service engineer"@en .

scoro:employer  a     scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent that employs one or more people to undertake work on that agent's behalf."@en ;
        rdfs:label    "employer"@en .

scoro:provides-administrative-support
        a             scoro:OrganizationalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of providing administrative support for the research project.  "@en ;
        rdfs:label    "provides administrative support"@en .

scoro:contact-person  a  scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role of a person who serves as the primary contact and information provider for third parties wishing to find out more about an organization or an activity."@en ;
        rdfs:label    "contact person"@en .

scoro:member  a       scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent of belonging to a group or organization."@en ;
        rdfs:label    "member"@en .

[ a                    owl:AllDifferent ;
  owl:distinctMembers  ( scoro:article-guarantor )
] .

scoro:principal-investigator
        a             scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being the principal investigator for a research investigation."@en ;
        rdfs:label    "principal investigator"@en .

scoro:employee  a     scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person employed for wages or a salary by an organization, to undertake work for that organization."@en ;
        rdfs:label    "employee"@en .

scoro:co-applicant  a  scoro:ProjectRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a co-applicant on the grant application to obtain funding to fund an endeavour such as a research project."@en ;
        rdfs:label    "co-applicant"@en .

scoro:isEqualToRoleInTIme
        a       owl:AnnotationProperty .

scoro:designs-experiments
        a             scoro:IntellectualContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of experimental design and planning."@en ;
        rdfs:label    "designs experiments"@en .

scoro:DataRole  a        owl:Class ;
        rdfs:comment     "A role that an agent might hold with respect to data.  Individual members of this class are used to specify particular roles."@en ;
        rdfs:label       "data role"@en ;
        rdfs:subClassOf  pro:Role .

tvc:withinContext  a  owl:ObjectProperty .

scoro:funding-recipient
        a             scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent who is the official recipient of funding, for example the university of which a funded research project leader is a member."@en ;
        rdfs:label    "funding recipient"@en .

scoro:owner  a        scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent who is the owner of something with actual or potential financial value, for example a building, a piece of equipment, the copyright of a document, or the intellectual property rights relating to an idea or discovery."@en ;
        rdfs:label    "owner"@en .

scoro:participant  a  scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent who takes part in something, for example a conference, a private meeting or a public event."@en ;
        rdfs:label    "participant"@en .

scoro:researcher  a   scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a researcher involved in a research investigation."@en ;
        rdfs:label    "researcher"@en .

scoro:minor-effort  a  scoro:ContributionEffort , owl:NamedIndividual ;
        rdfs:comment  "The contributor provided a minor effort towards achieving this contribution. "@en ;
        rdfs:label    "contributed minor effort"@en .

scoro:provides-service
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of providing a research service, for example a DNA sequencing service or a custom peptide synthesis service."@en ;
        rdfs:label    "provided service"@en .

scoro:undergraduate-student
        a             scoro:EducationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role of a person registerd for an undergraduate degree at a university."@en ;
        rdfs:label    "undergraduate student"@en .

scoro:research-assistant
        a             scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a research assistant involved in a research investigation."@en ;
        rdfs:label    "research assistant"@en .

scoro:rights-holder  a  scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent who holds the legal rights to a project or activity and its products and outcomes, for example the exploitation, licensing and intellectual property rights.  \n\n[See also http://purl.org/spar/scoro/patent-holder and http://purl.org/spar/pro/copyright-owner]."@en ;
        rdfs:label    "rights holder"@en .

tvc:withValue  a  owl:ObjectProperty .

scoro:manager  a      scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person responsible for managing the day-to-day activities of an activity, enterprise or organization."@en ;
        rdfs:label    "manager"@en .

scoro:embargo-controller
        a             scoro:DataRole , owl:NamedIndividual ;
        rdfs:comment  "The role of the person who has responsibility for setting and lifting embargos that restrict access to a dataset (or a document) for a specified period of time."@en ;
        rdfs:label    "embargo controller"@en .

timeindexedsituation:forEntity
        a       owl:ObjectProperty .

scoro:article-guarantor
        a             scoro:AuthorshipRole , owl:NamedIndividual ;
        rdfs:comment  "The role of a person who takes responsibility for the integrity of the work as a whole, from the inception of the research investigation to the published research article."@en ;
        rdfs:label    "article guarantor"@en .

scoro:collaborator  a  scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person, typically from another research group or institution, who collaborates with the team undertaking a research investigation."@en ;
        rdfs:label    "collaborator"@en .

scoro:data-manager  a  scoro:DataRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a data manager, responsible for day-to-day management and back-up of data, for example those arising from the research project."@en ;
        rdfs:label    "data manager"@en .

scoro:EducationalRole
        a                owl:Class ;
        rdfs:comment     "A role that an agent might hold with respect to an educational organization or activity.  Individual members of this class are used to specify particular educational roles."@en ;
        rdfs:label       "educational role"@en ;
        rdfs:subClassOf  pro:Role .

scoro:revises-manuscript
        a             scoro:AuthorshipContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of critically reviewing and revising drafts of the manuscript of the research paper."@en ;
        rdfs:label    "revises manuscript"@en .

scoro:project-manager
        a             scoro:ProjectRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being project manager for a project such as a research project."@en ;
        rdfs:label    "project manager"@en .

scoro:isRelatedToContributionSituation
        a                   owl:ObjectProperty ;
        rdfs:comment        "A property relating an entity to the contribution situation for which that entity provides the context."@en ;
        rdfs:label          "is related to contribution situation"@en ;
        rdfs:range          scoro:ContributionSituation ;
        rdfs:subPropertyOf  timeindexedsituation:hasTimeIndexedSetting ;
        owl:inverseOf       scoro:hasContributionContext .

scoro:chief-scientist
        a             scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "A senior scientist who takes a leadership role in an organization or group."@en ;
        rdfs:label    "chief scientist"@en .

rdfs:seeAlso  a  owl:AnnotationProperty .

scoro:processes-data  a  scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of processing, cleaning, assembling, condensing or refining the data."@en ;
        rdfs:label    "processs data"@en .

scoro:secures-funding
        a             scoro:OrganizationalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of securing funding for the research project.  \n\nNote that an individual other than the lead applicant and the co-applicants on a grant application may make contributions to securing funding for a research project, for example by assisting in writing the grant application."@en ;
        rdfs:label    "secures funding"@en .

scoro:affiliate  a      scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment    "The role an agent has of being affiliated to some organization or institution in a context of some entity or activity, for example a document, a project or an internship."@en ;
        rdfs:label      "affiliate"@en ;
        dc:description  "The affiliation of a person to an organization or institution can be specified by using the property pro:relatesToEntity, as shown in the following example:\n\nfoaf:Person scoro:holdsOrganizationalRole [ pro:withRole pro:affiliate ; pro:relatesToEntity _:aff ] .\n\n_:aff a foaf:Organization ; vcard:extended-address 'Department of Zoology, University of Oxford, United Kingdom' ."@en .

scoro:makesContribution
        a                   owl:ObjectProperty ;
        rdfs:comment        "An object property linking an agent to a contribution.  This is done via the indirection: foaf:Agent scoro:makesContribution [ a scoro:ContributionSituation ; scoro:withContribution scoro:Contribution ; scoro:withEffort scoro:ContributionEffort ] .  The ontology permits one to specify the time period over which a contribution is made, and the other contextual entities to which that agent's contribution relates."@en ;
        rdfs:domain         foaf:Agent ;
        rdfs:label          "makes contribution"@en ;
        rdfs:range          scoro:ContributionSituation ;
        rdfs:subPropertyOf  tvc:hasValue .

scoro:supervises-colleagues
        a             scoro:OrganizationalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of supervising, mentoring, instructing or training colleagues, particularly junior research assistants and research students, in aspects of the research or its methodologies."@en ;
        rdfs:label    "Supervises, mentors or trains colleagues"@en .

foaf:Person  a      owl:Class ;
        rdfs:label  "person"@en .

scoro:withEffort  a         owl:ObjectProperty ;
        rdfs:comment        "An object property connecting an agent's contribution with respect to a particular entity to the amount of effort exerted by this agent in making that contribution, defined by individuals of the class scoro:ContributionEffort."@en ;
        rdfs:domain         scoro:ContributionSituation ;
        rdfs:label          "with contribution effort"@en ;
        rdfs:range          scoro:ContributionEffort ;
        rdfs:subPropertyOf  timeindexedsituation:forEntity .

scoro:possessor  a    scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent as possessor or owner of some entity such as an address or a telephone number."@en ;
        rdfs:label    "possessor"@en .

scoro:illustrator  a  scoro:AuthorshipRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being an illustrator of, or for, some entity."@en ;
        rdfs:label    "illustrator"@en .

scoro:conceives-project
        a             scoro:IntellectualContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of conceiving the research project in the first place.  \n\nNote that an individual other than the lead applicant and the co-applicants on a grant application may make considerable contributions to conceiving a research project, but may be prevented from being a formal applicant by reason of affiliation or status (e.g. as a non-academic partner, or a research student)."@en ;
        rdfs:label    "conceives project"@en .

scoro:departmental-administrator
        a             scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person who is the administrator of a department within a larger organization, for example of a university academic department."@en ;
        rdfs:label    "departmental administrator"@en .

scoro:funder  a       scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent of providing funds, for example for a research project."@en ;
        rdfs:label    "funder"@en .

scoro:collects-data  a  scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of making experimental observations and collecting data, for example using a telescope, a microscope or an analytical instrument."@en ;
        rdfs:label    "collects data"@en .

scoro:withContribution
        a                   owl:FunctionalProperty , owl:ObjectProperty ;
        rdfs:comment        "An object property connecting an agent's contribution situation to a definition of the contribution made by this agent, specified as an instance of the class scoro:Contribution or of one of its sub-classes."@en ;
        rdfs:domain         scoro:ContributionSituation ;
        rdfs:label          "with contribution"@en ;
        rdfs:range          scoro:Contribution ;
        rdfs:subPropertyOf  tvc:withValue .

scoro:college-fellow  a  scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an academic as a senior member of a college."@en ;
        rdfs:label    "college fellow"@en .

scoro:co-investigator
        a             scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a co-investigator for a research investigation."@en ;
        rdfs:label    "co-investigator"@en .

scoro:lead-applicant  a  scoro:ProjectRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being the lead applicant on the grant application to obtain funding to fund an endeavour such as a research project."@en ;
        rdfs:label    "lead applicant"@en .

scoro:builds-and-or-maintains-instruments
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of building instruments, equipment or scientific apparatus and/or maintaining the same in a workable condition for use in experiments."@en ;
        rdfs:label    "builds and/or maintains instruments"@en .

dc:creator  a   owl:AnnotationProperty .

scoro:provides-software
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of providing software to facilitate the research, while not actively participating in the work."@en ;
        rdfs:label    "provides software"@en .

scoro:isEqualToContributionSituation
        a             owl:TransitiveProperty , owl:SymmetricProperty , owl:ObjectProperty ;
        rdfs:comment  "This property allows the link of two contribution situations that are equals for particular aspects (i.e. for type of contribution, contribution effort and context)."@en ;
        rdfs:domain   scoro:ContributionSituation ;
        rdfs:label    "is equal to contribution situation"@en ;
        rdfs:range    scoro:ContributionSituation .

scoro:consortium-author
        a             scoro:AuthorshipRole , owl:NamedIndividual ;
        rdfs:comment  "The authorship role of an organisation or consortium that has contributed collectively to the work described in the publication and is named in the list of authors, e.g. 'Wellcome Trust Case Control Consortium' or 'Malaria Genomic Epidemiology Network'.  (The members of such a group should be separately listed elsewhere in the document)."@en ;
        rdfs:label    "consortium author"@en .

pro:RoleInTime  a  owl:Class .

scoro:patent-holder  a  scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent who holds a patent.\n\n[See also http://purl.org/spar/scoro/rights-holder and http://purl.org/spar/pro/copyright-owner]."@en ;
        rdfs:label    "patent holder"@en .

scoro:director  a     scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person who is in charge of an organization or activity, for example a research institute or a dramatic performance."@en ;
        rdfs:label    "director"@en .

pro:Role  a     owl:Class .

scoro:financial-controller
        a             scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent of controlling the finances, for example the finances of an organization, a building project or a research project.  This role extends to the control of or the authorisation of expenditure from a budget, and may be undertaken by a computational agent."@en ;
        rdfs:label    "financial controller"@en .

scoro:leads-investigation
        a             scoro:IntellectualContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of providing intellectual leadership and direction for the research.  \n\nNote that an individual other than the principal investigator and the co-investigators may make considerable leadership contributions to a research investigation."@en ;
        rdfs:label    "leads investigation"@en .

scoro:chief-executive-officer
        a             scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person who is the chief executive officer of an organization, responsible for directing and managing the business of that organization."@en ;
        rdfs:label    "chief executive officer"@en , "CEO"@en .

scoro:maintains-organisms-or-cells
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of maintaining multicellular organisms (plants, animals or fungi) or cell cultures used as experimental subjects, and of breeding these where appropriate.  \n\n[Note that there may be occasions when it is inappropriate to acknowledge such contributions, for fear of personal reprisals on the individuals involved from animal rights activists]."@en ;
        rdfs:label    "maintains organisms or cell cultures"@en .

scoro:FinancialRole  a   owl:Class ;
        rdfs:comment     "A role that an agent might hold with respect to academic and project finance.  Individual members of this class are used to specify particular roles."@en ;
        rdfs:label       "financial role"@en ;
        rdfs:subClassOf  pro:Role .

scoro:project-leader  a  scoro:ProjectRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person or an institution has of being project leader for a project such as a research project."@en ;
        rdfs:label    "project leader"@en .

scoro:administrator  a  scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent responsible for the day-to-day management and running of an organization or activity."@en ;
        rdfs:label    "administrator"@en .

scoro:contractor  a   scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent who enters into a contract to undertake specified work or to supply specified services in return for payment."@en ;
        rdfs:label    "contractor"@en .

scoro:OrganizationalContribution
        a                owl:Class ;
        rdfs:comment     "Defines the type or nature of an organizational or administrative contribution.  Individual members of this class are used to specify particular contributions."@en ;
        rdfs:label       "organizational contribution"@en ;
        rdfs:subClassOf  scoro:Contribution .

scoro:maintains-IT-Infrastructure
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of maintaining the IT infrastructure required for the research."@en ;
        rdfs:label    "maintains IT infrastructure"@en .

scoro:analyses-data  a  scoro:IntellectualContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of data analysis.  This might include statistical analysis, bioinformatics analysis, or image analysis."@en ;
        rdfs:label    "analyses data"@en .

scoro:provides-advice
        a             scoro:IntellectualContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of providing advice to facilitate the research, while not actively participating in the work."@en ;
        rdfs:label    "provides advice"@en .

scoro:stakeholder  a  scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent with an interest or concern in something, for example the creation of a new university."@en ;
        rdfs:label    "stakeholder"@en .

tvc:ValueInTime  a  owl:Class .

scoro:  a                owl:Ontology ;
        rdfs:comment     "SCoRO is written in OWL 2 DL, and imports FOAF, the Friend of a Friend Ontology (http://xmlns.com/foaf/spec/) and PRO the Publishing Roles Ontology (http://purl.org/spar/pro/).   This ontology is available at http://purl.org/spar/scoro/, and uses the namespace prefix 'scoro'.\n\nIt is designed to be used in conjunction with FRAPO, the Funding, Research Administration and Projects Ontology (http://purl.org/spar/frapo/), and with FaBiO, the FRBR-aligned Bibliographic Ontology (http://purl.org/spar/fabio/).\n\nA Web-based scholarly contributions report form based on this ontology, for use in creating metadata to describe authorship roles and scholarly contributions to journal articles and their underlying research, is available from http://purl.org/spar/scoro/**** (to be created).\n\nAlternatively, an Excel Scholarly Contributions Report Form based on this ontology, created using RightField (http://www.sysmo-db.org/rightfield) also for use in creating metadata to describe authorship roles and scholarly contributions to journal articles and their underlying research, is available from http://purl.org/spar/scoro/SCoRF.xls."@en ;
        dc:contributor   "Silvio Peroni"^^xsd:string ;
        dc:creator       "David Shotton"^^xsd:string ;
        dc:date          "2014-05-21"^^xsd:string ;
        dc:description   "Since roles have contexts and times that are important to take into account (for example, a person can be author of one paper and reviewer of another, while another person will be editor of a journal only for a defined period, not for ever), the SCORO ontology permit these contexts to be specified, using an ontological design pattern called the time-indexed value in context (TVC) (http://www.essepuntato.it/2012/04/tvc/) which is part of the PRO ontology (http://purl.org/spar/pro/) imported by SCORO. \n \nThis ontology design pattern permits one to construct the following RDF statements:\n       foaf:Agent pro:holdsRoleInTime [ a pro:RoleInTime pro:withRole pro:editor ] .\n\nThus the domain of pro:withRole is not foaf:Agent, but rather an anonymous member of the class pro:RoleInTime, which itself is the range of the property pro:holdsRoleInTime, for which the domain is foaf:Agent. \n\nThe range of pro:withRole is the class pro:Role, and its sub-classes pro:PublishingRole, scoro:InvestigationalRole, scoro:OrganizationalRole, etc., whose members permit specific roles to be specified.  \n   \nThis single step of indirection permits other contextual and temporal attributes (not shown here) to be specified for this member of the class pro:RoleInTime, putting that role into context.\n\nExemplar usage: \n\nfabio:Dataset  dcterms:contributor  [ rdf:type foaf:Organization ; \n   foaf:name \"Met Office\" ;  \n   foaf:homepage <http://www.metoffice.gov.uk/>  ; \n   pro:holdsRoleInTime [ a pro:RoleInTime \n      pro:withRole  scoro:data-creator ]  ] .\n\nExactly the same use of TVC and of individuals within the class scoro:Contribution and its sub-classes is used for scoro:withContribution, enabling contributions to be time-limited and given contexts.\n"@en , "SCoRO, the Scholarly Contributions and Roles Ontology, is an ontology for use by authors and publishers for describing the contributions that may be made and the roles that may be held by a person with respect to a journal article or other publication, and by research administrators for describing contributions and roles with respect to other aspects of scholarly research.\n\nIn this ontology, the object properties (e.g. scoro:makesContribution) and the individuals within classes (e.g. scoro:conducts-experiments) are expressed in the present tense, as is conventional for ontologies, but should be taken as potentially referring to past, present or future roles and contributions."@en ;
        dc:rights        "This work is distributed under a Creative Commons Attribution License (http://creativecommons.org/licenses/by/3.0/)."@en ;
        dc:title         "SCoRO, the Scholarly Contributions and Roles Ontology"@en ;
        owl:imports      <http://purl.org/spar/pro> ;
        owl:versionInfo  "1.8.2.1"^^xsd:string .

dc:description  a  owl:AnnotationProperty .

scoro:purchaser  a    scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent of making purchases of goods or services, the cost of which is typically charged to a particular academic budget, for example a project manager placing an order to purchase equipment funded from a research grant."@en ;
        rdfs:label    "purchaser"@en .

scoro:develops-methodology
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of developing experimental methodology that enables research."@en ;
        rdfs:label    "develops methodology"@en .

scoro:major-effort  a  scoro:ContributionEffort , owl:NamedIndividual ;
        rdfs:comment  "The contributor provided a major effort towards achieving this contribution.  (It would be unusual for more than three contributors to be described as providing major effort towards achieving any particular contribution)."@en ;
        rdfs:label    "contributed major effort"@en .

scoro:research-student
        a             scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a research student, namely someone engaged in research as part of studying for a higher degree at a university or research institute, whose research will be presented in a thesis in part fulfilment of the requirements of that degree."@en ;
        rdfs:label    "research student"@en .

scoro:auditor  a      scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person who conducts a formal audit of financial accounts."@en ;
        rdfs:label    "auditor"@en .

scoro:provides-patients
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of providing access to medical or veterinary patients, to make possible the research."@en ;
        rdfs:label    "provides patients"@en .

scoro:provides-exising-data
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of providing pre-existing data for re-use in the research."@en ;
        rdfs:label    "provides existing data"@en .

scoro:writes-manuscript-draft
        a             scoro:AuthorshipContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of writing the initial draft of the manuscript of the research paper."@en ;
        rdfs:label    "writes manuscript draft"@en .

scoro:AuthorshipContribution
        a                owl:Class ;
        rdfs:comment     "Defines the type or nature of a contribution made with respect to the authorship of a publication such as a journal article.  Individual members of this class are used to specify particular authorship contributions."@en ;
        rdfs:label       "authorship contribution"@en ;
        rdfs:subClassOf  scoro:Contribution .

scoro:undertakes-modelling
        a             scoro:IntellectualContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of design, implementation and analysis of mathematical models or computer simulations."@en ;
        rdfs:label    "undertakes modelling"@en .

scoro:data-user  a    scoro:DataRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a user or re-user of existing data."@en ;
        rdfs:label    "data user"@en .

scoro:Contribution  a     owl:Class ;
        rdfs:comment      "Defines the type or nature of a contribution made by an agent.  Individual members of this class or its sub-classes are used to specify particular contributions."@en ;
        rdfs:label        "contribution"@en ;
        rdfs:subClassOf   owl:Thing ;
        owl:disjointWith  scoro:ContributionEffort .

scoro:postgraduate-student
        a             scoro:EducationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role of a person registerd for a graduate (masters or doctoral) degree at a university."@en ;
        rdfs:label    "postgraduate student"@en .

scoro:isMadeBy  a           owl:ObjectProperty ;
        rdfs:comment        "A property relating a contribution situation that an agent makes, to that agent."@en ;
        rdfs:domain         scoro:ContributionSituation ;
        rdfs:label          "is made by"@en ;
        rdfs:range          foaf:Agent ;
        rdfs:subPropertyOf  timeindexedsituation:forEntity ;
        owl:inverseOf       scoro:makesContribution .

scoro:creates-novel-organisms-or-cells
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of creating novel organisms (e.g. mouse mutants) or cells (e.g. tranformed cell lines), either for use in the research, or as a research output of use to others."@en ;
        rdfs:label    "creates novel organisms or cells"@en .

scoro:OrganizationalRole
        a                owl:Class ;
        rdfs:comment     "A role that an agent might hold with respect to an organization and its administration, particularly within an academic context.  Individual members of this class are used to specify particular roles."@en ;
        rdfs:label       "organizational role"@en ;
        rdfs:subClassOf  pro:Role .

scoro:data-curator  a  scoro:DataRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a data curator, responsible for long-term preservation of data, for example those arising from the research project."@en ;
        rdfs:label    "data curator"@en .

scoro:partner  a      scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person who is a partner with one or more other people in an organization, project, endeavour, activity or sphere of common interest."@en ;
        rdfs:label    "partner"@en .

scoro:vice-chancellor
        a             scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by the chief executive officer of a university, responsible for directing and managing its business and academic activities."@en ;
        rdfs:label    "vice-chancellor"@en .

scoro:consultant  a   scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a consultant who is providing expertise or services."@en ;
        rdfs:label    "consultant"@en .

scoro:supplier  a     scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent of providing goods or services in exchange for payment."@en ;
        rdfs:label    "supplier"@en .

dc:contributor  a  owl:AnnotationProperty .

scoro:InvestigationRole
        a                owl:Class ;
        rdfs:comment     "A role that an agent might hold with respect to an investigation.  Individual members of this class are used to specify particular roles."@en ;
        rdfs:label       "investigation role"@en ;
        rdfs:subClassOf  pro:Role .

timeindexedsituation:hasTimeIndexedSetting
        a       owl:ObjectProperty .

scoro:non-academic-staff-member
        a             scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role of a person employed within a university department, school or institute who is not a member of the academic faculty."@en ;
        rdfs:label    "non-academic staff member"@en .

scoro:accountant  a   scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person whose job is to manage financial accounts, for example the accounts of an organization, or of a particular funded project or activity of that organization."@en ;
        rdfs:label    "accountant"@en .

scoro:prepares-illustrations
        a             scoro:AuthorshipContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of selecting the data and preparing the figures and tables presenting these data for inclusion in the publication. "@en ;
        rdfs:label    "prepares illustrations"@en .

scoro:solo-effort  a  scoro:ContributionEffort , owl:NamedIndividual ;
        rdfs:comment  "The contributor was the only person who provided effort in achieving this contribution, which was entirely due to this contributor."@en ;
        rdfs:label    "contributed all the effort"@en .

scoro:web-master  a   scoro:DataRole , owl:NamedIndividual ;
        rdfs:comment  "The role of a person responsible for maintaining a web site and its content."@en ;
        rdfs:label    "web-master"@en .

scoro:scholar  a      scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role of an academic who undertakes scholarly activities, including research investigations.  This role is particularly used to describe endeavours in the arts and humanites that are frequently undertaken without financial support from a particular funded project dedicated to these scholarly activities."@en ;
        rdfs:label    "scholar"@en .

scoro:provides-technical-support
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of providing technical support that facilitates research."@en ;
        rdfs:label    "provides technical support"@en .

scoro:creates-novel-reagents
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of creating novel physical or chemical compounds, or molecular biology constructs, either for use in the research, or as a research output of use to others."@en ;
        rdfs:label    "creates novel reagents"@en .

dc:date  a      owl:AnnotationProperty .

foaf:Agent  a   owl:Class .

scoro:technician  a   scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a technician, providing technical assistance in some endeavour such as a research investigation."@en ;
        rdfs:label    "technician"@en .

dc:rights  a    owl:AnnotationProperty .

scoro:sponsor  a      scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent that provides funds or support for something such as a project or activity, often in return for access to the exploitable commercial potential associated with that project or activity, for example through advertising.\n\n[Note: there are also several other non-financial meaning of 'sponsor'.]"@en ;
        rdfs:label    "sponsor"@en .

scoro:corresponding-author
        a             scoro:AuthorshipRole , owl:NamedIndividual ;
        rdfs:comment  "The role an author has of being the corresponding author of the document, with whom editors and readers should correspond concerning it. "@en ;
        rdfs:label    "corresponding author"@en .

scoro:project-member  a  scoro:ProjectRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a member of a project such as a research project."@en ;
        rdfs:label    "project member"@en .

scoro:hasContributionContext
        a                   owl:ObjectProperty ;
        rdfs:comment        "A property relating a contribution situation to an entity representing the context for that situation."@en ;
        rdfs:domain         scoro:ContributionSituation ;
        rdfs:label          "has contribution context"@en ;
        rdfs:subPropertyOf  tvc:withinContext .

scoro:percentageFTE  a  owl:DatatypeProperty , owl:AnnotationProperty ;
        rdfs:comment  "A data property permitting specification of the proportion of a person's full time activity that is dedicated to a particular role (e.g. for part-time employment, 50% FTE)."@en ;
        rdfs:domain   pro:RoleInTime ;
        rdfs:label    "percentage full-time equivalent"@en .

scoro:isContributionIn
        a                   owl:ObjectProperty ;
        rdfs:comment        "The property relating the definition of a specific contribution to the contribution situation of an agent making that contribution."@en ;
        rdfs:domain         scoro:Contribution ;
        rdfs:label          "is contribution in"@en ;
        rdfs:range          scoro:ContributionSituation ;
        rdfs:subPropertyOf  timeindexedsituation:hasTimeIndexedSetting ;
        owl:inverseOf       scoro:withContribution .

scoro:faculty-member  a  scoro:EducationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role of an academic employed to undertake research and teaching within a university department, school or institute."@en ;
        rdfs:label    "faculty member"@en .

scoro:provides-reagents-specimens-or-materials
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of providing reagents, specimens or materials, including living organisms and cells, for example those not readily obtainable from commercial sources, to make possible the research."@en ;
        rdfs:label    "provides reagents, specimens or materials"@en .

scoro:approves-final-manuscript
        a             scoro:AuthorshipContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of approving the final version of the manuscript, prior to submission for publication. "@en ;
        rdfs:label    "approves final manuscript"@en .

scoro:postdoctoral-researcher
        a             scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a post-doctoral researcher involved in a research investigation."@en ;
        rdfs:label    "postdoctoral researcher"@en .

scoro:formulates-research-questions
        a             scoro:IntellectualContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of formulating the questions to be answered and the hypotheses to be tested by the research to be undertaken."@en ;
        rdfs:label    "formulates research questions"@en .

scoro:provides-tools-equipment-or-facilities
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of providing tools, equipment or facilities for the research, while not actively participating in the work."@en ;
        rdfs:label    "provides tools, equipment or facilities"@en .

scoro:withJobTitle  a  owl:DatatypeProperty , owl:AnnotationProperty ;
        rdfs:comment  "A data property connecting an agent's role in time to a job title that characterizes that role.  This data property complements the definition of that role in time specified by scoro:Role.  For example, for the scoro:OrganizationalRole 'Head of Department', the specific Job Title might be 'Linacre Professor of Zoology' or 'Regius Professor of Moral Philosophy'. "@en ;
        rdfs:domain   pro:RoleInTime ;
        rdfs:label    "with job title"@en .

scoro:data-publisher  a  scoro:DataRole , owl:NamedIndividual ;
        rdfs:comment  "The role of an agent who publishes data."@en ;
        rdfs:label    "data publisher"@en .

scoro:workpackage-leader
        a             scoro:ProjectRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person or institution has of being leader for a workpackage within a project such as a research project."@en ;
        rdfs:label    "workpackage leader"@en .

scoro:registrar  a    scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent responsible for maintaining official records, for example the records of student enrollment and academic standing held by a college or university, or the records of securities owned by a financial institution."@en ;
        rdfs:label    "registrar"@en .

rdfs:comment  a  owl:AnnotationProperty .

scoro:sub-contractor  a  scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent who enters into a contract to take over part of another contractor's obligation to undertake specified work or supply specified services in return for payment."@en ;
        rdfs:label    "sub-contractor"@en .

tvc:hasValue  a  owl:ObjectProperty .

scoro:inventor  a     scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being the inventor or some entity (for example a computer algorithm, or an experimental procedure)."@en ;
        rdfs:label    "inventor"@en .

scoro:ContributionSituation
        a                owl:Class ;
        rdfs:comment     "The situation an agent has of making a contribution to realizing a goal or completing a task relating to a scholarly activity or entity such as journal article.  The type or nature of the contribution is defined by the object of the object property scoro:withContribution, which is an individual in the class scoro:Contribution or one of its sub-classes.  The entity to which the contribution relates is specified using the object property scoro:relatesToEntity or one of its sub-properties, and the effort expended in making that contribution is specified using the object property scoro:withContributionEffort."@en ;
        rdfs:label       "contribution situation"@en ;
        rdfs:subClassOf  tvc:ValueInTime ;
        rdfs:subClassOf  [ a                   owl:Restriction ;
                           owl:onProperty      scoro:withContribution ;
                           owl:someValuesFrom  scoro:Contribution
                         ] ;
        dc:description   "A contribution situation describes always:\n- a particular contribution C\n- a particular agent A making contribution C\n- a particular time interval T in which A makes C (optional)\n- a particular contribution effort E \n- one or more entities (e.g. an investigation or data) that defines a context for C contributed by A. (At least one contextual entity should be specified for a particular contribution)."@en .

dc:title  a     owl:AnnotationProperty .

scoro:repository-manager
        a             scoro:DataRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a manager of a repository where entities are given secure long-term storage, typically digital repository for information objects, for example a data repository."@en ;
        rdfs:label    "repository manager"@en .

scoro:isEqualToRoleInTime
        a             owl:ObjectProperty ;
        rdfs:comment  "This property allows the link of two roles in time as being equal, and is particularly useful for specifying that two principal authorship roles are equal."@en ;
        rdfs:domain   pro:RoleInTime ;
        rdfs:label    "is equal to role in time"@en ;
        rdfs:range    pro:RoleInTime .

scoro:controls-project-finances
        a             scoro:OrganizationalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of controlling the financial aspects of the research project, including authorizing expenditure, creating and managing budgets, and submitting financial reports to the funding agency."@en ;
        rdfs:label    "controls project finances"@en .

owl:versionInfo  a  owl:AnnotationProperty .

scoro:obtains-and-or-prepares-specimens
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of obtaining and/or preparing specimens in a manner requiring research expertise or skill, for example collecting and preparing fossils, or preparing electron microscope specimens for examination.  "@en ;
        rdfs:label    "obtains and/or prepared specimens"@en .

scoro:hasPersonalIdentifier
        a               owl:DatatypeProperty ;
        rdfs:comment    "A data property specifying a unique and unambiguous identifier for an individual person within a given context, specified by the object property scoro:usesPersonalIdentifierScheme and an individual member of the class scoro:PersonalIdentifierScheme."@en ;
        rdfs:domain     foaf:Person ;
        rdfs:label      "has personal identifier"@en ;
        dc:description  " owl:equivalentProperty <http://purl.org/spar/frapo/hasPersonalIdentifier> ."@en , " owl:equivalentProperty <http://purl.org/spar/datacite/hasPersonalIdentifier> ."@en .

scoro:principal-author
        a             scoro:AuthorshipRole , owl:NamedIndividual ;
        rdfs:comment  "The role an author has of being the principal or primary author of the document, who has contributed most to the work described.  \n\n(In some communities, but not all, the principal author is named first in the author list.  Two or more authors can have the role of principal author if they contributed equally to the work.  This role should not be employed in communities in which no primacy of authorship is asserted, but where all authors are deemed to have made an equal contribution to the work.)"@en ;
        rdfs:label    "principal author"@en .

scoro:project-student
        a             scoro:InvestigationRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a project student, namely someone engaged in an undergraduate research project as part of studying for an undergraduate degree at a university, whose research will be presented in an undergraduate research project report in part fulfilment of the requirements of that degree."@en ;
        rdfs:label    "project student"@en .

scoro:IntellectualContribution
        a                owl:Class ;
        rdfs:comment     "Defines the type or nature of an intellectual contribution made to the research described in a journal article.  Individual members of this class are used to specify particular contributions."@en ;
        rdfs:label       "intellectual contribution"@en ;
        rdfs:subClassOf  scoro:Contribution .

scoro:agent  a        scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "An agent that acts on behalf of another agent."@en ;
        rdfs:label    "agent"@en .

[ a            owl:AllDisjointClasses ;
  owl:members  ( scoro:AuthorshipContribution scoro:ExperimentalContribution scoro:IntellectualContribution scoro:OrganizationalContribution )
] .

scoro:organizer  a    scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by an agent who organizes something, such as a conference, a private meeting or a public event."@en ;
        rdfs:label    "organizer"@en .

scoro:head-of-department
        a             scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person who is the chief executive officer of a department within a larger organization, for example a senior academic who is head of a university academic department."@en ;
        rdfs:label    "head of department"@en .

scoro:claimant  a     scoro:FinancialRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person making a financial claim, for example for refund of travel expenses from a project grant."@en ;
        rdfs:label    "claimant"@en .

pro:author  a   scoro:AuthorshipRole , owl:NamedIndividual .

scoro:programme-manager
        a             scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The role held by a person who is in charge of a programme of related activities or projects, for example on behalf of a funding agency."@en ;
        rdfs:label    "programme manager"@en .

scoro:AuthorshipRole  a  owl:Class ;
        rdfs:comment     "A role that a person might hold as an author of a publication or the creator of a work.  Individual members of this class are used to specify particular roles."@en ;
        rdfs:label       "authorship role"@en ;
        rdfs:subClassOf  pro:Role .

scoro:hasORCID  a           owl:DatatypeProperty ;
        rdfs:comment        "A data property relating an individual to his/her Open Researcher and Contributor ID (ORCID), an alphanumeric identifier for individual scholars described at http://about.orcid.org/."@en ;
        rdfs:domain         foaf:Person ;
        rdfs:label          "has ORCID"@en ;
        rdfs:subPropertyOf  scoro:hasPersonalIdentifier ;
        dc:description      "owl:equivalentProperty <http://purl.org/spar/frapo/hasORCID> ."@en .

scoro:photographer  a  scoro:AuthorshipRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a photographer of, or for, some entity."@en ;
        rdfs:label    "photographer"@en .

scoro:successor  a    scoro:OrganizationalRole , owl:NamedIndividual ;
        rdfs:comment  "The relationship held by an entity that succeeds another, for example a new electron microscope that replaces an out-dated one, a research project that follows on from a preceding project, or a faculty member who succeeds a colleague as head of department."@en ;
        rdfs:label    "successor"@en .

scoro:publishes-data  a  scoro:AuthorshipContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of publishing in a public database or data repository data arising from the research.  \n\nNote that this activity is distinct from that of publishing a journal article containing a selection of these data."@en ;
        rdfs:label    "publishes data"@en .

scoro:data-creator  a  scoro:DataRole , owl:NamedIndividual ;
        rdfs:comment  "The role a person has of being a creator or originator of new data."@en ;
        rdfs:label    "data creator"@en .

scoro:creates-software
        a             scoro:ExperimentalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of creating computer software, including software for experimental control, analytical tools, mathematical model and computational simulations for use in the research, or of creating an IT service (e.g. a bioinformatics database or Web service) of use to others.  "@en ;
        rdfs:label    "creates software"@en .

rdfs:label  a   owl:AnnotationProperty .

scoro:manages-project
        a             scoro:OrganizationalContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of project management.  \n\nNote that an individual other than the person holding the formal role of project manager may make considerable contributions to the management of a research project."@en ;
        rdfs:label    "manages project"@en .

[ a            owl:AllDisjointClasses ;
  owl:members  ( scoro:AuthorshipRole scoro:DataRole scoro:FinancialRole scoro:InvestigationRole scoro:OrganizationalRole scoro:ProjectRole )
] .

scoro:interprets-results
        a             scoro:IntellectualContribution , owl:NamedIndividual ;
        rdfs:comment  "The contribution of interpretation of experimental results."@en ;
        rdfs:label    "interprets results"@en .
