        rdfs:domain   :Tender ;
        rdfs:label    "Procuring entity"@en ;
        rdfs:range    :Organization .

:milestoneTitle  a   owl:FunctionalProperty , owl:DatatypeProperty ;
        rdfs:domain  :Milestone ;
        rdfs:label   "Milestone title"@en ;
        rdfs:range   xsd:string .

