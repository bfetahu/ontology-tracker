        rdfs:label    "Number of tenders"@en ;
        rdfs:range    xsd:integer .

:awardCriteriaBestValueToGovernment
        a              :AwardCriteria , owl:NamedIndividual ;
        rdfs:label     "bestValueToGovernment"@en ;
        dcterms:title  "Best Value to Government"@en .

