bevon:Aging a owl:Class ;
	dcterms:issued "2013-07-04+09:00"^^xsd:date ;
	dcterms:modified "2014-03-01+09:00"^^xsd:date ;
	rdfs:isDefinedBy <http://rdfs.co/bevon/> ;
	status:term_status "unstable"@en ;
	skos:definition """Aging is the process of storing beverages in casks for a specific period of
    time to remove harsh flavors and add distinct characteristics found in the
    cask's wood."""@en ;
	skos:prefLabel "Aging"@en , "숙성"@ko .

bevon:AlcoholicBeverage a owl:Class ;
	dcterms:issued "2013-07-04+09:00"^^xsd:date ;
	dcterms:modified "2014-03-01+09:00"^^xsd:date ;
	rdfs:isDefinedBy <http://rdfs.co/bevon/> ;
	rdfs:seeAlso <http://en.wikipedia.org/wiki/Alcoholic_beverage> ;
	rdfs:subClassOf bevon:Beverage , pto:Alcoholic_beverage ;
	owl:disjointWith bevon:NonAlcoholicBeverage ;
	status:term_status "testing"@en ;
	skos:definition """An alcoholic beverage is a drink and psychoactive drug containing
     ethyl alcohol which is commonly referred to as ethanol."""@en ;
	skos:prefLabel "Alcoholic Beverage"@en , "알코올 음료"@ko .

bevon:Ale a owl:Class ;
	dcterms:issued "2013-07-04+09:00"^^xsd:date ;
	dcterms:modified "2014-03-01+09:00"^^xsd:date ;
	rdfs:comment """The yeast will ferment the beer quickly, giving it a sweet, full bodied and
    fruity taste."""@en ;
	rdfs:isDefinedBy <http://rdfs.co/bevon/> ;
	rdfs:seeAlso <http://en.wikipedia.org/wiki/Ale> ;
	rdfs:subClassOf bevon:Beer , pto:Ale ;
	status:term_status "unstable"@en ;
	skos:definition """Ale is a type of beer brewed from malted barley using a warm-fermentation
    with a strain of brewers' yeast."""@en ;
	skos:prefLabel "Ale"@en , "에일"@ko .

bevon:AmericanWhiskey a owl:Class ;
	dcterms:issued "2013-07-04+09:00"^^xsd:date ;
	dcterms:modified "2014-03-01+09:00"^^xsd:date ;
	rdfs:comment """The production and labeling of American whiskey are governed by Title 27 of
    the U.S. Code of Federal Regulations."""@en ;
	rdfs:isDefinedBy <http://rdfs.co/bevon/> ;
	rdfs:seeAlso <http://en.wikipedia.org/wiki/American_whiskey> ;
	rdfs:subClassOf bevon:Whisky , pto:American_whiskey ;
	status:term_status "unstable"@en ;
	skos:altLabel "American Whisky"@en ;
	skos:definition """American whiskey is a distilled beverage produced in the United States from
    a fermented mash of cereal grain."""@en ;
	skos:prefLabel "American Whiskey"@en , "아메리칸 위스키"@ko .

bevon:Beer a owl:Class ;
	dcterms:issued "2013-07-04+09:00"^^xsd:date ;
	dcterms:modified "2014-03-01+09:00"^^xsd:date ;
	rdfs:comment """The starch and saccharification enzymes are often derived from malted cereal
    grains, most commonly malted barley and malted wheat."""@en ;
	rdfs:isDefinedBy <http://rdfs.co/bevon/> ;
	rdfs:seeAlso <http://en.wikipedia.org/wiki/Beer> ;
	rdfs:subClassOf bevon:FermentedBeverage , pto:Beer ;
	owl:disjointWith bevon:NonAlcoholicBeer ;
	status:term_status "testing"@en ;
	skos:definition """Beer is an alcoholic beverage produced by the saccharification of starch and
    fermentation of the resulting sugar."""@en ;
	skos:prefLabel "Beer"@en , "맥주"@ko .

bevon:BeerBottle a owl:Class ;
	dcterms:issued "2013-07-04+09:00"^^xsd:date ;
	dcterms:modified "2014-03-01+09:00"^^xsd:date ;
	rdfs:comment """Dark amber or brown glass greatly reduces UV light from spoiling the beer.
    However, lighter colored bottles are often used for marketing reasons."""@en ;
	rdfs:isDefinedBy <http://rdfs.co/bevon/> ;
	rdfs:seeAlso <http://en.wikipedia.org/wiki/Beer_bottle> ;
	rdfs:subClassOf bevon:GlassBottle ;
	status:term_status "unstable"@en ;
	skos:definition """A beer bottle is a bottle made to contain beer, usually made of glass and
    come in various sizes, shapes and colors."""@en ;
	skos:prefLabel "Beer Bottle"@en , "맥주병"@ko .

bevon:BeerCocktail a owl:Class ;
	dcterms:issued "2013-07-04+09:00"^^xsd:date ;
	dcterms:modified "2014-03-01+09:00"^^xsd:date ;
	rdfs:isDefinedBy <http://rdfs.co/bevon/> ;
	rdfs:seeAlso <http://en.wikipedia.org/wiki/Beer_cocktail> ;
	rdfs:subClassOf bevon:Cocktail , pto:Beer_cocktail ;
	status:term_status "unstable"@en ;
	skos:definition """A beer cocktail is a cocktail that is made by mixing beer with a distilled
    beverage. In this type of cocktail, the primary ingredient is beer."""@en ;
	skos:prefLabel "Beer Cocktail"@en , "맥주 칵테일"@ko .

